# vim: set ft=sh:
# explore performance analyzes data

coog_perf_main() { # query perf analyzes - command [args]
        [ -z "$1" ] && echo missing command && return 1
        if [ "$1" = help ] || [ "$1" = io ] || [ "$1" = profile ] || [ "$1" = query ]
        then
                coog_redis_eval perf "$@"
        else
                coog_redis_eval perf "$@" | column -t | less
        fi
}
