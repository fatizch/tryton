        </record>
    </data>
    <data noupdate="1">
        <!-- ########### -->
        <!-- # Journal # -->
        <!-- ########### -->
        <record model="account.journal" id="journal_commission_reset">
            <field name="name">Commission Reset Journal</field>
            <field name="code">COMRESET</field>
