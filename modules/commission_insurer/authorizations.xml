<tryton>
    <data>
        <!-- Commission Management Group -->
        <!-- Commission -->
        <record model="ir.ui.menu-res.group" id="menu_commission_create_invoice_principal_commission_manage">
            <field name="menu" ref="account_invoice_slip.menu_create_insurer_slip"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
        </record>
        <record model="ir.model.access" id="access_account_invoice_group_commission_manage">
            <field name="model" search="[('model', '=', 'account.invoice')]"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_account_invoice_line_group_commission_manage">
            <field name="model" search="[('model', '=', 'account.invoice.line')]"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <!-- Report Engine -->
        <record model="ir.ui.menu-res.group" id="menu_report_insurer_commission_manage">
            <field name="menu" ref="insurer_reporting.menu_report_insurer"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_report_insurer_contract_commission_manage">
            <field name="menu" ref="insurer_reporting.menu_report_insurer_contract"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
        </record>
    </data>
</tryton>
