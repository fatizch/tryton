<tryton>
    <data>
        <!-- ######################## -->
        <!-- # Recalculate Contract # -->
        <!-- ######################## -->
        <record model="ir.ui.view" id="recalculate_contract_view_form">
            <field name="model">endorsement.contract.recalculate</field>
            <field name="type">form</field>
            <field name="name">recalculate_contract_form</field>
        </record>
        <!-- ############################## -->
        <!-- # Change Contract Start Date # -->
        <!-- ############################## -->
        <record model="ir.ui.view" id="endorsement_change_contract_start_date_view_form">
            <field name="model">endorsement.contract.change_start_date</field>
            <field name="type">form</field>
            <field name="name">endorsement_change_contract_start_date_form</field>
        </record>
        <!-- ####################### -->
        <!-- # Reactivate Contract # -->
        <!-- ####################### -->
        <record model="ir.ui.view" id="endorsement_reactivate_contract_view_form">
            <field name="model">endorsement.contract.reactivate</field>
            <field name="type">form</field>
            <field name="name">endorsement_reactivate_contract_form</field>
        </record>
        <!-- ############################## -->
        <!-- # Change Contract Extra Data  -->
        <!-- ############################## -->
        <record model="ir.ui.view" id="endorsement_change_contract_extra_data_view_form">
            <field name="model">endorsement.contract.change_extra_data</field>
            <field name="type">form</field>
            <field name="name">endorsement_change_contract_extra_data_form</field>
        </record>
        <!-- ################## -->
        <!-- # Manage Options # -->
        <!-- ################## -->
        <record model="ir.ui.view" id="contract_manage_options_view_form">
            <field name="model">contract.manage_options</field>
            <field name="type">form</field>
            <field name="name">contract_manage_options_form</field>
        </record>
        <!-- #################### -->
        <!-- # Option Displayer # -->
        <!-- #################### -->
        <record model="ir.ui.view" id="contract_manage_options_option_displayer_view_form">
            <field name="model">contract.manage_options.option_displayer</field>
            <field name="type">form</field>
            <field name="name">contract_manage_options_option_displayer_form</field>
        </record>
        <record model="ir.ui.view" id="contract_manage_options_option_displayer_view_list">
            <field name="model">contract.manage_options.option_displayer</field>
            <field name="type">tree</field>
            <field name="name">contract_manage_options_option_displayer_list</field>
        </record>
        <!-- ####################### -->
        <!-- # Terminate Contract    -->
        <!-- ####################### -->
        <record model="ir.ui.view" id="endorsement_terminate_contract_view_form">
            <field name="model">endorsement.contract.terminate</field>
            <field name="type">form</field>
            <field name="name">endorsement_terminate_contract_form</field>
        </record>
        <!-- ################# -->
        <!-- # Void Contract # -->
        <!-- ################# -->
        <record model="ir.ui.view" id="endorsement_void_contract_view_form">
            <field name="model">endorsement.contract.void</field>
            <field name="type">form</field>
            <field name="name">endorsement_void_contract_form</field>
        </record>
        <!-- ############################################################ -->
        <!-- ########### Change Contract Subscriber ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="endorsement_change_contract_subscriber_view_form">
            <field name="model">endorsement.contract.subscriber_change</field>
            <field name="type">form</field>
            <field name="name">endorsement_change_contract_subscriber_form</field>
        </record>
        <!-- ###################### -->
        <!-- # Select Endorsement # -->
        <!-- ###################### -->
        <record model="ir.ui.view" id="select_endorsement_view_form">
            <field name="model">endorsement.start.select_endorsement</field>
            <field name="type">form</field>
            <field name="name">select_endorsement_form</field>
        </record>
        <record model="ir.ui.view" id="endorsement_summary_view_form">
            <field name="model">endorsement.start.select_endorsement</field>
            <field name="type">form</field>
            <field name="name">endorsement_summary_form</field>
        </record>
        <!-- ################### -->
        <!-- # Preview Changes # -->
        <!-- ################### -->
        <record model="ir.ui.view" id="preview_changes_view_form">
            <field name="model">endorsement.start.preview_changes</field>
            <field name="type">form</field>
            <field name="name">preview_changes_form</field>
        </record>
        <!-- ##################### -->
        <!-- # Start Endorsement # -->
        <!-- ##################### -->
        <record model="ir.action.wizard" id="act_start_endorsement">
            <field name="name">Start Endorsement</field>
            <field name="wiz_name">endorsement.start</field>
        </record>
        <!-- We need different actions because the name must be different -->
        <record model="ir.action.wizard" id="act_resume_endorsement">
            <field name="name">Resume Endorsement</field>
            <field name="wiz_name">endorsement.start</field>
        </record>
        <!-- ##################################### -->
        <!-- # Open Contract at Application Date # -->
        <!-- ##################################### -->
        <record model="ir.action.wizard" id="act_contract_open">
            <field name="name">Open Contract</field>
            <field name="wiz_name">endorsement.contract.open</field>
        </record>
        <!-- Relate to open contract at application date -->
        <record model="ir.action.act_window" id="act_open_contract_at_date">
            <field name="name">Contract</field>
            <field name="res_model">contract</field>
            <field name="domain" eval="[('id', 'in', Eval('contracts'))]" pyson="1"/>
            <field name="context" eval="{'_datetime': Eval('application_date'), 'contract': Eval('contract')}" pyson="1"/>
        </record>
        <!-- ############################## -->
        <!-- # Decline Endorsement Wizard # -->
        <!-- ############################## -->
        <record model="ir.ui.view" id="select_endorsement_decline_reason_view_form">
            <field name="model">endorsement.decline.select_reason</field>
            <field name="type">form</field>
            <field name="name">select_decline_reason_form</field>
        </record>
        <record model="ir.action.wizard" id="act_decline_endorsement">
            <field name="name">Decline Endorsement</field>
            <field name="wiz_name">endorsement.decline</field>
        </record>
        <!-- ################### -->
        <!-- # Manage Contacts # -->
        <!-- ################### -->
        <record model="ir.ui.view" id="manage_contacts_view_form">
            <field name="model">endorsement.manage_contacts</field>
            <field name="type">form</field>
            <field name="name">manage_contacts_form</field>
        </record>
        <!-- ############################# -->
        <!-- # Manage Contacts Displayer # -->
        <!-- ############################# -->
        <record model="ir.ui.view" id="manage_contacts_displayer_view_form">
            <field name="model">endorsement.manage_contacts.contact</field>
            <field name="type">form</field>
            <field name="name">manage_contacts_displayer_form</field>
        </record>
        <record model="ir.ui.view" id="manage_contacts_displayer_view_list">
            <field name="model">endorsement.manage_contacts.contact</field>
            <field name="type">tree</field>
            <field name="name">manage_contacts_displayer_list</field>
        </record>
    </data>
</tryton>
