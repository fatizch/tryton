<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <!-- ########################## -->
        <!-- # Endorsement Definition # -->
        <!-- ########################## -->
        <record model="ir.ui.view" id="endorsement_definition_view_form">
            <field name="model">endorsement.definition</field>
            <field name="type">form</field>
            <field name="name">endorsement_definition_form</field>
        </record>
        <record model="ir.ui.view" id="endorsement_definition_view_list">
            <field name="model">endorsement.definition</field>
            <field name="type">tree</field>
            <field name="name">endorsement_definition_list</field>
        </record>
        <record model="ir.ui.view" id="endorsement_definition_light_view_list">
            <field name="model">endorsement.definition</field>
            <field name="type">tree</field>
            <field name="name">endorsement_definition_light_list</field>
        </record>
        <record model="ir.action.act_window" id="act_endorsement_definition">
            <field name="name">Endorsement Definition</field>
            <field name="res_model">endorsement.definition</field>
        </record>
        <record model="ir.action.act_window.view" id="act_endorsement_definition_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="endorsement_definition_view_list"/>
            <field name="act_window" ref="act_endorsement_definition"/>
        </record>
        <record model="ir.action.act_window.view" id="act_endorsement_definition_view2">
            <field name="sequence" eval="10"/>
            <field name="view" ref="endorsement_definition_view_form"/>
            <field name="act_window" ref="act_endorsement_definition"/>
        </record>
        <menuitem parent="endorsement.menu_configuration" sequence="30" action="act_endorsement_definition" id="menu_endorsement_definition"/>
        <!-- ############################################################ -->
        <!-- Endorsement Part -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="endorsement_part_view_form">
            <field name="model">endorsement.part</field>
            <field name="type">form</field>
            <field name="name">endorsement_part_form</field>
        </record>
        <record model="ir.ui.view" id="endorsement_part_view_list">
            <field name="model">endorsement.part</field>
            <field name="type">tree</field>
            <field name="name">endorsement_part_list</field>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_endorsement_part">
            <field name="name">Endorsement Part</field>
            <field name="res_model">endorsement.part</field>
        </record>
        <record model="ir.action.act_window.view" id="act_endorsement_part_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="endorsement_part_view_list"/>
            <field name="act_window" ref="act_endorsement_part"/>
        </record>
        <record model="ir.action.act_window.view" id="act_endorsement_part_view2">
            <field name="sequence" eval="10"/>
            <field name="view" ref="endorsement_part_view_form"/>
            <field name="act_window" ref="act_endorsement_part"/>
        </record>
        <menuitem parent="endorsement.menu_configuration" sequence="10" action="act_endorsement_part" id="menu_endorsement_part"/>
        <!-- #################################################### -->
        <!-- # Endorsement Definition Part Relation # -->
        <!-- #################################################### -->
        <record model="ir.ui.view" id="endorsement_definition_part_relation_view_list">
            <field name="model">endorsement.definition-endorsement.part</field>
            <field name="type">tree</field>
            <field name="name">endorsement_definition_part_relation_list</field>
        </record>
        <!-- ############################################################ -->
        <!-- Endorsement Contract Fields -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="endorsement_contract_field_view_form">
            <field name="model">endorsement.contract.field</field>
            <field name="type">form</field>
            <field name="name">endorsement_field_form</field>
        </record>
        <record model="ir.ui.view" id="endorsement_contract_field_view_list">
            <field name="model">endorsement.contract.field</field>
            <field name="type">tree</field>
            <field name="name">endorsement_field_list</field>
        </record>
        <!-- ############################################################ -->
        <!-- Endorsement Option Field -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="endorsement_contract_option_field_view_list">
            <field name="model">endorsement.contract.option.field</field>
            <field name="type">tree</field>
            <field name="name">endorsement_field_list</field>
        </record>
        <record model="ir.ui.view" id="endorsement_contract_option_field_view_form">
            <field name="model">endorsement.contract.option.field</field>
            <field name="type">form</field>
            <field name="name">endorsement_field_form</field>
        </record>
        <!-- ####################################### -->
        <!-- # Endorsement Actvation History Field # -->
        <!-- ####################################### -->
        <record model="ir.ui.view" id="endorsement_contract_activation_history_field_view_form">
            <field name="model">endorsement.contract.activation_history.field</field>
            <field name="type">form</field>
            <field name="name">endorsement_field_form</field>
        </record>
        <record model="ir.ui.view" id="endorsement_contract_activation_history_field_view_list">
            <field name="model">endorsement.contract.activation_history.field</field>
            <field name="type">tree</field>
            <field name="name">endorsement_field_list</field>
        </record>
        <!-- ############################################################ -->
        <!-- Product -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="product_view_form">
            <field name="model">offered.product</field>
            <field name="type">form</field>
            <field name="inherit" ref="offered.product_view_form"/>
            <field name="name">product_form</field>
        </record>
        <!-- ###################### -->
        <!-- # Method Definitions # -->
        <!-- ###################### -->
        <record model="ir.model.method" id="contract_update_start_date">
            <field name="method_name">update_start_date</field>
            <field name="name">Update Start Date</field>
            <field name="model" search="[('model', '=', 'contract')]"/>
            <field name="priority">1</field>
        </record>
        <record model="ir.model.method" id="contract_calculate_after_endorsement_method">
            <field name="method_name">calculate_after_endorsement</field>
            <field name="name">Calculate After Endorsement</field>
            <field name="model" search="[('model', '=', 'contract')]"/>
            <field name="priority">1000</field>
        </record>
        <record model="ir.model.method" id="contract_plan_termination">
            <field name="method_name">plan_termination_or_terminate</field>
            <field name="name">Plan Termination</field>
            <field name="model" search="[('model', '=', 'contract')]"/>
            <field name="priority">2</field>
        </record>
        <!-- ################################### -->
        <!-- # Change Start Date Configuration # -->
        <!-- ################################### -->
        <record model="endorsement.part" id="change_contract_start_date_revision_part">
            <field name="code">change_contract_start_date</field>
            <field name="name">Change Contract Start Date</field>
            <field name="kind">contract</field>
            <field name="view">change_start_date</field>
        </record>
        <!-- ############################################ -->
        <!-- # Change Contract Subscriber Configuration # -->
        <!-- ############################################ -->
        <record model="endorsement.part" id="change_contract_subscriber_revision_part">
            <field name="code">change_contract_subscriber</field>
            <field name="name">Change Contract Subscriber</field>
            <field name="kind">contract</field>
            <field name="view">change_contract_subscriber</field>
        </record>
        <!-- ################################## -->
        <!-- # Stop Endorsement Configuration # -->
        <!-- ################################## -->
        <record model="endorsement.definition" id="stop_contract_definition">
            <field name="code">stop_contract</field>
            <field name="name">Stop Contract</field>
        </record>
        <record model="endorsement.part" id="stop_contract_revision_part">
            <field name="code">stop_contract</field>
            <field name="name">Stop Contract</field>
            <field name="kind">contract</field>
            <field name="view">terminate_contract</field>
        </record>
        <record model="endorsement.definition-endorsement.part" id="stop_contract_relation">
            <field name="definition" ref="stop_contract_definition"/>
            <field name="endorsement_part" ref="stop_contract_revision_part"/>
            <field name="order">1</field>
        </record>
        <record model="endorsement.contract.field" id="status_date_contract_stop_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'status')]"/>
            <field name="endorsement_part" ref="stop_contract_revision_part"/>
        </record>
        <record model="endorsement.contract.field" id="sub_status_contract_stop_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'sub_status')]"/>
            <field name="endorsement_part" ref="stop_contract_revision_part"/>
        </record>
        <record model="endorsement.contract.field" id="end_date_contract_stop_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'end_date')]"/>
            <field name="endorsement_part" ref="stop_contract_revision_part"/>
        </record>
        <!-- ################################## -->
        <!-- # Void Endorsement Configuration # -->
        <!-- ################################## -->
        <record model="endorsement.definition" id="void_contract_definition">
            <field name="code">void_contract</field>
            <field name="name">void Contract</field>
        </record>
        <record model="endorsement.part" id="void_contract_revision_part">
            <field name="code">void_contract</field>
            <field name="name">void Contract</field>
            <field name="kind">contract</field>
            <field name="view">void_contract</field>
        </record>
        <record model="endorsement.definition-endorsement.part" id="void_contract_relation">
            <field name="definition" ref="void_contract_definition"/>
            <field name="endorsement_part" ref="void_contract_revision_part"/>
            <field name="order">1</field>
        </record>
        <record model="endorsement.contract.field" id="status_date_contract_void_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'status')]"/>
            <field name="endorsement_part" ref="void_contract_revision_part"/>
        </record>
        <record model="endorsement.contract.field" id="sub_status_contract_void_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'sub_status')]"/>
            <field name="endorsement_part" ref="void_contract_revision_part"/>
        </record>
        <!-- ######################################## -->
        <!-- # Reactivate Endorsement Configuration # -->
        <!-- ######################################## -->
        <record model="endorsement.definition" id="reactivate_contract_definition">
            <field name="code">reactivate_contract</field>
            <field name="name">reactivate Contract</field>
        </record>
        <record model="endorsement.part" id="reactivate_contract_revision_part">
            <field name="code">reactivate_contract</field>
            <field name="name">reactivate Contract</field>
            <field name="kind">contract</field>
            <field name="view">dummy_step</field>
        </record>
        <record model="endorsement.definition-endorsement.part" id="reactivate_contract_relation">
            <field name="definition" ref="reactivate_contract_definition"/>
            <field name="endorsement_part" ref="reactivate_contract_revision_part"/>
            <field name="order">1</field>
        </record>
        <record model="endorsement.contract.field" id="status_date_contract_reactivate_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'status')]"/>
            <field name="endorsement_part" ref="reactivate_contract_revision_part"/>
        </record>
        <record model="endorsement.contract.field" id="sub_status_contract_reactivate_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'sub_status')]"/>
            <field name="endorsement_part" ref="reactivate_contract_revision_part"/>
        </record>
        <record model="endorsement.contract.field" id="end_date_contract_reactivate_field">
            <field name="field" search="[('model.model', '=', 'contract'), ('name', '=', 'end_date')]"/>
            <field name="endorsement_part" ref="reactivate_contract_revision_part"/>
        </record>
        <!-- ################################ -->
        <!-- # Manage Options Configuration # -->
        <!-- ################################ -->
        <record model="endorsement.part" id="manage_options_revision_part">
            <field name="code">manage_options</field>
            <field name="name">Manage Options</field>
            <field name="kind">contract</field>
            <field name="view">manage_options</field>
        </record>
    </data>
</tryton>
