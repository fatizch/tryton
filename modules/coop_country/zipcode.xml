<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <!--  *******************************************
              **************    ZipCode  ****************
              *******************************************-->
        <record model="ir.ui.view" id="zipcode_view_form">
            <field name="model">country.zipcode</field>
            <field name="type">form</field>
            <field name="name">zipcode_form</field>
        </record>
        <record model="ir.ui.view" id="zipcode_view_tree">
            <field name="model">country.zipcode</field>
            <field name="type">tree</field>
            <field name="name">zipcode_tree</field>
        </record>

        <record model="ir.action.act_window" id="act_zipcode_form">
            <field name="name">ZipCode</field>
            <field name="res_model">country.zipcode</field>
        </record>
        <record model="ir.action.act_window.view" id="act_zipcode_form_view_tree">
            <field name="sequence" eval="10" />
            <field name="view" ref="zipcode_view_tree" />
            <field name="act_window" ref="act_zipcode_form" />
        </record>
        <record model="ir.action.act_window.view" id="act_zipcode_form_view_form">
            <field name="sequence" eval="20" />
            <field name="view" ref="zipcode_view_form" />
            <field name="act_window" ref="act_zipcode_form" />
        </record>

        <menuitem parent="ir.menu_administration" id="menu_zipcode_form"
            action="act_zipcode_form" />
    </data>
</tryton>
