<?xml version="1.0"?>
<data>
    <xpath expr="/form/label[@name='work_time_type']" position="before">
        <label name="administrative_situation"/>
        <field name="administrative_situation"/>
        <label name="administrative_situation_sub_status"/>
        <field name="administrative_situation_sub_status" widget="selection"/>
        <newline/>
    </xpath>
    <xpath expr="/form/field[@name='work_time_type']" position="after">
        <label name="public_service_work_category"/>
        <field name="public_service_work_category"/>
        <newline/>
        <label name="work_subdivision"/>
        <field name="work_subdivision"/>
        <label name="work_country"/>
        <field name="work_country"/>
        <field name="is_civil_service_employment" invisible="1"/>
    </xpath>
    <xpath expr="/form/field[@name='annual_salary_premium']" position="after">
        <newline/>
        <label name="increased_index"/>
        <field name="increased_index"/>
        <label name="gross_index"/>
        <field name="gross_index"/>
    </xpath>
</data>
