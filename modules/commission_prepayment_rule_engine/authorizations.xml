<tryton>
    <data>
        <!-- Commission Management Group -->
        <!-- Commission -->
        <record model="ir.ui.menu-res.group" id="menu_prepayment_payment_rule_form_commission_manage">
            <field name="menu" ref="commission_prepayment_rule_engine.menu_prepayment_payment_rule_form"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
        </record>
        <!-- Laboratory -->
        <record model="ir.ui.menu-res.group" id="menu_rule_engine_commission_manage">
            <field name="menu" ref="rule_engine.menu_rule_engine"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
        </record>
        <record model="ir.model.access" id="access_commission_plan_prepayment_payment_rule_commission_manage_group">
            <field name="model" search="[('model', '=', 'commission.plan.prepayment.payment_rule')]"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <!-- Default Commission Prepayment Rule Engine Access Rights -->
        <record model="ir.model.access" id="access_commission_plan_prepayment_payment_rule">
            <field name="model" search="[('model', '=', 'commission.plan.prepayment.payment_rule')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
    </data>
</tryton>
