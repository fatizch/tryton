<tryton>
    <data>
        <!-- Default Image Attachement Access -->
        <record model="ir.model.access" id="access_report_template_image_attachment">
            <field name="model" search="[('model', '=', 'report.template.image_attachment')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <!-- Admin Group -->
        <record model="ir.model.access" id="access_report_template_image_attachment_admin_group">
            <field name="model" search="[('model', '=', 'report.template.image_attachment')]"/>
            <field name="group" ref="res.group_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <!-- Global Configuration Group -->
       <record model="ir.model.access" id="access_report_template_image_attachment_group_global_config">
            <field name="model" search="[('model', '=', 'report.template.image_attachment')]"/>
            <field name="group" ref="coog_core.group_global_config"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
    </data>
</tryton>
