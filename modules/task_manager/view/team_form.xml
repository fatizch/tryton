<?xml version="1.0"?>
<form string="Team">
  <group id="name">
    <label name="name" xalign="0"/>
    <field name="name" colspan="3"/>
    <newline/>
    <label name="code" xalign="0"/>
    <field name="code" colspan="3"/>
  </group>
  <newline/>
  <group id="lists" yexpand="1" yfill="1" xexpand="1" xfill="1">
    <field name="priorities"/>
    <group id="members" yexpand="1" yfill="1" xexpand="1" xfill="1">
      <field name="members"/>
      <newline/>
      <button string="Add User" name="add_user_button"/>
    </group>
    <field name="authorizations"/>
  </group>
</form>
