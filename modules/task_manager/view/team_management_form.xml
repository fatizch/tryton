<form>
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <notebook colspan="4">
        <page string="Summary" id="summary">
            <label name="nb_tasks"/>
            <field name="nb_tasks"/>
            <newline/>
            <field name="priorities" yexpand="1" yfill="1" colspan="2" mode="graph,tree"/>
            <field name="users_links" yexpand="1" yfill="1" colspan="2" mode="graph,tree"/>
            <field name="tasks_team" yexpand="1" yfill="1" colspan="4" mode="graph,tree"/>
        </page>
        <page string="Tasks Details" id="task_details">
            <field name="tasks_team" colspan="4"/>
        </page>
        <page string="Priorities" id="priorities">
            <field name="priorities" colspan="2"/>
            <field name="users"/>
        </page>
    </notebook>
</form>
