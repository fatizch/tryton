<tryton>
    <data>
        <!-- Contract -->
        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="name">contract_form</field>
        </record>
    </data>
    <data depends="contract_life_clause">
        <record model="ir.ui.view" id="beneficiary_view_form">
            <field name="model">contract.option.beneficiary</field>
            <field name="inherit" ref="contract_life_clause.beneficiary_view_form"/>
            <field name="name">beneficiary_form</field>
        </record>
    </data>
    <data depends="contract_insurance_invoice">
        <record model="ir.ui.view" id="contract_billing_information_view_form">
            <field name="model">contract.billing_information</field>
            <field name="inherit" ref="contract_insurance_invoice.contract_billing_information_view_form"/>
            <field name="name">contract_billing_information_form</field>
        </record>
    </data>
    <data depends="endorsement_insurance_invoice">
        <record model="ir.ui.view" id="endorsement_billing_information_view_form">
            <field name="model">contract.billing_information</field>
            <field name="type">form</field>
            <field name="inherit" ref="endorsement_insurance_invoice.endorsement_billing_information_view_form"/>
            <field name="name">endorsement_billing_information_form</field>
        </record>
    </data>
</tryton>
