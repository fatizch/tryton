<tryton>
    <data>
        <!-- ###################### -->
        <!-- # Method Definitions # -->
        <!-- ###################### -->
        <record model="ir.model.method" id="update_commissions_after_endorsement_application_method">
            <field name="method_name">update_commissions_after_endorsement_application</field>
            <field name="name">Update Commissions after Endorsement Application</field>
            <field name="model" search="[('model', '=', 'contract')]"/>
            <field name="priority">1950</field>
        </record>
        <record model="ir.model.method" id="update_commissions_after_endorsement_cancellation_method">
            <field name="method_name">update_commissions_after_endorsement_cancellation</field>
            <field name="name">Update Commissions after Endorsement Cancellation</field>
            <field name="model" search="[('model', '=', 'contract')]"/>
            <field name="priority">1950</field>
        </record>
        <!-- ########################## -->
        <!-- # Change Commission Data # -->
        <!-- ########################## -->
        <record model="endorsement.part" id="change_commission_data_revision_part">
            <field name="code">change_commission_data</field>
            <field name="name">Change Commission Data</field>
            <field name="kind">commission</field>
            <field name="view">change_commission</field>
        </record>
        <!-- ################# -->
        <!-- # Change Broker # -->
        <!-- ################# -->
        <record model="endorsement.part" id="change_broker_revision_part">
            <field name="code">change_broker</field>
            <field name="name">Change Broker</field>
            <field name="kind">commission</field>
            <field name="view">change_broker</field>
        </record>
    </data>
</tryton>
