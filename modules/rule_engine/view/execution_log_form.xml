<form col="4">
    <label name="user"/>
    <field name="user"/>
    <label name="result"/>
    <field name="result"/>
    <label name="calculation_date"/>
    <field name="calculation_date"/>
    <newline/>
    <notebook colspan="2">
        <page name="debug" col="1">
            <separator name="debug"/>
            <field name="debug"/>
            <separator name="info"/>
            <field name="info"/>
            <separator name="result_details"/>
            <field name="result_details"/>
            <separator name="warnings"/>
            <field name="warnings"/>
            <separator name="errors"/>
            <field name="errors"/>
        </page>
        <page name="rule_algorithm">
            <field name="rule_algorithm"/>
        </page>
    </notebook>
    <notebook colspan="2">
        <page name="low_level_debug">
            <field name="low_level_debug"/>
        </page>
        <page name="context">
            <field name="context"/>
        </page>
    </notebook>
</form>
