<form>
    <label name="name"/>
    <field name="name"/>
    <label name="short_name"/>
    <field name="short_name"/>
    <label name="type_"/>
    <field name="type_"/>
    <label name="result_type"/>
    <field name="result_type"/>
    <label name="context"/>
    <field name="context"/>
    <label name="status"/>
    <field name="status"/>
    <label name="debug_mode"/>
    <field name="debug_mode"/>
    <notebook>
        <page name="algorithm">
            <field name="algorithm" widget="source" context_tree="data_tree" colspan="4"/>
            <group id="extra_data_group" expandable="0" string="Display More Data" yfill="1">
                <notebook height="250">
                    <page name="rules_used">
                        <field name="rules_used" colspan="4"/>
                    </page>
                    <page name="tables_used">
                        <field name="tables_used" colspan="4"/>
                    </page>
                    <page name="parameters">
                        <field name="parameters" view_ids="rule_engine.parameter_tree,rule_engine.parameter_form" colspan="4"/>
                    </page>
                </notebook>
            </group>
        </page>
        <page name="tags">
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
    <newline/>
    <field name="data_tree" invisible="1"/>
</form>
