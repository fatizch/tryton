<tryton>
    <data>
        <record model="benefit.loss.description.deduction_period_kind" id="part_time_deduction_type">
            <field name="name">Part Time</field>
            <field name="code">part_time</field>
        </record>
        <!-- IJSS extra data -->
        <record model="extra_data" id="ijss_extra_data">
            <field name="has_default_value" eval="False"/>
            <field name="string">IJSS</field>
            <field name="name">ijss</field>
            <field name="kind">benefit</field>
            <field name="type_">numeric</field>
            <field name="digits" eval="2"/>
        </record>
    </data>
    <data depends="claim_eligibility">
        <record model="benefit.eligibility.decision" id="eligiblity_decision_accepte_pena_franchise_declaration">
            <field name="name">Accepté avec pénalité: Franchise à partir de la date de déclaration</field>
            <field name="code">coog_accepte_pena_franchise_declaration</field>
            <field name="description">La franchise sera calculée à partir de la date de déclaration</field>
            <field name="state">accepted</field>
        </record>
        <record model="benefit.eligibility.decision" id="eligiblity_decision_accepte_pena_declaration_date">
            <field name="name">Accepté avec pénalité: Indemnisation le jour de la déclaration</field>
            <field name="code">coog_accepte_pena_declaration_date</field>
            <field name="description">L'indemnisation débutera le jours de la déclaration du sinistre</field>
            <field name="state">accepted</field>
        </record>
    </data>
</tryton>
