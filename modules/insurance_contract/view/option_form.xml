<?xml version="1.0"?>
<form string="option">
    <label name="contract"/>
    <field name="contract"/>
    <newline/>
    <label name="offered"/>
    <field name="offered"/>
    <label name="status"/>
    <field name="status" readonly="1"/>
    <newline/>
    <label name="start_date"/>
    <field name="start_date"/>
    <label name="end_date"/>
    <field name="end_date"/>
    <label name="deductible_duration"/>
    <field name="deductible_duration" widget="selection"/>
    <newline/>
    <notebook colspan="4">
        <page string="Covered Data" id="covered_data">
            <field name="covered_data" colspan="4"/>
        </page>
        <page string="Status History" id="status_history">
            <field name="status_history" colspan="4"/>
        </page>
    </notebook>
</form>
