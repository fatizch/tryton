<?xml version="1.0"?>
<tryton>
  <data>
    <!--################################################################-->
    <!-- Move Line -->
    <!--################################################################-->
    <record model="ir.ui.view" id="move_line_tree">
      <field name="model">account.move.line</field>
      <field name="type">tree</field>
      <field name="name">move_line_tree</field>
    </record>
    <record model="ir.ui.view" id="move_line_amount_date_tree">
      <field name="model">account.move.line</field>
      <field name="type">tree</field>
      <field name="name">move_line_amount_date_tree</field>
    </record>
    <record model="ir.ui.view" id="move_line_detail_tree">
      <field name="model">account.move.line</field>
      <field name="type">tree</field>
      <field name="name">move_line_detail_tree</field>
    </record>
    <record model="ir.ui.view" id="move_line_detail_form">
      <field name="model">account.move.line</field>
      <field name="type">form</field>
      <field name="name">move_line_detail_form</field>
    </record>
    <!-- ############################################################ -->
    <!-- Move -->
    <!-- ############################################################ -->
    <record model="ir.ui.view" id="move_form">
      <field name="model">account.move</field>
      <field name="type">form</field>
      <field name="name">move_form</field>
    </record>
    <record model="ir.ui.view" id="move_tree">
      <field name="model">account.move</field>
      <field name="type">tree</field>
      <field name="name">move_tree</field>
    </record>
    <record model="ir.action.act_window.view" id="act_move_view1">
      <field name="sequence" eval="16"/>
      <field name="view" ref="move_tree"/>
      <field name="act_window" ref="act_open_move_form"/>
    </record>
    <record model="ir.action.act_window.view" id="act_move_view2">
      <field name="sequence" eval="16"/>
      <field name="view" ref="move_form"/>
      <field name="act_window" ref="act_open_move_form"/>
    </record>
  </data>
</tryton>
