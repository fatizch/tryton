<form col="6">
    <group id="person" colspan="6" col="8">
        <label name="code"/>
        <field name="code"/>
        <label name="gender"/>
        <field name="gender" widget="selection" xexpand="0"/>
        <label name="name"/>
        <field name="name"/>
        <label name="first_name"/>
        <field name="first_name"/>
        <label name="birth_date"/>
        <field name="birth_date"/>
        <label name="birth_name"/>
        <field name="birth_name"/>
        <label name="ssn"/>
        <field name="ssn"/>
        <label name="lang"/>
        <field name="lang"/>
    </group>
    <group id="company" colspan="6" col="6">
        <label name="commercial_name"/>
        <field name="commercial_name"/>
        <label name="name"/>
        <field name="name"/>
    </group>
    <separator string="Contact" colspan="6" id="contact"/>
    <label name="main_address"/>
    <field name="main_address" colspan="5"/>
    <label name="phone"/>
    <field name="phone"/>
    <label name="mobile"/>
    <field name="mobile"/>
    <label name="email"/>
    <field name="email"/>
</form>
