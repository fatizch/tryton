<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="party_configuration_view_form">
            <field name="model">party.configuration</field>
            <field name="inherit" ref="party.party_configuration_view_form"/>
            <field name="name">configuration_form</field>
        </record>
        <record model="ir.property" id="property_party_lang">
            <field name="field"
                search="[('model.model', '=', 'party.party'), ('name', '=', 'lang')]"/>
            <field name="value" eval="'ir.lang,' + str(ref('ir.lang_en'))"/>
        </record>
    </data>
</tryton>
