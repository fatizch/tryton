<tryton>
    <data>
        <!--################################################################ -->
        <!-- Indemnification -->
        <!--################################################################ -->
        <record model="ir.ui.view" id="indemnification_view_form">
            <field name="model">claim.indemnification</field>
            <field name="type">form</field>
            <field name="name">indemnification_form</field>
        </record>
        <record model="ir.ui.view" id="indemnification_view_list">
            <field name="model">claim.indemnification</field>
            <field name="type">tree</field>
            <field name="name">indemnification_list</field>
        </record>
        <record model="ir.ui.view" id="indemnification_simple_view_list">
            <field name="model">claim.indemnification</field>
            <field name="type">tree</field>
            <field name="name">indemnification_simple_list</field>
        </record>
        <record model="ir.ui.view" id="indemnification_control_rule_form">
          <field name="model">claim.indemnification.control.rule</field>
          <field name="type">form</field>
          <field name="name">indemnification_control_rule_form</field>
        </record>
        <record model="ir.ui.view" id="indemnification_control_rule_tree">
          <field name="model">claim.indemnification.control.rule</field>
          <field name="type">tree</field>
          <field name="name">indemnification_control_rule_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_indemnification_control_rule">
            <field name="name">Indemnification Control Rule</field>
            <field name="res_model">claim.indemnification.control.rule</field>
        </record>
        <record model="ir.action.act_window.view" id="act_indemnification_control_rule_view1">
            <field name="sequence" eval="20"/>
            <field name="view" ref="indemnification_control_rule_form"/>
            <field name="act_window" ref="act_indemnification_control_rule"/>
        </record>
        <record model="ir.action.act_window.view" id="act_indemnification_control_rule_view2">
            <field name="sequence" eval="10"/>
            <field name="view" ref="indemnification_control_rule_tree"/>
            <field name="act_window" ref="act_indemnification_control_rule"/>
        </record>
        <menuitem name="Indemnification Control Rule" parent="claim.menu_claim_configuration"
              id="menu_indemnification_control_rule" action="act_indemnification_control_rule"
              icon="tryton-settings"/>
        <record model="ir.model.button" id="claim_indemnification_modify_indemnification_button">
            <field name="name">modify_indemnification</field>
            <field name="model" search="[('model', '=', 'claim.indemnification')]"/>
        </record>
        <record model="ir.model.button" id="claim_indemnification_validate_indemnification_button">
            <field name="name">validate_indemnification</field>
            <field name="model" search="[('model', '=', 'claim.indemnification')]"/>
        </record>
        <record model="ir.model.button" id="claim_indemnification_calculate_button">
            <field name="name">calculate</field>
            <field name="model" search="[('model', '=', 'claim.indemnification')]"/>
        </record>
        <record model="ir.model.button" id="claim_indemnification_schedule_button">
            <field name="name">schedule</field>
            <field name="model" search="[('model', '=', 'claim.indemnification')]"/>
        </record>
        <record model="ir.model.button" id="claim_indemnification_cancel_payment_button">
            <field name="name">cancel_payment</field>
            <field name="model" search="[('model', '=', 'claim.indemnification')]"/>
        </record>
        <record model="ir.model.button" id="claim_indemnification_reject_indemnification_button">
            <field name="name">reject_indemnification</field>
            <field name="model" search="[('model', '=', 'claim.indemnification')]"/>
        </record>
        <!--################################################################ -->
        <!-- Indemnification Detail -->
        <!--################################################################ -->
        <record model="ir.ui.view" id="indemnification_detail_view_form">
            <field name="model">claim.indemnification.detail</field>
            <field name="type">form</field>
            <field name="name">indemnification_detail_form</field>
        </record>
        <record model="ir.ui.view" id="indemnification_detail_view_list">
            <field name="model">claim.indemnification.detail</field>
            <field name="type">tree</field>
            <field name="name">indemnification_detail_list</field>
        </record>
        <!-- Extra Details Configuration -->
        <record model="extra_details.configuration" id="indemnification_extra_details_configuration">
            <field name="model_name">claim.indemnification.detail</field>
        </record>
        <!-- ############################################################ -->
        <!-- ########### claim ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="claim_view_form">
            <field name="model">claim</field>
            <field name="inherit" ref="claim.claim_view_form"/>
            <field name="name">claim_form</field>
        </record>
        <record model="ir.model.button" id="claim_calculate_button">
            <field name="name">button_calculate</field>
            <field name="model" search="[('model', '=', 'claim')]"/>
        </record>
        <record model="ir.model.button" id="claim_schedule_all_button">
            <field name="name">schedule_all</field>
            <field name="model" search="[('model', '=', 'claim')]"/>
        </record>
        <!--################################################################ -->
        <!-- Claim Service -->
        <!--################################################################ -->
        <record model="ir.ui.view" id="claim_service_view_form">
            <field name="model">claim.service</field>
            <field name="inherit" ref="claim.claim_service_view_form"/>
            <field name="name">service_form</field>
        </record>
        <record model="ir.ui.view" id="claim_service_view_list">
            <field name="model">claim.service</field>
            <field name="inherit" ref="claim.claim_service_view_list"/>
            <field name="name">service_list</field>
        </record>
        <!-- ###################################################### -->
        <!-- ########### Indemnification Payback Reason ########### -->
        <!-- ###################################################### -->
        <record model="ir.ui.view" id="indemnification_payback_reason_form">
            <field name="model">claim.indemnification.payback_reason</field>
            <field name="type">form</field>
            <field name="name">indemnification_payback_reason_form</field>
        </record>
        <record model="ir.ui.view" id="indemnification_payback_reason_list">
            <field name="model">claim.indemnification.payback_reason</field>
            <field name="type">tree</field>
            <field name="name">indemnification_payback_reason_list</field>
        </record>
        <record model="ir.action.act_window" id="act_indemnification_payback_reason_form">
            <field name="name">Payback Reason</field>
            <field name="res_model">claim.indemnification.payback_reason</field>
        </record>
        <record model="ir.action.act_window.domain" id="act_indemnification_payback_reason_domain_active">
            <field name="name">Active</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[('active', '=', True)]" pyson="1"/>
            <field name="act_window" ref="act_indemnification_payback_reason_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_indemnification_payback_reason_domain_inactive">
            <field name="name">Inactive</field>
            <field name="sequence" eval="20"/>
            <field name="domain" eval="[('active', '!=', True)]" pyson="1"/>
            <field name="act_window" ref="act_indemnification_payback_reason_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_indemnification_payback_reason_view_list">
            <field name="sequence" eval="10"/>
            <field name="view" ref="indemnification_payback_reason_list"/>
            <field name="act_window" ref="act_indemnification_payback_reason_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_indemnification_payback_reason_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="indemnification_payback_reason_form"/>
            <field name="act_window" ref="act_indemnification_payback_reason_form"/>
        </record>
        <menuitem name="Indemnification Payback Reasons" parent="claim.menu_claim_configuration" id="menu_indemnification_payback_reasons" action="act_indemnification_payback_reason_form"/>
        <record model="claim.indemnification.payback_reason" id="migration_payback_reason">
            <field name="code">migration</field>
            <field name="name">Migration</field>
            <field name="active">False</field>
        </record>
        <record model="claim.indemnification.payback_reason" id="erreur_payback_reason">
            <field name="code">erreur</field>
            <field name="name">Erreur</field>
            <field name="active">True</field>
        </record>
        <!--################# -->
        <!-- Claim Sub Status -->
        <!--################# -->
        <record model="ir.ui.view" id="claim_sub_status_view_form">
            <field name="model">claim.sub_status</field>
            <field name="inherit" ref="claim.sub_status_view_form"/>
            <field name="name">claim_sub_status_form</field>
        </record>
        <record model="ir.ui.view" id="claim_sub_status_view_list">
            <field name="model">claim.sub_status</field>
            <field name="inherit" ref="claim.sub_status_view_list"/>
            <field name="name">claim_sub_status_list</field>
        </record>
        <!-- ######################## -->
        <!-- # Claim Invoice Relate # -->
        <!-- ######################## -->
        <record model="ir.action.act_window" id="act_claim_invoices">
            <field name="name">Claim Invoices</field>
            <field name="res_model">account.invoice</field>
            <field name="domain" eval="[('lines.claim_details.claim', 'in', Eval('active_ids'))]" pyson="1"/>
            <field name="order" eval="[('invoice_date', 'DESC')]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_invoice_window_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="account_invoice.invoice_view_form"/>
            <field name="act_window" ref="act_claim_invoices"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_invoice_window_view2">
            <field name="sequence" eval="5"/>
            <field name="view" ref="account_invoice.invoice_view_tree"/>
            <field name="act_window" ref="act_claim_invoices"/>
        </record>
        <record model="ir.action.keyword" id="claim_invoice_relate">
            <field name="keyword">form_relate</field>
            <field name="model">claim,-1</field>
            <field name="action" ref="act_claim_invoices"/>
        </record>
        <!-- ####################### -->
        <!-- # Claim Invoices Menu # -->
        <!-- ####################### -->
        <record model="ir.action.act_window" id="act_all_claim_invoices">
            <field name="name">Claim Invoices</field>
            <field name="res_model">account.invoice</field>
            <field name="domain" eval="[('business_kind', '=', 'claim_invoice')]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_invoice_window2_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="account_invoice.invoice_view_form"/>
            <field name="act_window" ref="act_all_claim_invoices"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_invoice_window2_view2">
            <field name="sequence" eval="5"/>
            <field name="view" ref="account_invoice.invoice_view_tree"/>
            <field name="act_window" ref="act_all_claim_invoices"/>
        </record>
        <menuitem parent="claim.menu_claim" id="claim_invoices_form" icon="invoice" action="act_all_claim_invoices"/>
    </data>
</tryton>
