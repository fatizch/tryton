<form>
    <label name="service"/>
    <field name="service"/>
    <label name="status"/>
    <field name="status"/>
    <label name="beneficiary"/>
    <field name="beneficiary"/>
    <label name="journal"/>
    <field name="journal"/>
    <group id="left" colspan="2" col="2">
        <field name="note" colspan="2"/>
        <label name="start_date"/>
        <field name="start_date"/>
        <label name="end_date"/>
        <field name="end_date"/>
        <label name="product"/>
        <field name="product"/>
        <label name="manual"/>
        <field name="manual"/>
        <group id="cancel_payment" colspan="1" col="1" xexpand="0">
            <button string="Cancel payment" name="cancel_payment" />
        </group>
    </group>
    <group id="right" colspan="2" col="3">
        <label name="amount"/>
        <field name="amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
        <label name="tax_amount"/>
        <field name="tax_amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
        <label name="total_amount"/>
        <field name="total_amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
        <label name="local_currency_amount"/>
        <field name="local_currency_amount"/>
        <field name="local_currency"/>
    </group>
    <field name="details" colspan="4"/>
    <field name="invoice_line_details" colspan="4"/>
    <field name="kind" invisible="1"/>
    <field name="possible_products" invisible="1"/>
</form>
