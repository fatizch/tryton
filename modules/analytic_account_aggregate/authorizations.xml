<tryton>
    <data>
        <!-- Financial Consultation Group -->
        <!-- Financial / Entries -->
        <record model="ir.ui.menu-res.group" id="menu_analytic_account_line_aggregates_form_financial_read">
            <field name="menu" ref="analytic_account_aggregate.menu_analytic_account_line_aggregates_form"/>
            <field name="group" ref="account_cog.group_financial_read"/>
        </record>
        <!-- Financial Management Group -->
        <!-- Financial / Entries -->
        <record model="ir.ui.menu-res.group" id="menu_analytic_account_line_aggregates_form_financial_manage">
            <field name="menu" ref="analytic_account_aggregate.menu_analytic_account_line_aggregates_form"/>
            <field name="group" ref="account_cog.group_financial_manage"/>
        </record>
        <!-- Default Aggregated Lines Access -->
        <record model="ir.model.access" id="access_analytic_account_line_aggregated">
            <field name="model" search="[('model', '=', 'analytic_account.line.aggregated')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <!-- Admin Group -->
        <record model="ir.model.access" id="access_analytic_account_line_aggregated_admin_group">
            <field name="model" search="[('model', '=', 'analytic_account.line.aggregated')]"/>
            <field name="group" ref="res.group_admin"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_analytic_account_line_aggregated_financial_manage_group">
            <field name="model" search="[('model', '=', 'analytic_account.line.aggregated')]"/>
            <field name="group" ref="account_cog.group_financial_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
    </data>
</tryton>
