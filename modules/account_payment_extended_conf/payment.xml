<tryton>
    <data>
	<!-- VIEWS -->
	<record model="ir.ui.view" id="party_journal_relation_view_tree">
	    <field name="model">account.payment.party_journal_relation</field>
	    <field name="type">tree</field>
	    <field name="name">party_journal_relation_tree</field>
	</record>
	<record model="ir.ui.view" id="party_journal_relation_view_form">
	    <field name="model">account.payment.party_journal_relation</field>
	    <field name="type">form</field>
	    <field name="name">party_journal_relation_form</field>
	</record>
	<!-- ACTIONS -->
	<record model="ir.action.act_window" id="act_party_journal_relation">
	    <field name="name">Party Journals relations</field>
	    <field name="res_model">account.payment.party_journal_relation</field>
	</record>
	<record model="ir.action.act_window.view" id="act_party_journal_relation_view1">
	    <field name="sequence" eval="10"/>
	    <field name="view" ref="party_journal_relation_view_tree"/>
	    <field name="act_window" ref="act_party_journal_relation"/>
	</record>
	<record model="ir.action.act_window.view" id="act_party_journal_relation_view2">
	    <field name="sequence" eval="20"/>
	    <field name="view" ref="party_journal_relation_view_form"/>
	    <field name="act_window" ref="act_party_journal_relation"/>
	</record>
	<!-- MENU -->
	<menuitem parent="account_payment.menu_payment_configuration" sequence="40"
	    action="act_party_journal_relation" id="menu_party_journal_relation"/>
    </data>
</tryton>
