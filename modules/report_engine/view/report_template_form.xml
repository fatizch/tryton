<?xml version="1.0"?>
<form string="Report Template">
    <label name="name" xalign="0"/>
    <field name="name"/>
    <label name="code" xalign="0"/>
    <field name="code"/>
    <label name="on_model" xalign="0"/>
    <field name="on_model" colspan="3" widget="selection"/>
    <label name="kind" xalign="0"/>
    <field name="kind" colspan="3"/>
    <label name="output_kind"/>
    <field name="output_kind" colspan="1"/>
    <newline/>
    <label name="format_for_internal_edm" xalign="0"/>
    <field name="format_for_internal_edm"/>
    <label name="document_desc"/>
    <field name="document_desc"/>
    <label name="modifiable_before_printing"/>
    <field name="modifiable_before_printing"/>
    <label name="template_extension"/>
    <field name="template_extension"/>
    <label name="export_dir"/>
    <field name="export_dir"/>
    <separator id="automatic_generation" string="Automatic Generation" colspan="4"/>
    <label name="convert_to_pdf"/>
    <field name="convert_to_pdf" colspan="3"/>
    <label name="split_reports"/>
    <field name="split_reports" colspan="3"/>
    <notebook>
        <page string="Versions" id="versions">
            <field name="versions"/>
        </page>
        <page name="event_type_actions">
            <field name="event_type_actions" colspan="4"/>
        </page>
        <page string="Mail" id="Mail" col="2">
            <label name="mail_subject"/>
            <field name="mail_subject"/>
            <newline/>
            <field name="mail_body" colspan="2"/>
        </page>
        <page name="tags">
            <field name="tags" colspan="4"/>
        </page>
        <page name="parameters">
            <field name="parameters" colspan="4"/>
        </page>
    </notebook>
</form>
