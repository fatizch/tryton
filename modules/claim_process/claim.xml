<tryton>
    <data>
        <record model="ir.ui.view" id="loss_services_view_form">
            <field name="model">claim.loss</field>
            <field name="type">form</field>
            <field name="priority" eval="30"/>
            <field name="name">loss_service_form</field>
        </record>
        <record model="ir.ui.view" id="declaration_process_parameters_form">
            <field name="model">claim.declare.find_process</field>
            <field name="name">claim_declare_find_process_form</field>
            <field name="inherit" ref="process_cog.process_parameters_form"/>
        </record>
        <record model="ir.ui.view" id="claim_declare_element_tree">
            <field name="model">claim.declare.element</field>
            <field name="type">tree</field>
            <field name="name">claim_declare_element_view_tree</field>
        </record>
        <record model="ir.ui.view" id="claim_declare_element_form">
            <field name="model">claim.declare.element</field>
            <field name="type">form</field>
            <field name="name">claim_declare_element_view_form</field>
        </record>
        <record model="ir.action.wizard" id="act_close_claim_wizard">
            <field name="name">Close Claims</field>
            <field name="wiz_name">claim.close</field>
        </record>
        <record model="ir.action.wizard" id="declaration_process_launcher">
            <field name="name">Start Declaration</field>
            <field name="wiz_name">claim.declare</field>
        </record>
        <menuitem parent="claim.menu_claim" sequence="1" action="declaration_process_launcher" id="menu_start_declaration" icon="claim_declaration"/>
        <record model="ir.action.act_window" id="act_claim_logs">
            <field name="name">Logs</field>
            <field name="res_model">process.log</field>
            <field name="domain" eval="[('task', '=', (Eval('active_model'),Eval('active_id')))]" pyson="1"/>
        </record>
        <record model="ir.action.keyword" id="act_claim_logs_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">claim,-1</field>
            <field name="action" ref="act_claim_logs"/>
        </record>
        <record model="ir.model.button" id="claim_process_resume_button">
            <field name="name">button_resume</field>
            <field name="model" search="[('model', '=', 'claim')]"/>
        </record>
    </data>
</tryton>
