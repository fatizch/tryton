            <field name="action" ref="declaration_process_launcher"/>
        </record>
        <record model="ir.action.keyword" id="act_contract_declare_claim">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="declaration_process_launcher"/>
