<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="group_party_view_form">
            <field name="model">party.group</field>
            <field name="type">form</field>
            <field name="name">group_party_form</field>
        </record>
        <record model="ir.ui.view" id="group_party_view_tree">
            <field name="model">party.group</field>
            <field name="type">tree</field>
            <field name="name">group_party_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_group_party_form">
            <field name="name">Group Party</field>
            <field name="res_model">party.group</field>
        </record>

        <record model="ir.action.act_window.view" id="act_group_party_form_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="group_party_view_tree"/>
            <field name="act_window" ref="act_group_party_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_group_party_form_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="group_party_view_form"/>
            <field name="act_window" ref="act_group_party_form"/>
        </record>
        <menuitem parent="coop_party.menu_company_form" id="menu_group_party_form"
            icon="tryton-list" action="act_group_party_form"/>
    </data>
</tryton>
