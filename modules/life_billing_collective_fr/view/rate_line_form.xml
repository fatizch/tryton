<?xml version="1.0"?>
<form string="Rate Line">
    <label name="contract"/>
    <field name="contract"/>
    <newline/>
    <label name="start_date_"/>
    <field name="start_date_"/>
    <label name="end_date"/>
    <field name="end_date"/>
    <label name="covered_element"/>
    <field name="covered_element"/>
    <label name="option_"/>
    <field name="option_"/>
    <newline/>
    <label name="parent"/>
    <field name="parent"/>
    <newline/>
    <field name="childs" colspan="4"/>
    <newline/>
    <label name="rate"/>
    <group col="2" id="rate">
        <field name="rate" factor="100" xexpand="0"/>
        <label name="rate" string="%" xalign="0.0" xexpand="1"/>
    </group>
    <newline/>
    <label name="tranche"/>
    <field name="tranche"/>
    <newline/>
    <label name="index"/>
    <field name="index"/>
    <label name="indexed_value"/>
    <field name="indexed_value"/>

    <newline/>
    <field name="manual_billing"/>
</form>
