<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/notebook/page[@id='managers']" position="inside">
        <page string="Rating" id="rating"
            states="{'invisible': ~Eval('is_group')}">
            <field name="rating_rules" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form" position="inside">
        <newline/>
        <field name="is_rating_by_fare_class"/>
        <field name="use_rates"/>
        <newline/>
    </xpath>
</data>
