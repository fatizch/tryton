<tryton>
    <data>
        <record model="ir.ui.view" id="configuration_view_form">
            <field name="model">offered.configuration</field>
            <field name="type">form</field>
            <field name="name">configuration_form</field>
        </record>
        <record model="ir.action.act_window" id="act_configuration_form">
            <field name="name">Offered Configuration</field>
            <field name="res_model">offered.configuration</field>
        </record>
        <record model="ir.action.act_window.view" id="act_configuration_form_view_form">
            <field name="sequence" eval="1"/>
            <field name="view" ref="configuration_view_form"/>
            <field name="act_window" ref="act_configuration_form"/>
        </record>
        <menuitem parent="menu_contract_configuration" sequence="1" id="menu_configuration_form" icon="tryton-preferences" action="act_configuration_form"/>
        <record model="offered.configuration" id="default_style_configuration_template">
            <field name="path">offered/style/default_style_template.odt</field>
        </record>
    </data>
</tryton>
