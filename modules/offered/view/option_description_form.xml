<?xml version="1.0"?>
<form string="Coverage">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="start_date"/>
    <field name="start_date" xexpand="0"/>
    <label name="end_date"/>
    <field name="end_date" xexpand="0"/>
    <notebook>
        <page string="Summary" id="summary">
            <field name="description" colspan="4"/>
        </page>
        <page string="Extra Data" id="extra_data">
            <field name="extra_data_def" colspan="4"/>
        </page>
        <page string="Rules" id="managers">
            <notebook>
                <page name="ending_rule">
                    <field name="ending_rule" mode="form"/>
                </page>
            </notebook>
        </page>
        <page string="Subscription" id="subscription">
            <label name="subscription_behaviour"/>
            <field name="subscription_behaviour"/>
            <newline/>
            <field name="options_required" colspan="2"/>
            <field name="options_excluded" colspan="2"/>
            <newline/>
        </page>
        <page string="Administration" id="admin" col="2">
            <group id="left">
                <label name="currency"/>
                <field name="currency"/>
            </group>
            <group id="right" col="2">
                <label name="company"/>
                <field name="company"/>
                <newline/>
                <field name="extra_data" colspan="2"/>
            </group>
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
</form>
