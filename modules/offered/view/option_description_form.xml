<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/notebook/page[@id='summary']" position="after">
        <page string="Extra Data" id="extra_data">
            <field name="extra_data_def" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='managers']/notebook" position="inside">
        <page name="ending_rule">
            <field name="ending_rule" mode="form"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']" position="before">
        <page string="Subscription" id="subscription">
            <label name="subscription_behaviour"/>
            <field name="subscription_behaviour"/>
            <newline/>
            <field name="options_required" colspan="2"/>
            <field name="options_excluded" colspan="2"/>
            <newline/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/group[@id='left']" position="inside">
        <label name="currency"/>
        <field name="currency"/>
        <newline/>
    </xpath>
</data>
