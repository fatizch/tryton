<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Product"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='summary']" position="after">
        <page string="Extra Data" id="extra_data">
            <field name="extra_data_def" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='managers']" position="before">
        <page string="OptionDescriptions" id="option">
            <label name="change_coverages_order"/>
            <field name="change_coverages_order"/>
            <newline/>
            <field name="coverages" colspan="4"/>
            <field name="ordered_coverages" colspan="4" mode="tree"/>
            <field name="packages" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/group[@id='left']"
        position="inside">
        <label name="contract_generator"/>
        <field name="contract_generator"/>
        <newline/>
        <label name="currency"/>
        <field name="currency"/>
        <newline/>
        <label name="subscriber_kind"/>
        <field name="subscriber_kind"/>
    </xpath>
</data>
