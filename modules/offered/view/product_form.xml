<form>
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="start_date"/>
    <field name="start_date"/>
    <label name="end_date"/>
    <field name="end_date"/>
    <notebook colspan="4">
        <page string="Main" id="summary">
            <group id="data" colspan="4">
                <label name="company"/>
                <group id="small">
                    <field name="company"/>
                    <label name="currency"/>
                    <field name="currency"/>
                </group>
                <label name="subscriber_kind"/>
                <field name="subscriber_kind"/>
                <label name="contract_generator"/>
                <field name="contract_generator"/>
            </group>
            <field name="extra_data_def"/>
            <field name="extra_data" yfill="1" yexpand="1"/>
            <separator name="description" colspan="4"/>
            <field name="description" colspan="4"/>
        </page>
        <page string="Option Descriptions" id="option">
            <label name="change_coverages_order"/>
            <field name="change_coverages_order"/>
            <newline/>
            <field name="coverages" colspan="4"/>
            <field name="ordered_coverages" colspan="4" mode="tree"/>
            <field name="packages" colspan="2"/>
        </page>
        <page string="Rules" id="managers">
            <notebook>
                <page string="Report Templates" id="report_templates">
                    <label name="report_style_template"/>
                    <field name="report_style_template"/>
                    <newline/>
                    <field name="report_templates" colspan="4"/>
                </page>
            </notebook>
        </page>
        <page string="Administration" id="admin">
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
</form>
