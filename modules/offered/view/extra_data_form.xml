<?xml version="1.0" encoding="utf-8"?>
<form string="Extra Data Descriptor">
    <label name="string"/>
    <field name="string"/>
    <label name="name"/>
    <field name="name"/>
    <notebook>
        <page id="main" string="Main">
            <label name="kind"/>
            <field name="kind"/>
            <newline/>
            <label name="type_"/>
            <field name="type_"/>
            <field name="selection" colspan="4"/>
            <label name="selection_sorted"/>
            <field name="selection_sorted"/>
            <label name="digits"/>
            <field name="digits"/>
            <newline/>
            <label name="with_default_value"/>
            <group id="default value">
                <field name="with_default_value" xexpand="0"/>
                <field name="default_value_boolean" xexpand="0"/>
                <field name="default_value_selection"/>
                <field name="default_value"/>
            </group>
        </page>
        <page id="sub_data" string="Sub Data">
            <label name="sub_data_config_kind"/>
            <field name="sub_data_config_kind"/>
            <label name="rule"/>
            <field name="rule"/>
            <newline/>
            <field name="sub_datas" yexpand="1" yfill="1" colspan="4"/>
        </page>
        <page name="tags">
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
</form>
