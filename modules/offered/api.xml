<tryton>
    <data depends="web_configuration">
        <!-- ######################## -->
        <!-- # web configuration Relate # -->
        <!-- ######################## -->
        <record model="ir.action.keyword" id="act_open_extra_data_web_ui_resources">
            <field name="keyword">form_relate</field>
            <field name="model">extra_data,-1</field>
            <field name="action" ref="web_configuration.act_web_ui_resource_for_keyword_window"/>
        </record>
        <record model="ir.ui.view" id="web_configuration_product_view_form">
            <field name="model">offered.product</field>
            <field name="inherit" ref="offered.product_view_form"/>
            <field name="name">web_configuration_product_form</field>
        </record>
    </data>
</tryton>
