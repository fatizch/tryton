<tryton>
    <data>
        <!-- ############################################################ -->
        <!-- ########### Extra Data ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="extra_data_view_list">
            <field name="model">extra_data</field>
            <field name="type">tree</field>
            <field name="name">extra_data_list</field>
        </record>
        <record model="ir.ui.view" id="extra_data_view_form">
            <field name="model">extra_data</field>
            <field name="type">form</field>
            <field name="name">extra_data_form</field>
        </record>
        <record model="ir.action.act_window" id="act_model_extra_data">
            <field name="name">Extra Data</field>
            <field name="res_model">extra_data</field>
        </record>
        <record model="ir.action.act_window.view" id="act_model_extra_data_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="extra_data_view_list"/>
            <field name="act_window" ref="act_model_extra_data"/>
        </record>
        <record model="ir.action.act_window.view" id="act_model_extra_data_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="extra_data_view_form"/>
            <field name="act_window" ref="act_model_extra_data"/>
        </record>
        <menuitem parent="menu_product_configuration" action="act_model_extra_data" id="menu_model_extra_data"/>
        <!-- ############################################################ -->
        <!-- ########### Extra Data Sub Extra Data Relation   ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="extra_data-sub_extra_data_view_form">
            <field name="model">extra_data-sub_extra_data</field>
            <field name="type">form</field>
            <field name="name">extra_data-sub_extra_data_form</field>
        </record>
        <record model="ir.ui.view" id="extra_data-sub_extra_data_view_tree">
            <field name="model">extra_data-sub_extra_data</field>
            <field name="type">tree</field>
            <field name="name">extra_data-sub_extra_data_tree</field>
        </record>
    </data>
</tryton>
