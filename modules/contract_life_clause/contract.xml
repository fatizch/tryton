<?xml version="1.0" encoding="utf-8"?>
<tryton>
  <data>
    <!-- ############### -->
    <!-- # Beneficiary # -->
    <!-- ############### -->
    <record model="ir.ui.view" id="beneficiary_view_form">
      <field name="model">contract.clause.beneficiary</field>
      <field name="type">form</field>
      <field name="name">beneficiary_form</field>
    </record>
    <record model="ir.ui.view" id="beneficiary_view_list">
      <field name="model">contract.clause.beneficiary</field>
      <field name="type">tree</field>
      <field name="name">beneficiary_list</field>
    </record>
    <!-- ################### -->
    <!-- # Contract Clause # -->
    <!-- ################### -->
    <record model="ir.ui.view" id="clause_view_form">
      <field name="model">contract.clause</field>
      <field name="type">form</field>
      <field name="inherit" ref="contract_clause.clause_view_form"/>
      <field name="name">clause_form</field>
    </record>
    <record model="ir.ui.view" id="clause_view_list">
      <field name="model">contract.clause</field>
      <field name="type">tree</field>
      <field name="inherit" ref="contract_clause.clause_view_list"/>
      <field name="name">clause_list</field>
    </record>
    <!-- ############################################################ -->
    <!-- ########### Covered Data ########### -->
    <!-- ############################################################ -->
    <record model="ir.ui.view" id="subscription_covered_data_view_form">
      <field name="model">contract.covered_data</field>
      <field name="type">form</field>
      <field name="inherit" ref="contract_insurance.covered_data_view_form"/>
      <field name="name">covered_data_form</field>
    </record>
  </data>
</tryton>
