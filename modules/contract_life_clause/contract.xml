<tryton>
    <data>
        <!-- ################### -->
        <!-- # Contract Option # -->
        <!-- ################### -->
        <record model="ir.ui.view" id="option_view_form">
            <field name="model">contract.option</field>
            <field name="inherit" ref="contract.option_view_form"/>
            <field name="name">option_form</field>
        </record>
        <!-- ############### -->
        <!-- # Beneficiary # -->
        <!-- ############### -->
        <record model="ir.ui.view" id="beneficiary_view_form">
            <field name="model">contract.option.beneficiary</field>
            <field name="type">form</field>
            <field name="name">beneficiary_form</field>
        </record>
        <record model="ir.ui.view" id="beneficiary_view_list">
            <field name="model">contract.option.beneficiary</field>
            <field name="type">tree</field>
            <field name="name">beneficiary_list</field>
        </record>
    </data>
</tryton>
