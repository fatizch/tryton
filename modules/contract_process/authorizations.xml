<tryton>
    <data>
        <!-- Contract Management Group -->
        <!-- Contract -->
        <record model="ir.ui.menu-res.group" id="menu_start_subscription_contract_manage">
            <field name="menu" ref="contract_process.menu_start_subscription"/>
            <field name="group" ref="contract.group_contract_manage"/>
        </record>
        <record model="ir.model.access" id="access_contract_subscribe_find_process">
            <field name="model" search="[('model', '=', 'contract.subscribe.find_process')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="contract_subscribe_find_process_contract_management_group">
            <field name="model" search="[('model', '=', 'contract.subscribe.find_process')]"/>
            <field name="group" ref="contract.group_contract_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
    </data>
</tryton>
