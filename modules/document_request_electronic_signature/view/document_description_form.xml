<data>
    <xpath expr="//field[@name='prerequisite']" position="after">
        <label name="digital_signature_required"/>
        <field name="digital_signature_required"/>
    </xpath>
    <xpath expr="/form/notebook" position="inside">
        <page id="electronic_signature" string='Electronic Signature'>
            <label name="signature_configuration"/>
            <field name="signature_configuration"/>
            <label name="signature_credential"/>
            <field name="signature_credential"/>
            <field name="signature_coordinates" colspan="1"/>
            <field name="sub_documents" colspan="3"/>
        </page>
    </xpath>
</data>
