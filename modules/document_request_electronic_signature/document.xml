        <record model="ir.ui.view" id="signature_coordinate_view_form">
            <field name="model">document.description.signature.coordinate</field>
            <field name="type">form</field>
            <field name="name">signature_coordinate_form</field>
        </record>
        <record model="ir.ui.view" id="signature_coordinate_view_list">
            <field name="model">document.description.signature.coordinate</field>
            <field name="type">tree</field>
