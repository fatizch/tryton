<tryton>
    <data>
        <record model="ir.ui.view" id="party_view_form">
            <field name="model">party.party</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_form</field>
        </record>
    </data>
    <!-- This override is used to add birth data information under ssn
    (else ssn ) is displayed after birth information -->
    <data depends="party_ssn">
        <record model="ir.ui.view" id="party_ssn_view_form">
            <field name="model">party.party</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_ssn_form</field>
        </record>
    </data>
</tryton>
