<tryton>
    <data>
        <record model="rule_engine.context" id="commission_context">
            <field name="name">Commission Context</field>
        </record>
        <record model="rule_engine.context-function" id="def_context_commission-te_contract">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="contract.te_contract"/>
        </record>
        <record model="rule_engine.context-function" id="cte_commission_coverage">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.context-function" id="cte_commission_dossier_personne_couverte">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="offered_insurance.dossier_personne_couverte"/>
        </record>
        <record model="rule_engine.context-function" id="cte_commission_tools">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="rule_engine.cte_default_dossier_outils"/>
        </record>
        <record model="rule_engine.context-function" id="cte_commission_coverage_data">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="offered_insurance.dossier_donnees_couverture"/>
        </record>
        <record model="rule_engine.context-function" id="cte_commission_data">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="commission_insurance.dossier_commission"/>
        </record>
    </data>
    <data depends="loan">
        <record model="rule_engine.context-function" id="cte_default_pret">
            <field name="context" ref="commission_context"/>
            <field name="tree_element" ref="loan.pret"/>
        </record>
    </data>
    <data noupdate="1">
        <record model="rule_engine.function" id="plan_commissionnement">
            <field name="translated_technical_name">plan_commssionnement</field>
            <field name="description">Plan de commissionnement</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_commission_plan</field>
            <field name="long_description">Retourne la valeur de la clé du 
moteur de règle renseignée au niveau du plan de commissionnement</field>
            <field name="parent" ref="contract.te_contract"/>
        </record>
    </data>
</tryton>
