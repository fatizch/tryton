<?xml version="1.0"?>
<tryton>
    <data>
        <menuitem name="Actors" id="menu_health_party" sequence="10" parent="menu_health" icon="tryton-party"/>
        <menuitem name="Persons" parent="menu_health_party"
               sequence="0" action="act_person_form"
               id="menu_person_form"/>
        <record model="ir.action.act_window" id="act_person_form">
            <field name="name">Persons</field>
            <field name="res_model">party.party</field>
            <field name="domain">[('is_person', '=', True)]</field>
        </record>
        <record model="ir.action.act_window.view" id="act_address_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="health_person_view_tree"/>
            <field name="act_window" ref="act_address_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_party_form_view2">
          <field name="sequence" eval="20"/>
          <field name="view" ref="address_view_form"/>
          <field name="act_window" ref="act_address_form"/>
        </record>
        <record model="ir.ui.view" id="health_person_view_tree">
            <field name="model">party.party</field>
            <field name="type">tree</field>
            <field name="priority" eval="50"/>
            <field name="inherit" ref="party.party_view_tree"/>
            <field name="name">health_party_form</field>
        </record>
        <!-- party -->
        <record model="ir.ui.view" id="party_view_form">
            <field name="model">party.party</field>
            <field name="type">form</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_form</field>
        </record>
        <!-- Party Complement -->
        <record model="ir.ui.view" id="party_complement_view_form">
            <field name="model">health.party_complement</field>
            <field name="type">form</field>
            <field name="name">party_complement_form</field>
        </record>
        <record model="ir.ui.view" id="party_complement_view_tree">
            <field name="model">health.party_complement</field>
            <field name="type">tree</field>
            <field name="name">party_complement_tree</field>
        </record>
    </data>
</tryton>
