<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <record model="ir.action.act_window" id="act_contract_form">
            <field name="name">Health Contracts</field>
            <field name="res_model">contract</field>
            <field name="domain">[('is_health', '=', True)]</field>
        </record>
        <menuitem name="Contracts" sequence="20" parent="menu_health" action="act_contract_form" id="menu_contract_form"/>
        <record model="ir.action.act_window.view" id="act_contract_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="contract.contract_view_tree"/>
            <field name="act_window" ref="act_contract_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_contract_form_view2">
          <field name="sequence" eval="20"/>
          <field name="view" ref="contract.contract_view_form"/>
          <field name="act_window" ref="act_contract_form"/>
        </record>




        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="type">form</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="name">contract_form</field>
        </record>
        <record model="ir.ui.view" id="option_view_form">
            <field name="model">contract.subscribed_option</field>
            <field name="type">form</field>
            <field name="inherit" ref="contract.option_view_form"/>
            <field name="name">option_form</field>
        </record>
        <record model="ir.ui.view" id="covered_element_view_form">
            <field name="model">ins_contract.covered_element</field>
            <field name="type">form</field>
            <field name="inherit" ref="insurance_contract.covered_element_view_form"/>
            <field name="name">covered_element_form</field>
        </record>

    </data>
</tryton>
