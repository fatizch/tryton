<tryton>
    <data noupdate="1">
        <record model="rule_engine.function" id="te_health">
            <field name="description">Santé</field>
            <field name="type">folder</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="translated_technical_name">sante</field>
        </record>
        <record model="rule_engine.context-function" id="cte_default_health">
            <field name="context" ref="rule_engine.default_context"/>
            <field name="tree_element" ref="te_health"/>
        </record>
    </data>
    <data>
        <record model="rule_engine.function" id="te_health_hc_system">
            <field name="description">Code Régime</field>
            <field name="name">_re_health_hc_system</field>
            <field name="namespace">rule_engine.runtime</field>
            <field name="type">function</field>
            <field name="parent" ref="te_health"/>
            <field name="translated_technical_name">code_hc_system</field>
            <field name="long_description">retourne le code régime de l'assuré</field>
            <field name="language" ref="ir.lang_fr"/>
        </record>
        <record model="rule_engine.function" id="te_health_subscriber_hc_system">
            <field name="description">Code régime du souscripteur</field>
            <field name="name">_re_health_subscriber_hc_system</field>
            <field name="namespace">rule_engine.runtime</field>
            <field name="type">function</field>
            <field name="parent" ref="te_health"/>
            <field name="translated_technical_name">code_regime_souscripteur</field>
            <field name="long_description">Retourne le code régime du souscripteur</field>
            <field name="language" ref="ir.lang_fr"/>
        </record>
        <record model="rule_engine.function" id="te_health_department">
            <field name="description">Département</field>
            <field name="name">_re_health_department</field>
            <field name="namespace">rule_engine.runtime</field>
            <field name="type">function</field>
            <field name="parent" ref="te_health"/>
            <field name="translated_technical_name">code_departement</field>
            <field name="long_description">Retourne le code du département. 
Pour la corse, le code sera '2A' ou '2B'. Pour les départements 97 et 98 le 
code est les trois premiers chiffres du code postal</field>
            <field name="language" ref="ir.lang_fr"/>
        </record>
        <record model="rule_engine.function" id="te_health_subscriber_department">
            <field name="description">Département du souscripteur</field>
            <field name="name">_re_health_subscriber_department</field>
            <field name="namespace">rule_engine.runtime</field>
            <field name="type">function</field>
            <field name="parent" ref="te_health"/>
            <field name="translated_technical_name">code_departement_souscripteur</field>
            <field name="long_description">Retourne le code du département du souscripteur.
Pour la corse, le code sera '2A' ou '2B'. Pour les départements 97 et 98 le 
code est les trois premiers chiffres du code posta</field>
            <field name="language" ref="ir.lang_fr"/>
        </record>
    </data>
</tryton>
