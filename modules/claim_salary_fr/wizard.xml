<tryton>
    <data>
         <!-- ##################### -->
        <!-- # Salaries Wizard # -->
        <!-- ##################### -->
        <record model="ir.action.wizard" id="salaries_wizard">
            <field name="name">Salaries Computation Wizard</field>
            <field name="wiz_name">claim.salaries_computation</field>
        </record>
        <!-- ########################### -->
        <!-- # Start Set Salaries # -->
        <!-- ########################### -->
        <record model="ir.ui.view" id="start_set_salaries_view_form">
            <field name="model">claim.start_set_salaries</field>
            <field name="type">form</field>
            <field name="name">start_set_salaries_form</field>
        </record>
        <!-- ########################### -->
        <!-- # Start Set Contributions # -->
        <!-- ########################### -->
        <record model="ir.ui.view" id="start_set_contributions_view_form">
            <field name="model">claim.start_set_contributions</field>
            <field name="type">form</field>
            <field name="name">start_set_contributions_form</field>
        </record>
        <record model="ir.ui.view" id="contributions_view_list">
            <field name="model">claim.contributions_view</field>
            <field name="type">tree</field>
            <field name="name">contributions_list</field>
        </record>
        <record model="ir.ui.view" id="fixed_contributions_view_list">
            <field name="model">claim.contributions_view</field>
            <field name="type">tree</field>
            <field name="name">fixed_contributions_list</field>
        </record>
    </data>
</tryton>
