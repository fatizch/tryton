<data>
    <xpath expr="/tree/field[@name='state']" position="replace"/>
    <xpath expr="/tree/field[@name='type']" position="before">
        <field name="state" icon="icon"/>
        <field name="business_kind"/>
    </xpath>
    <xpath expr="/tree" position="inside">
        <field name="color" tree_invisible="1"/>
    </xpath>
    <xpath expr="/tree/field[@name='type']" position="replace"/>
    <xpath expr="/tree" position="inside">
        <field name="type" tree_invisible="1"/>
    </xpath>
</data>
