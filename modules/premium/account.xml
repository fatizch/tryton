<?xml version="1.0"?>
<tryton>
    <data>
        <!-- ####### -->
        <!-- # Fee # -->
        <!-- ####### -->
        <record model="ir.ui.view" id="fee_view_form">
            <field name="model">account.fee</field>
            <field name="type">form</field>
            <field name="name">fee_form</field>
        </record>
        <record model="ir.ui.view" id="fee_view_list">
            <field name="model">account.fee</field>
            <field name="type">tree</field>
            <field name="name">fee_list</field>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_fee">
            <field name="name">Fees</field>
            <field name="res_model">account.fee</field>
        </record>
        <record model="ir.action.act_window.view" id="act_fee_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="fee_view_list"/>
            <field name="act_window" ref="act_fee"/>
        </record>
        <record model="ir.action.act_window.view" id="act_fee_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="fee_view_form"/>
            <field name="act_window" ref="act_fee"/>
        </record>
        <menuitem parent="account.menu_account_configuration" action="act_fee" id="menu_fee"/>
    </data>
</tryton>
