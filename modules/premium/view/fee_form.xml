<form>
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="type"/>
    <field name="type"/>
    <newline/>
    <label name="amount"/>
    <group name="amount" string="">
        <field name="amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="1" xalign="0"/>
    </group>
    <label name="frequency"/>
    <field name="frequency"/>
    <newline/>
    <label name="rate"/>
    <group name="rate" string="">
        <field name="rate" factor="100" xexpand="0"/>
        <label name="rate" string="%" xexpand="1" xalign="0"/>
    </group>
    <newline/>
    <label name="allow_override"/>
    <field name="allow_override"/>
</form>
