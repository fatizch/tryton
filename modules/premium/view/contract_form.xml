<data>
    <xpath expr="/form/notebook/page[@id='admin']" position="before">
        <page string="Invoicing" id="invoicing">
            <field name="fees" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/group[@id='button_change_start_date']" position="inside">
        <button string="Display Premium" name="button_display_premium"/>
    </xpath>
    <xpath expr="/form" position="inside">
        <field name="show_premium" invisible="1"/>
    </xpath>
</data>
