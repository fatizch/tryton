<tryton>
    <data>
       <!-- ******************************************* -->
       <!-- ***************** Request ***************** -->
       <!-- ******************************************* -->
        <record model="ir.ui.view" id="claim_ij_subscription_request_view_list">
            <field name="model">claim.ij.subscription_request</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_subscription_request_list</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_subscription_request_view_form">
            <field name="model">claim.ij.subscription_request</field>
            <field name="type">form</field>
            <field name="name">claim_ij_subscription_request_form</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_subscription_request_full_view_list">
            <field name="model">claim.ij.subscription_request</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_subscription_request_full_list</field>
        </record>
       <!-- ************************************************* -->
       <!-- ***************** Request group ***************** -->
       <!-- ************************************************* -->
        <record model="ir.ui.view" id="claim_ij_subscription_request_group_view_form">
            <field name="model">claim.ij.subscription_request.group</field>
            <field name="type">form</field>
            <field name="name">claim_ij_subscription_request_group_form</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_subscription_request_group_view_list">
            <field name="model">claim.ij.subscription_request.group</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_subscription_request_group_list</field>
        </record>
        <record model="ir.action.act_window" id="act_claim_ij_subscription_request_group_action">
            <field name="name">Prest IJ Requests Group</field>
            <field name="res_model">claim.ij.subscription_request.group</field>
            <field name="search_value" eval="[('state', '=', 'processing')]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_ij_subscription_request_group_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="claim_ij_subscription_request_group_view_form"/>
            <field name="act_window" ref="act_claim_ij_subscription_request_group_action"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_ij_subscription_request_group_list_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="claim_ij_subscription_request_group_view_list"/>
            <field name="act_window" ref="act_claim_ij_subscription_request_group_action"/>
        </record>
       <!-- ************************************************* -->
       <!-- ***************** Subscription ****************** -->
       <!-- ************************************************* -->
        <record model="ir.ui.view" id="claim_ij_subscription_view_list">
            <field name="model">claim.ij.subscription</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_subscription_list</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_subscription_view_form">
            <field name="model">claim.ij.subscription</field>
            <field name="type">form</field>
            <field name="name">claim_ij_subscription_form</field>
        </record>
        <record model="ir.action.act_window" id="act_claim_ij_subscription_relate_form">
            <field name="name">Subscriptions IJ</field>
            <field name="res_model">claim.ij.subscription</field>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_ij_subscription_form_view1">
            <field name="sequence" eval="20"/>
            <field name="view" ref="claim_ij_subscription_view_form"/>
            <field name="act_window" ref="act_claim_ij_subscription_relate_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_claim_ij_subscription_list_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="claim_ij_subscription_view_list"/>
            <field name="act_window" ref="act_claim_ij_subscription_relate_form"/>
        </record>
        <record model="ir.action.keyword" id="act_consult_ij_claim_subscription_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">claim,-1</field>
            <field name="action" ref="act_consult_ij_subscription"/>
        </record>
        <!-- ################### -->
        <!-- # Prest'IJ Period # -->
        <!-- ################### -->
        <record model="ir.ui.view" id="claim_ij_period_view_form">
            <field name="model">claim.ij.period</field>
            <field name="type">form</field>
            <field name="name">claim_ij_period_form</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_period_view_list">
            <field name="model">claim.ij.period</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_period_list</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_period_with_party_view_list">
            <field name="model">claim.ij.period</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_period_with_party_list</field>
        </record>
        <record model="ir.action.act_window" id="act_open_claim_ij_period">
            <field name="name">PrestIJ Period</field>
            <field name="res_model">claim.ij.period</field>
            <field name="domain" eval="[('party', 'in', Eval('active_ids'))]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_open_claim_ij_period_view_list">
            <field name="sequence" eval="10"/>
            <field name="view" ref="claim_ij_period_view_list"/>
            <field name="act_window" ref="act_open_claim_ij_period"/>
        </record>
        <record model="ir.action.act_window.view" id="act_open_claim_ij_period_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="claim_ij_period_view_form"/>
            <field name="act_window" ref="act_open_claim_ij_period"/>
        </record>
        <record model="ir.action.act_window" id="act_open_all_claim_ij_period">
            <field name="name">PrestIJ Period</field>
            <field name="res_model">claim.ij.period</field>
        </record>
        <record model="ir.action.act_window.view" id="act_open_all_claim_ij_period_view_list">
            <field name="sequence" eval="10"/>
            <field name="view" ref="claim_ij_period_with_party_view_list"/>
            <field name="act_window" ref="act_open_all_claim_ij_period"/>
        </record>
        <record model="ir.action.act_window.view" id="act_open_all_claim_ij_period_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="claim_ij_period_view_form"/>
            <field name="act_window" ref="act_open_all_claim_ij_period"/>
        </record>
        <!-- ################### -->
        <!-- # Indemnification # -->
        <!-- ################### -->
        <record model="ir.ui.view" id="indemnification_view_form">
            <field name="model">claim.indemnification</field>
            <field name="inherit" ref="claim_indemnification.indemnification_view_form"/>
            <field name="name">indemnification_form</field>
        </record>
        <record model="ir.ui.view" id="indemnification_view_list">
            <field name="model">claim.indemnification</field>
            <field name="inherit" ref="claim_indemnification.indemnification_view_list"/>
            <field name="name">indemnification_list</field>
        </record>
        <!-- ######################## -->
        <!-- # Prest'IJ Period Line # -->
        <!-- ######################## -->
        <record model="ir.ui.view" id="claim_ij_period_line_view_form">
            <field name="model">claim.ij.period.line</field>
            <field name="type">form</field>
            <field name="name">claim_ij_period_line_form</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_period_line_view_list">
            <field name="model">claim.ij.period.line</field>
            <field name="type">tree</field>
            <field name="name">claim_ij_period_line_list</field>
        </record>
        <!-- ############################################################ -->
        <!-- ############### Prest'IJ Erroneous Requests ################ -->
        <!-- ############################################################ -->
        <record model="ir.action.act_window" id="act_erroneous_requests_prest_ij">
            <field name="name">Erroneous Requests</field>
            <field name="res_model">claim.ij.subscription</field>
            <field name="domain" eval="[('state', '=', 'in_error')]" pyson="1"/>
        </record>
        <record model="ir.ui.view" id="claim_ij_relaunch_subscription_view_list">
            <field name="model">claim.ij.subscription</field>
            <field name="inherit" ref="claim_ij_subscription_view_list"/>
            <field name="name">claim_ij_relaunch_subscription_list</field>
        </record>
        <record model="ir.ui.view" id="claim_ij_relaunch_subscription_view_form">
            <field name="model">claim.ij.subscription</field>
            <field name="inherit" ref="claim_ij_subscription_view_form"/>
            <field name="name">claim_ij_relaunch_subscription_form</field>
        </record>
        <record model="ir.action.act_window.view" id="act_erroneous_requests_prest_ij_list_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="claim_ij_relaunch_subscription_view_list"/>
            <field name="act_window" ref="act_erroneous_requests_prest_ij"/>
        </record>
        <record model="ir.action.act_window.view" id="act_erroneous_requests_prest_ij_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="claim_ij_relaunch_subscription_view_form"/>
            <field name="act_window" ref="act_erroneous_requests_prest_ij"/>
        </record>
        <!-- ####################################################### -->
        <!-- ###################### Buttons  ####################### -->
        <!-- ####################################################### -->
        <record model="ir.model.button" id="subscription_relaunch_process_button">
            <field name="name">button_relaunch_process</field>
            <field name="model" search="[('model', '=', 'claim.ij.subscription')]"/>
        </record>
        <record model="ir.model.button" id="create_ij_subscription_request_button">
            <field name="name">button_create_ij_subscription_request</field>
            <field name="model" search="[('model', '=', 'claim.ij.subscription')]"/>
        </record>
        <record model="ir.model.button" id="button_start_period_treatment">
            <field name="name">button_start_period_treatment</field>
            <field name="model" search="[('model', '=', 'claim.ij.subscription')]"/>
        </record>
        <record model="ir.model.button" id="button_resume_process">
            <field name="name">button_resume</field>
            <field name="model" search="[('model', '=', 'claim.ij.subscription')]"/>
        </record>
        <!-- ############################################################ -->
        <!-- ###################### Prest'IJ Menu ####################### -->
        <!-- ############################################################ -->
        <menuitem name="Prest'IJ" id="menu_prest_ij" sequence="16" icon="batch"/>
        <menuitem name="Erroneous Requests" parent="menu_prest_ij" sequence="10" id="menu_erroneous_requests_prest_ij" action="act_erroneous_requests_prest_ij"/>
        <menuitem name="Prest IJ Requests Groups" parent="menu_prest_ij" sequence="20" id="menu_group_requests_prest_ij" action="act_claim_ij_subscription_request_group_action"/>
        <menuitem name="Prest'IJ Periods" id="menu_prest_ij_periods" parent="menu_prest_ij" sequence="30" icon="batch" action="act_open_all_claim_ij_period"/>
    </data>
</tryton>
