<tryton>
    <data>
        <!-- ############################################################ -->
        <!-- Select Date -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="select_date_form">
            <field name="model">contract.wizard.collection_to_cash_value.select_date</field>
            <field name="type">form</field>
            <field name="name">select_date_form</field>
        </record>
        <!-- ############################################################ -->
        <!-- Collection To Cash Value -->
        <!-- ############################################################ -->
        <record model="ir.action.wizard" id="act_collection_to_cash_value">
            <field name="name">Collection To Cash Value</field>
            <field name="wiz_name">contract.wizard.collection_to_cash_value</field>
        </record>
        <record model="ir.action.keyword" id="act_collection_to_cash_value_keyword">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_collection_to_cash_value"/>
        </record>
        <!-- ############################################################ -->
        <!-- Cash Value update -->
        <!-- ############################################################ -->
        <record model="ir.action.wizard" id="act_cash_value_update">
            <field name="name">Cash Value Update</field>
            <field name="wiz_name">contract.wizard.cash_value_update</field>
        </record>
        <record model="ir.action.keyword" id="act_cash_value_update_keyword">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_cash_value_update"/>
        </record>
        <!-- ############################################################ -->
        <!-- Cash Surrender Parameters -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="parameters_form">
            <field name="model">contract.wizard.cash_surrender.parameters</field>
            <field name="type">form</field>
            <field name="name">parameters_form</field>
        </record>
        <!-- ############################################################ -->
        <!-- Cash Surrender Wizard -->
        <!-- ############################################################ -->
        <record model="ir.action.wizard" id="act_cash_surrender">
            <field name="name">Cash Surrender</field>
            <field name="wiz_name">contract.wizard.cash_surrender</field>
        </record>
        <record model="ir.action.keyword" id="act_cash_surrender_keyword">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_cash_surrender"/>
        </record>
    </data>
</tryton>
