<tryton>
    <data>
        <!-- Pasrau Relate  -->
        <record model="ir.action.act_window" id="act_dsn_message">
            <field name="name">DSN messages</field>
            <field name="res_model">dsn.message</field>
            <field name="domain" eval="[If(Eval('active_ids', []) == [Eval('active_id')], ('origin.id', '=', Eval('active_id'), 'account.invoice'), ('origin.id', 'in', Eval('active_ids'), 'account.invoice'))]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_dsn_message_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="dsn_standard.dsn_message_view_list"/>
            <field name="act_window" ref="act_dsn_message"/>
        </record>
        <record model="ir.action.act_window.view" id="act_dsn_message_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="dsn_standard.dsn_message_view_form"/>
            <field name="act_window" ref="act_dsn_message"/>
        </record>
        <record model="ir.action.keyword" id="act_dsn_message_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">account.invoice,-1</field>
            <field name="action" ref="act_dsn_message"/>
        </record>
    </data>
    <data group="1">
        <record model="ir.message" id="msg_dsn_do_not_be_generated">
            <field name="text">You can not generate the DSN message after the 10th month.</field>
        </record>
        <record model="ir.message" id="msg_existing_messages_not_done">
            <field name="text">There are untreated DSN messages for this slip.</field>
        </record>
        <record model="ir.message" id="msg_invalid_file">
            <field name="text">The file is not valid.</field>
        </record>
        <record model="ir.message" id="msg_no_party_found">
            <field name="text">No party found for NIR %(ssn)s and matricule %(matricule)s.</field>
        </record>
        <record model="ir.message" id="msg_no_rate_found">
            <field name="text">No pasrau rate found for NIR %(ssn)s.</field>
        </record>
        <record model="ir.message" id="msg_party_date_unique">
            <field name="text">The rate must be unique per party / date.</field>
        </record>
        <record model="ir.message" id="msg_no_pasrau_region">
            <field name="text">Unable to find PASRAU region for zip code %(zip)s.</field>
        </record>
        <record model="ir.message" id="msg_no_default_pasrau">
            <field name="text">Could not compute a default pasrau value for parameters:

Zip: %(zip)s
Income: %(income)s
Start: %(start)s
End: %(end)s
Invoice Date: %(invoice_date)s</field>
        </record>
        <record model="ir.message" id="msg_no_zip_for_pasrau">
            <field name="text">A zip is required to compute the default pasrau rate.</field>
        </record>
    </data>
</tryton>
