<tryton>
    <data>
        <!-- ############################################################ -->
        <!-- Table Definition -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="table_definition_view_form">
            <field name="model">table</field>
            <field name="type">form</field>
            <field name="name">table_definition_form</field>
        </record>
        <record model="ir.ui.view" id="table_basic_data_view_form">
            <field name="model">table</field>
            <field name="type">form</field>
            <field name="name">table_basic_data_form</field>
        </record>
        <record model="ir.ui.view" id="table_definition_view_list">
            <field name="model">table</field>
            <field name="type">tree</field>
            <field name="name">table_definition_list</field>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_table_definition_form">
            <field name="name">Tables</field>
            <field name="res_model">table</field>
        </record>
        <record model="ir.action.act_window.view" id="act_table_definition_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="table_definition_view_list"/>
            <field name="act_window" ref="act_table_definition_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_table_definition_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="table_definition_view_form"/>
            <field name="act_window" ref="act_table_definition_form"/>
        </record>
        <menuitem action="act_table_definition_form" sequence="10" id="menu_table_defintion_form" icon="table"/>
        <!-- ############################################################ -->
        <!-- Table Definition Dimension -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="table_definition_dimension_view_form">
            <field name="model">table.dimension.value</field>
            <field name="type">form</field>
            <field name="name">dimension_value_form</field>
        </record>
        <record model="ir.ui.view" id="table_definition_dimension_view_list">
            <field name="model">table.dimension.value</field>
            <field name="type">tree</field>
            <field name="name">dimension_value_list</field>
        </record>
        <record model="ir.ui.view" id="table_definition_dimension_sequence_view_list">
            <field name="model">table.dimension.value</field>
            <field name="type">tree</field>
            <field name="name">dimension_value_sequence_list</field>
        </record>
        <record model="ir.ui.view" id="table_dimension_value_open_ask_type_view_form">
            <field name="model">table.dimension.value.open.ask_type</field>
            <field name="type">form</field>
            <field name="name">dimension_value_open_ask_type_form</field>
        </record>
        <record model="ir.action.act_window" id="act_definition_dimension_form">
            <field name="name">Dimension Value</field>
            <field name="res_model">table.dimension.value</field>
        </record>
        <record model="ir.action.wizard" id="act_dimension_value_open">
            <field name="name">Define Dimension Value</field>
            <field name="wiz_name">table.dimension.value.open</field>
        </record>
        <record model="ir.action.keyword" id="act_dimension_value_open_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">table,-1</field>
            <field name="action" ref="act_dimension_value_open"/>
        </record>
        <!-- ############################################################ -->
        <!-- Table Cell -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="cell_view_form">
            <field name="model">table.cell</field>
            <field name="type">form</field>
            <field name="name">cell_form</field>
        </record>
        <record model="ir.ui.view" id="cell_view_list">
            <field name="model">table.cell</field>
            <field name="type">tree</field>
            <field name="name">cell_list</field>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_cell_form">
            <field name="name">Cells</field>
            <field name="res_model">table.cell</field>
        </record>
        <record model="ir.action.act_window.view" id="act_cell_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="cell_view_list"/>
            <field name="act_window" ref="act_cell_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_cell_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="cell_view_form"/>
            <field name="act_window" ref="act_cell_form"/>
        </record>
        <!-- Relate -->
        <record model="ir.action.act_window" id="act_cell_relate_form">
            <field name="name">Cells</field>
            <field name="res_model">table.cell</field>
            <field name="domain" eval="[('definition', '=', Eval('active_id'))]" pyson="1"/>
            <field name="context" eval="{'table': Eval('active_id')}" pyson="1"/>
        </record>
        <record model="ir.action.keyword" id="act_table_relate_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">table,-1</field>
            <field name="action" ref="act_cell_relate_form"/>
        </record>
    </data>
</tryton>
