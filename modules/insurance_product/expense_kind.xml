<?xml version="1.0"?>
<tryton>
    <data>
        <!--################################################################ -->
        <!-- Expenses kind-->
        <!--################################################################ -->

        <record model="ir.ui.view" id="expense_kind_view_tree">
            <field name="model">ins_product.expense_kind</field>
            <field name="type">tree</field>
            <field name="name">expense_kind_tree</field>
        </record>
        <record model="ir.ui.view" id="expense_kind_view_form">
            <field name="model">ins_product.expense_kind</field>
            <field name="type">form</field>
            <field name="name">expense_kind_form</field>
        </record>
        <record model="ir.action.act_window" id="act_model_expense_kind">
            <field name="name">Expenses Kind</field>
            <field name="res_model">ins_product.expense_kind</field>
        </record>
        <record model="ir.action.act_window.view" id="act_model_expense_kind_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="expense_kind_view_tree"/>
            <field name="act_window" ref="act_model_expense_kind"/>
        </record>
        <record model="ir.action.act_window.view" id="act_model_expense_kind_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="expense_kind_view_form"/>
            <field name="act_window" ref="act_model_expense_kind"/>
        </record>
        <menuitem parent="menu_product_configuration" action="act_model_expense_kind"
            id="menu_model_expense_kind"/>
    </data>
</tryton>
