<?xml version="1.0"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Product"/>
    </xpath>
    <xpath
        expr="/form/notebook/page/notebook/page[@id='eligibility']"
        position="before">
        <page string="Complementary Data" id="complementary_data">
            <field name="complementary_data_def" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='managers']" position="before">
        <page string="Items/Coverages" id="option">
            <field name="item_descriptors"/>
            <field name="coverages"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page/notebook" position="inside">
        <page string="Deductible" id="deductible">
            <field name="deductible_rules" colspan="4"/>
        </page>
        <page string="Clause" id="clause">
            <field name="clause_rules" colspan="4"/>
        </page>
        <page string="Documents" id="documents">
            <field name="document_rules"/>
        </page>
        <page string="Term - Renewal" id="term_renewal">
            <field name="term_renewal_rules" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/group[@id='left']"
        position="inside">
        <label name="contract_generator"/>
        <field name="contract_generator"/>
        <newline/>
        <label name="currency"/>
        <field name="currency"/>
        <newline/>
        <label name="tmp_claim_manager"/>
        <field name="tmp_claim_manager"/>
        <newline/>
        <label name="tmp_contract_manager"/>
        <field name="tmp_contract_manager"/>
    </xpath>
</data>
