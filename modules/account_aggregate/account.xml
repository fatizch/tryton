<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="journal_view_form">
            <field name="model">account.journal</field>
            <field name="inherit" ref="account.journal_view_form"/>
            <field name="name">journal_form</field>
        </record>

        <record model="ir.ui.view" id="move_snapshot_start_view_form">
            <field name="model">account.move.snapshot.start</field>
            <field name="type">form</field>
            <field name="name">snapshot_start_form</field>
        </record>

        <record model="ir.ui.view" id="move_snapshot_done_view_form">
            <field name="model">account.move.snapshot.done</field>
            <field name="type">form</field>
            <field name="name">snapshot_done_form</field>
        </record>

        <record model="ir.action.wizard" id="act_snapshot">
            <field name="name">Take a Snapshot of Moves</field>
            <field name="wiz_name">account.move.snapshot</field>
        </record>
        <menuitem parent="account.menu_processing" action="act_snapshot"
            id="menu_snapshot" sequence="50"/>

        <record model="ir.ui.view" id="move_line_aggregated_view_form">
            <field name="model">account.move.line.aggregated</field>
            <field name="type">form</field>
            <field name="name">move_line_aggregated_form</field>
        </record>
        <record model="ir.ui.view" id="move_line_aggregated_view_list">
            <field name="model">account.move.line.aggregated</field>
            <field name="type">tree</field>
            <field name="name">move_line_aggregated_list</field>
        </record>

        <record model="ir.action.act_window" id="act_move_line_aggregated_form">
            <field name="name">Account Move Lines Aggregated</field>
            <field name="res_model">account.move.line.aggregated</field>
        </record>
        <record model="ir.action.act_window.view"
            id="act_move_line_aggregated_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="move_line_aggregated_view_list"/>
            <field name="act_window" ref="act_move_line_aggregated_form"/>
        </record>
        <record model="ir.action.act_window.view"
            id="act_move_line_aggregated_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="move_line_aggregated_view_form"/>
            <field name="act_window" ref="act_move_line_aggregated_form"/>
        </record>

        <record model="ir.action.wizard" id="act_open_line_aggregated">
            <field name="name">Open Move Aggregated Account</field>
            <field name="wiz_name">account.move.open_line_aggregated</field>
            <field name="model">account.account</field>
        </record>
        <record model="ir.action.keyword" id="act_open_line_aggregated_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">account.account,-1</field>
            <field name="action" ref="act_open_line_aggregated"/>
        </record>

        <record model="ir.model.access" id="access_move_line_aggregated">
            <field name="model" search="[('model', '=', 'account.move.line.aggregated')]"/>
            <field name="perm_read" eval="False"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_move_line_aggregated_account">
            <field name="model" search="[('model', '=', 'account.move.line.aggregated')]"/>
            <field name="group" ref="account.group_account"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>

        <record model="ir.action.wizard" id="act_open_line">
            <field name="name">Open Aggregate</field>
            <field name="wiz_name">account.move.line.aggregated.open_line</field>
            <field name="model">account.move.line.aggregated</field>
        </record>
        <record model="ir.action.keyword" id="act_open_line_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">account.move.line.aggregated,-1</field>
            <field name="action" ref="act_open_line"/>
        </record>

        <menuitem name="Account Move Line Aggregates" parent="account.menu_entries"
            action="act_move_line_aggregated_form" id="menu_account_move_line_aggregates_form"/>
    </data>
</tryton>
