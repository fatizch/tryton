        <record model="ir.ui.menu-res.group" id="menu_offered_distribution_network_read">
            <field name="menu" ref="distribution.menu_dist_network_form"/>
            <field name="group" ref="offered.group_product_read"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_commercial_product_manage">
            <field name="menu" ref="contract_distribution.menu_commercial_product_form"/>
            <field name="group" ref="offered.group_product_manage"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_commercial_product_read">
            <field name="menu" ref="contract_distribution.menu_commercial_product_form"/>
            <field name="group" ref="offered.group_product_read"/>
        </record>
    </data>
</tryton>
