<tryton>
    <data>
        <!-- Model -->
        <record model="ir.model.access" id="access_commercial_product_default">
            <field name="model" search="[('model', '=', 'distribution.commercial_product')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_distribution_network_commercial_product_default">
            <field name="model" search="[('model', '=', 'distribution.network-commercial_product')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_offered_distribution_network_manage">
            <field name="model" search="[('model', '=', 'distribution.network')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_commercial_product_manage">
            <field name="model" search="[('model', '=', 'distribution.commercial_product')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_distribution_network_commercial_product_manage">
            <field name="model" search="[('model', '=', 'distribution.network-commercial_product')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <!-- Menu -->
        <record model="ir.ui.menu-res.group" id="menu_offered_distribution_manage">
            <field name="menu" ref="distribution.menu_distribution"/>
            <field name="group" ref="offered.group_product_manage"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_offered_distribution_read">
            <field name="menu" ref="distribution.menu_distribution"/>
            <field name="group" ref="offered.group_product_read"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_offered_distribution_network_manage">
            <field name="menu" ref="distribution.menu_dist_network_form"/>
