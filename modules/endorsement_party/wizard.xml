<tryton>
    <data>
        <!-- ########################### -->
        <!-- # Change Party Birth Date # -->
        <!-- ########################### -->
        <record model="ir.ui.view" id="party_change_birth_date_view_form">
            <field name="model">endorsement.party.change_birth_date</field>
            <field name="type">form</field>
            <field name="name">party_change_birth_date_form</field>
        </record>
        <!-- ########################### -->
        <!-- # Change Party Death Date # -->
        <!-- ########################### -->
        <record model="ir.ui.view" id="party_change_death_date_view_form">
            <field name="model">endorsement.party.change_death_date</field>
            <field name="type">form</field>
            <field name="name">party_change_death_date_form</field>
        </record>
        <!-- ######################### -->
        <!-- # Change Party Address  # -->
        <!-- ######################### -->
        <record model="ir.ui.view" id="party_change_address_displayer_view_tree">
            <field name="model">endorsement.party.change_address_displayer</field>
            <field name="type">tree</field>
            <field name="name">party_change_address_displayer_tree</field>
        </record>
        <record model="ir.ui.view" id="party_change_address_displayer_view_form">
            <field name="model">endorsement.party.change_address_displayer</field>
            <field name="type">form</field>
            <field name="name">party_change_address_displayer_form</field>
        </record>
        <record model="ir.ui.view" id="party_change_address_view_form">
            <field name="model">endorsement.party.change_address</field>
            <field name="type">form</field>
            <field name="name">party_change_address_form</field>
        </record>
        <!-- #################### -->
        <!-- # Change Party Name # -->
        <!-- #################### -->
        <record model="ir.ui.view" id="party_change_name_displayer_view_form">
            <field name="model">endorsement.party.change_name.displayer</field>
            <field name="type">form</field>
            <field name="name">party_change_name_displayer_form</field>
        </record>
        <record model="ir.ui.view" id="party_change_name_displayer_view_list">
            <field name="model">endorsement.party.change_name.displayer</field>
            <field name="type">tree</field>
            <field name="name">party_change_name_displayer_list</field>
        </record>
        <record model="ir.ui.view" id="party_change_name_view_form">
            <field name="model">endorsement.party.change_name</field>
            <field name="type">form</field>
            <field name="name">party_change_name_form</field>
        </record>
        <!-- ############################################################ -->
        <!-- ########### Change Party Relations ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="party_change_relations_view_form">
            <field name="model">endorsement.party.change_relations</field>
            <field name="type">form</field>
            <field name="name">party_change_relations_form</field>
        </record>
        <record model="ir.ui.view" id="party_change_relation_displayer_view_tree">
            <field name="model">endorsement.party.change_relation_displayer</field>
            <field name="inherit" ref="party_cog.relation_view_editable_list"/>
            <field name="name">party_change_relation_displayer_tree</field>
        </record>
        <record model="ir.ui.view" id="party_change_relation_displayer_view_form">
            <field name="model">endorsement.party.change_relation_displayer</field>
            <field name="inherit" ref="party_relationship.relation_view_form"/>
            <field name="name">party_change_relation_displayer_form</field>
        </record>
        <!-- ###################### -->
        <!-- # Select Endorsement # -->
        <!-- ###################### -->
        <record model="ir.ui.view" id="select_endorsement_view_form">
            <field name="model">endorsement.start.select_endorsement</field>
            <field name="inherit" ref="endorsement.select_endorsement_view_form"/>
            <field name="name">select_endorsement_form</field>
        </record>
        <record model="ir.action.keyword" id="act_party_start_endorsement_shorcut_keyword">
            <field name="keyword">form_toolbar</field>
            <field name="model">party.party,-1</field>
            <field name="action" ref="endorsement.act_start_endorsement"/>
        </record>
    </data>
    <data depends="party_ssn">
        <!-- #################### -->
        <!-- # Change Party SSN # -->
        <!-- #################### -->
        <record model="ir.ui.view" id="party_change_ssn_view_form">
            <field name="model">endorsement.party.change_ssn</field>
            <field name="type">form</field>
            <field name="name">party_change_ssn_form</field>
        </record>
    </data>
</tryton>
