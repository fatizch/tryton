<tryton>
    <data>
        <!-- Claim Underwriting Group -->
        <record model="res.group" id="group_claim_underwriting">
            <field name="name">Coog Claim Underwriting</field>
        </record>
        <!-- Users -->
        <record model="res.user-res.group" id="user_admin_group_claim_underwriting">
            <field name="user" ref="res.user_admin"/>
            <field name="group" ref="group_claim_underwriting"/>
        </record>
        <!-- Underwriting Default Access Rights -->
        <record model="ir.model.access" id="access_underwriting_decision_type">
            <field name="model" search="[('model', '=', 'underwriting.decision.type')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_underwriting_result">
            <field name="model" search="[('model', '=', 'underwriting.result')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_underwriting_type">
            <field name="model" search="[('model', '=', 'underwriting.type')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_underwriting_document_rule">
            <field name="model" search="[('model', '=', 'underwriting.document_rule')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_underwriting">
            <field name="model" search="[('model', '=', 'underwriting')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <!-- Buttons -->
        <record model="ir.model.button-res.group" id="button_finalize_claim_underwriting">
            <field name="button" ref="button_finalize"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>
        <record model="ir.model.button-res.group" id="button_abandon_claim_underwriting">
            <field name="button" ref="button_abandon"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>

        <record model="ir.model.button-res.group" id="button_draft_claim_underwriting">
            <field name="button" ref="button_draft"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>
        <record model="ir.model.button-res.group" id="button_process_claim_underwriting">
            <field name="button" ref="button_process"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>
        <record model="ir.model.button-res.group" id="button_complete_claim_underwriting">
            <field name="button" ref="button_complete"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>
        <record model="ir.model.button-res.group" id="button_plan_button_claim_underwriting">
            <field name="button" ref="button_plan_button"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>
        <record model="ir.model.button-res.group" id="button_generic_send_letter_claim_underwriting">
            <field name="button" ref="button_generic_send_letter"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>
        <record model="ir.model.button-res.group" id="button_add_new_result_claim_underwriting">
            <field name="button" ref="button_add_new_result"/>
            <field name="group" ref="underwriting.group_claim_underwriting"/>
        </record>

    </data>
</tryton>
