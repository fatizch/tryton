<tryton>
    <data>
        <!-- Commission Consultation Group -->
        <!-- Distribution -->
        <record model="ir.ui.menu-res.group" id="menu_commercial_product_form_commission_read">
            <field name="menu" ref="contract_distribution.menu_commercial_product_form"/>
            <field name="group" ref="commission_insurance.group_commission_read"/>
        </record>
        <!-- Commission Management Group -->
        <record model="ir.ui.menu-res.group" id="menu_commercial_product_form_commission_manage">
            <field name="menu" ref="contract_distribution.menu_commercial_product_form"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
        </record>
        <record model="ir.model.access" id="access_distribution_commercial_product_group_commission_manage">
            <field name="model" search="[('model', '=', 'distribution.commercial_product')]"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_distribution_network-commercial_product_group_commission_manage">
            <field name="model" search="[('model', '=', 'distribution.network-commercial_product')]"/>
            <field name="group" ref="commission_insurance.group_commission_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
    </data>
</tryton>
