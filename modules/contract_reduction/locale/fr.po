msgctxt "model:ir.message,text:msg_auto_reducing"
msgid "Contracts will automatically be reduced."
msgstr "Les contrats vont être automatiquement mis en réduction."

msgctxt "model:ir.message,text:msg_cannot_reduce"
msgid "Contract %(contract)s cannot be reduced."
msgstr "Le contrat %(contract)s ne peut pas être mis en réduction."

msgctxt "model:ir.message,text:msg_cannot_reduce_surrendered"
msgid "A contract that was surrendered cannot be reduced."
msgstr "Il n'est pas possible de mettre en réduction un contrat racheté."

msgctxt "model:ir.message,text:msg_contract_not_reduced"
msgid ""
"Contract %(contract)s is not reduced. Therefore, it cannot be reactivated."
msgstr ""
"Le contract %(contract)s n'est pas réduit. Il n'est donc pas possible de le "
"réactiver."

msgctxt "model:ir.message,text:msg_invalid_date"
msgid ""
"Impossible to reduce the contract before the contract's start_date "
"(%(start_date)s)."
msgstr ""
"Impossible de réduire le contrat avant sa date d'effet (%(start_date)s)."

msgctxt "model:ir.message,text:msg_no_cancellation_on_reduced"
msgid ""
"The following contracts are reduced:\n"
"\n"
"%(contracts)s\n"
"\n"
"You cannot cancel endorsements on reduced contracts."
msgstr ""
"Les contrats suivants seront réduits:\n"
"\n"
"%(contracts)s\n"
"Vous ne pouvez annuler des avenants sur des contrats réduits."

msgctxt "model:ir.message,text:msg_no_reductionable_options"
msgid "No options were found that can be reduced for contract %(contract)s."
msgstr ""
"Aucune option ne peut être mise en réduction sur le contrat %(contract)s."

msgctxt "model:ir.message,text:msg_reduced_contract"
msgid ""
"Contract %(contract)s has been reduced since %(date)s, you cannot perform "
"any endorsements on it."
msgstr ""
"Le contrat %(contract)s est réduit depuis le %(date)s, vous ne pouvez lui "
"appliquer un avenant."

msgctxt "model:ir.message,text:msg_terminate_reduced_contract"
msgid ""
"Contract %(contract)s has been reduced since %(date)s, do you want to apply "
"this endorsement on it."
msgstr ""
"Le contrat %(contract)s est réduit depuis le %(date)s, voulez-vous lui "
"appliquer cet avenant."

msgctxt "model:ir.message,text:msg_will_cancel_reduction"
msgid "Contracts reduction will be cancelled."
msgstr "La mise en réduction des contrats sera annulée."

msgctxt "model:ir.message,text:msg_will_reduce"
