<tryton>
    <data>
        <record model="contract.sub_status" id="sub_status_reduced">
            <field name="name">Contract Reduced</field>
            <field name="code">contract_reduced</field>
            <field name="status">terminated</field>
        </record>
        <record model="contract.sub_status" id="sub_status_active_reduced">
            <field name="name">Contract Reduced</field>
            <field name="code">contract_active_reduced</field>
            <field name="status">active</field>
        </record>
    </data>
    <data depends="contract_insurance_invoice_dunning">
        <record model="contract.sub_status" id="contract_reduction.sub_status_reduced">
            <field name="deactivate_dunnings" eval="True"/>
        </record>
        <record model="contract.sub_status" id="contract_reduction.sub_status_active_reduced">
            <field name="deactivate_dunnings" eval="True"/>
        </record>
    </data>
</tryton>
