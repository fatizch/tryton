<data>
    <xpath expr="/form/group[@id='right']/field[@name='start_date']" position="after">
        <label name="reduction_date"/>
        <field name="reduction_date"/>
    </xpath>
    <xpath expr="/form/group[@id='button_change_start_date']" position="inside">
        <field name="can_reduce" invisible="1"/>
