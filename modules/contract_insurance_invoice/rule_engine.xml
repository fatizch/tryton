<tryton>
    <data noupdate="1">
        <record model="rule_engine.function" id="dossier_quittancement">
            <field name="translated_technical_name">dossier_quittancement</field>
            <field name="description">Quittancement</field>
            <field name="type">folder</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="parent" ref="contract.te_contract"/>
        </record>
        <record model="rule_engine.function" id="billing_folder">
            <field name="translated_technical_name">billing_folder</field>
            <field name="description">Billing</field>
            <field name="type">folder</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="parent" ref="contract.te_contract_en"/>
        </record>
    </data>
    <data>
        <record model="rule_engine.function" id="frequence_quittancement_code">
            <field name="translated_technical_name">frequence_quittancement_code</field>
            <field name="description">Code Fréquence Quittancement</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_billing_frequency</field>
            <field name="long_description">Retourne le code de la fréquence de
 facturation parmis les valeurs suivantes: 'once_per_contract', 'monthly',
 'quarterly', 'half_yearly', 'yearly'</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="frequence_quittancement_nom">
            <field name="translated_technical_name">frequence_quittancement_nom</field>
            <field name="description">Nom Fréquence Quittancement</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_billing_frequency_string</field>
            <field name="long_description">Retourne le nom de la fréquence
 de facturation traduite. Les valeurs possibles sont 'Une fois par contrat',
 'Mensuel', 'Trimestriel', 'Semestriel', 'Annuel'</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="prelevement_automatique">
            <field name="translated_technical_name">est_en_prelevement_automatique</field>
            <field name="description">Est en Prélèvement Automatique</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_direct_debit</field>
            <field name="long_description">Retourne vrai si la quittance sera
 prélevé automatiquement</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="jour_prelevement_automatique">
            <field name="translated_technical_name">jour_prelevement_automatique</field>
            <field name="description">Jour de Prélèvement</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_direct_debit_day</field>
            <field name="long_description">Retourne le jour de prélèvement</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="prime_unique">
            <field name="translated_technical_name">est_prime_unique</field>
            <field name="description">Est en Prime Unique</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_billing_is_once_per_contract</field>
            <field name="long_description">Retourne vrai si le mode quittancement
 est défini en prime unique</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="blocage_du_quittancement">
            <field name="translated_technical_name">block_invoicing_until</field>
            <field name="description">Blocage du quittancement jusqu'au</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_block_invoicing_until</field>
            <field name="long_description">Retourne la date éventuelle jusqu'à laquelle le quittancement est bloqué</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="billing_frequency_code">
            <field name="translated_technical_name">billing_frequency_code</field>
            <field name="description">Billing Frequency Code</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_billing_frequency</field>
            <field name="long_description">Return the billing frequency code.
 The value can be 'once_per_contract', 'monthly', 'quarterly', 'half_yearly',
 'yearly''</field>
            <field name="parent" ref="billing_folder"/>
        </record>
        <record model="rule_engine.function" id="billing_frequency_name">
            <field name="translated_technical_name">billing_frequency_name</field>
            <field name="description">Billing Frequency Name</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_billing_frequency_string</field>
            <field name="long_description">Return the billing frequency code.
 The value can be 'once_per_contract', 'monthly', 'quarterly', 'half_yearly',
 'yearly'</field>
            <field name="parent" ref="billing_folder"/>
        </record>
        <record model="rule_engine.function" id="is_direct_debit">
            <field name="translated_technical_name">is_direct_debit</field>
            <field name="description">Is Direct Debit</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_direct_debit</field>
            <field name="long_description">Return True if the billing mode of
 the contract is direct debit </field>
            <field name="parent" ref="billing_folder"/>
        </record>
        <record model="rule_engine.function" id="direct_debit_day">
            <field name="translated_technical_name">direct_debit_day</field>
            <field name="description">Direct Debit Day</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_direct_debit_day</field>
            <field name="long_description">Return the day when the invoice is
 withdrawn</field>
            <field name="parent" ref="billing_folder"/>
        </record>
        <record model="rule_engine.function" id="once_per_contract">
            <field name="translated_technical_name">is_once_per_contract</field>
            <field name="description">Is Once Per Contract</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_contract_billing_is_once_per_contract</field>
            <field name="long_description">Return True is the billing mode is
 once per contract</field>
            <field name="parent" ref="billing_folder"/>
        </record>
        <record model="rule_engine.function" id="option_paid_amount">
            <field name="translated_technical_name">montant_payee_pour_garantie</field>
            <field name="description">Montant payé pour la garantie à la date</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="fct_args">date</field>
            <field name="name">_re_get_option_paid_amount_at_date</field>
            <field name="long_description">Retourne la somme des quittances
 payées à aujourd'hui pour une garantie sur une période de couverture allant
 jusqu'à la date passée en paramètre.</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="last_paid_invoice_end">
            <field name="translated_technical_name">date_fin_derniere_quittance_payee</field>
            <field name="description">Date de fin de la dernière quittance payée sur le contrat</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_last_paid_invoice_end</field>
            <field name="long_description">Retourne la date de fin de la dernière
 quittance payée sur le contrat</field>
            <field name="parent" ref="dossier_quittancement"/>
        </record>
        <record model="rule_engine.function" id="block_invoicing_until">
            <field name="translated_technical_name">block_invoicing_until</field>
            <field name="description">Block Invoicing Until</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_en"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_block_invoicing_until</field>
            <field name="long_description">Returns the date (if existing) until the invoicing is blocked</field>
            <field name="parent" ref="billing_folder"/>
        </record>
    </data>
</tryton>
