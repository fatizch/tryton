<?xml version="1.0"?>
<tryton>
    <data>

        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="type">form</field>
            <field name="name">contract_form</field>
        </record>

        <record model="ir.ui.view" id="premium_view_form">
            <field name="model">contract.premium</field>
            <field name="type">form</field>
            <field name="name">premium_form</field>
        </record>
        <record model="ir.ui.view" id="premium_view_list">
            <field name="model">contract.premium</field>
            <field name="type">tree</field>
            <field name="name">premium_list</field>
        </record>

        <record model="ir.ui.view" id="contract_billing_information_view_list">
            <field name="model">contract.billing_information</field>
            <field name="type">tree</field>
            <field name="name">contract_billing_information_list</field>
        </record>
        <record model="ir.ui.view" id="contract_billing_information_view_form">
            <field name="model">contract.billing_information</field>
            <field name="type">form</field>
            <field name="name">contract_billing_information_form</field>
        </record>
        <record model="ir.ui.view" id="contract_billing_information_without_date_view_form">
            <field name="model">contract.billing_information</field>
            <field name="type">form</field>
            <field name="name">contract_billing_information_without_date_form</field>
        </record>

        <record model="ir.ui.view" id="contract_invoice_view_list">
            <field name="model">contract.invoice</field>
            <field name="type">tree</field>
            <field name="name">contract_invoice_list</field>
        </record>
        <record model="ir.ui.view" id="contract_invoice_view_form">
            <field name="model">contract.invoice</field>
            <field name="type">form</field>
            <field name="name">contract_invoice_form</field>
        </record>

        <record model="ir.action.act_window" id="act_invoice_form">
            <field name="name">Contract Invoices</field>
            <field name="res_model">account.invoice</field>
            <field name="domain">[('contract', 'in', Eval('active_ids'))]</field>
        </record>
        <record model="ir.action.act_window.view" id="act_invoice_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="invoice_view_list"/>
            <field name="act_window" ref="act_invoice_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_invoice_view2">
            <field name="sequence" eval="10"/>
            <field name="view" ref="invoice_view_form"/>
            <field name="act_window" ref="act_invoice_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_invoice_domain_posted_paid">
            <field name="name">Posted - Paid</field>
            <field name="sequence" eval="10"/>
            <field name="domain">[('state', 'in', ('posted', 'paid'))]</field>
            <field name="act_window" ref="act_invoice_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_invoice_domain_canceled">
            <field name="name">Canceled</field>
            <field name="sequence" eval="20"/>
            <field name="domain">[('state', '=', 'cancel')]</field>
            <field name="act_window" ref="act_invoice_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_invoice_domain_all">
            <field name="name">All</field>
            <field name="sequence" eval="9999"/>
            <field name="domain"></field>
            <field name="act_window" ref="act_invoice_form"/>
        </record>

        <record model="ir.action.keyword" id="act_invoice_form_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_invoice_form"/>
        </record>

        <record model="ir.ui.view" id="invoice_start_view_form">
            <field name="model">contract.invoice.start</field>
            <field name="type">form</field>
            <field name="name">invoice_start_form</field>
        </record>

        <record model="ir.action.wizard" id="act_invoice">
            <field name="name">Invoice Contracts</field>
            <field name="wiz_name">contract.invoice</field>
            <field name="model">contract</field>
        </record>
        <record model="ir.action.keyword" id="act_invoice_keyword1">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_invoice"/>
        </record>

        <record model="ir.ui.view" id="display_premiums_view_form">
            <field name="model">contract.premium.display.premiums</field>
            <field name="type">form</field>
            <field name="name">display_premiums_form</field>
        </record>

        <record model="ir.ui.view" id="display_premiums_line_view_form">
            <field name="model">contract.premium.display.premiums.line</field>
            <field name="type">form</field>
            <field name="name">display_premiums_line_form</field>
        </record>
        <record model="ir.ui.view" id="display_premiums_line_view_list">
            <field name="model">contract.premium.display.premiums.line</field>
            <field name="type">tree</field>
            <field name="field_childs">childs</field>
            <field name="name">display_premiums_line_list</field>
        </record>

        <record model="ir.action.wizard" id="act_premium_display">
            <field name="name">Display Contract Premium</field>
            <field name="wiz_name">contract.premium.display</field>
        </record>
        <record model="ir.action.keyword" id="act_premium_display_keyword">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_premium_display"/>
        </record>
    </data>
</tryton>
