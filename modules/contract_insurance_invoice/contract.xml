<?xml version="1.0"?>
<tryton>
    <data>

        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="name">contract_form</field>
        </record>

        <record model="ir.ui.view" id="covered_element_view_form">
            <field name="model">contract.covered_element</field>
            <field name="inherit" ref="contract_insurance.covered_element_view_form"/>
            <field name="name">covered_element_form</field>
        </record>

        <record model="ir.ui.view" id="option_view_form">
            <field name="model">contract.option</field>
            <field name="inherit" ref="contract.option_view_form"/>
            <field name="name">option_form</field>
        </record>

        <record model="ir.ui.view" id="extra_premium_view_form">
            <field name="model">contract.option.extra_premium</field>
            <field name="inherit" ref="contract_insurance.extra_premium_view_form"/>
            <field name="name">extra_premium_form</field>
        </record>

        <record model="ir.ui.view" id="premium_view_form">
            <field name="model">contract.premium</field>
            <field name="type">form</field>
            <field name="name">premium_form</field>
        </record>
        <record model="ir.ui.view" id="premium_view_list">
            <field name="model">contract.premium</field>
            <field name="type">tree</field>
            <field name="name">premium_list</field>
        </record>

        <record model="ir.ui.view" id="payment_term_view_list">
            <field name="model">contract.payment_term</field>
            <field name="type">tree</field>
            <field name="name">payment_term_list</field>
        </record>
        <record model="ir.ui.view" id="payment_term_view_form">
            <field name="model">contract.payment_term</field>
            <field name="type">form</field>
            <field name="name">payment_term_form</field>
        </record>

        <record model="ir.ui.view" id="contract_invoice_frequency_view_list">
            <field name="model">contract.invoice_frequency</field>
            <field name="type">tree</field>
            <field name="name">contract_invoice_frequency_list</field>
        </record>
        <record model="ir.ui.view" id="contract_invoice_frequency_view_form">
            <field name="model">contract.invoice_frequency</field>
            <field name="type">form</field>
            <field name="name">contract_invoice_frequency_form</field>
        </record>

        <record model="ir.ui.view" id="contract_invoice_view_list">
            <field name="model">contract.invoice</field>
            <field name="type">tree</field>
            <field name="name">contract_invoice_list</field>
        </record>
        <record model="ir.ui.view" id="contract_invoice_view_form">
            <field name="model">contract.invoice</field>
            <field name="type">form</field>
            <field name="name">contract_invoice_form</field>
        </record>

        <record model="ir.action.act_window" id="act_invoice_form">
            <field name="name">Contract Invoices</field>
            <field name="res_model">contract.invoice</field>
            <field name="domain">[('contract', 'in', Eval('active_ids'))]</field>
        </record>
        <record model="ir.action.keyword" id="act_invoice_form_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_invoice_form"/>
        </record>

        <record model="ir.ui.view" id="invoice_start_view_form">
            <field name="model">contract.invoice.start</field>
            <field name="type">form</field>
            <field name="name">invoice_start_form</field>
        </record>

        <record model="ir.action.wizard" id="act_invoice">
            <field name="name">Invoice Contracts</field>
            <field name="wiz_name">contract.invoice</field>
            <field name="model">contract</field>
        </record>
        <record model="ir.action.keyword" id="act_invoice_keyword1">
            <field name="keyword">form_action</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_invoice"/>
        </record>
    </data>
</tryton>
