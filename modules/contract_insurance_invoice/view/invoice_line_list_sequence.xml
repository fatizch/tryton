<?xml version="1.0"?>
<data>
    <xpath expr="/tree/field[@name='type']" position="after">
        <field name="coverage_start"/>
        <field name="coverage_end"/>
        <field name="covered_element"/>
        <field name="type_"/>
        <field name="amount">
            <suffix name="currency_symbol"/>
        </field>
    </xpath>
    <xpath expr="/tree/field[@name='description']" position="replace_attributes">
        <field name="description" expand="1"/>
    </xpath>
    <xpath expr="/tree/field[@name='type']" position="replace"/>
    <xpath expr="/tree/field[@name='product']" position="replace"/>
    <xpath expr="/tree/field[@name='quantity']" position="replace"/>
    <xpath expr="/tree/field[@name='unit']" position="replace"/>
    <xpath expr="/tree/field[@name='unit_price']" position="replace"/>
    <xpath expr="/tree/field[@name='taxes']" position="replace"/>
    <xpath expr="/tree/field[@name='account']" position="replace"/>
    <xpath expr="/tree/field[@name='amount']" position="replace"/>
</data>
