<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page[@id='admin']" position="before">
        <page string="Invoice Frequencies" name="frequencies">
            <label name="default_frequency"/>
            <field name="default_frequency" colspan="3"/>
            <field name="frequencies" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/group[@id='left']/field[@name='currency']" position="after">
        <label name="account_for_billing"/>
        <field name="account_for_billing"/>
    </xpath>
</data>
