<tryton>
    <data>
        <!-- ###################### -->
        <!-- ##  Payment Journal ## -->
        <!-- ###################### -->
        <record model="ir.ui.view" id="payment_journal_view_form">
            <field name="model">account.payment.journal</field>
            <field name="inherit" ref="account_payment.payment_journal_view_form"/>
            <field name="name">payment_journal_form</field>
        </record>
        <!-- ###################### -->
        <!-- # Payment Suspension # -->
        <!-- ###################### -->
        <record model="ir.ui.view" id="contract_payment_suspension_view_list">
            <field name="model">contract.payment_suspension</field>
            <field name="type">tree</field>
            <field name="name">contract_payment_suspension_list</field>
        </record>
        <record model="ir.ui.view" id="contract_payment_suspension_view_form">
            <field name="model">contract.payment_suspension</field>
            <field name="type">form</field>
            <field name="name">contract_payment_suspension_form</field>
        </record>
        <record model="ir.action.act_window" id="act_contract_payment_suspension_form">
            <field name="name">Payment Suspensions</field>
            <field name="res_model">contract.payment_suspension</field>
        </record>
        <record model="ir.action.act_window.view" id="act_payment_suspension_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="contract_payment_suspension_view_list"/>
            <field name="act_window" ref="act_contract_payment_suspension_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_payment_suspension_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="contract_payment_suspension_view_form"/>
            <field name="act_window" ref="act_contract_payment_suspension_form"/>
        </record>
    </data>
</tryton>
