<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="contract_set_view_form">
            <field name="model">contract.set</field>
            <field name="type">form</field>
            <field name="name">contract_set_form</field>
        </record>
        <record model="ir.ui.view" id="contract_set_view_list">
            <field name="model">contract.set</field>
            <field name="type">tree</field>
            <field name="name">contract_set_list</field>
        </record>
        <record model="ir.action.act_window" id="act_contract_set_form">
            <field name="name">Contract Sets</field>
            <field name="res_model">contract.set</field>
        </record>
        <record model="ir.action.act_window.view" id="act_contract_set_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="contract_set_view_list"/>
            <field name="act_window" ref="act_contract_set_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_contract_set_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="contract_set_view_form"/>
            <field name="act_window" ref="act_contract_set_form"/>
        </record>
        <menuitem parent="contract.menu_contract" sequence="20" id="menu_contract_set" icon="contract" action="act_contract_set_form"/>
        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="name">contract_form</field>
        </record>
        <record model="ir.action.wizard" id="act_decline_set">
            <field name="name">Decline Set</field>
            <field name="wiz_name">contract.set.decline</field>
        </record>
        <record model="ir.ui.view" id="select_set_decline_reason_view_form">
            <field name="model">contract.set.decline.select_reason</field>
            <field name="type">form</field>
            <field name="name">select_decline_reason_form</field>
        </record>
        <!-- ################# -->
        <!-- # Configuration # -->
        <!-- ################# -->
        <record model="ir.sequence.type" id="sequence_type_number">
            <field name="name">Contract Set Number</field>
            <field name="code">contract_set_number</field>
        </record>
        <record model="ir.sequence" id="sequence_number">
            <field name="name">Contract Set Number Sequence</field>
            <field name="code">contract_set_number</field>
        </record>
        <record model="ir.sequence.type-res.group" id="sequence_type_number_group_admin">
            <field name="sequence_type" ref="sequence_type_number"/>
            <field name="group" ref="res.group_admin"/>
        </record>
        <record model="ir.sequence.type-res.group" id="sequence_type_number_group_offered_admin">
            <field name="sequence_type" ref="sequence_type_number"/>
            <field name="group" ref="offered.group_offered_admin"/>
        </record>
        <record model="ir.ui.view" id="configuration_view_form">
            <field name="model">offered.configuration</field>
            <field name="type">form</field>
            <field name="inherit" ref="offered.configuration_view_form"/>
            <field name="name">configuration_form</field>
        </record>
    </data>
</tryton>
