<tryton>
    <data skiptest="1">
        <record model="rule_engine.function" id="te_relation_number_in_set">
            <field name="description">Numéro de la relation dans groupe de contrat</field>
            <field name="name">_re_relation_number_order_by_age_in_set</field>
            <field name="namespace">rule_engine.runtime</field>
            <field name="type">function</field>
            <field name="parent" ref="offered_insurance.dossier_donnees_couverture"/>
            <field name="translated_technical_name">numero_relation_dans_groupe</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="long_description">Retourne le rang de l'assuré selon 
l'âge (en commençant par le plus agé) dans un ensemble de contrat. La fonction 
prend le code de la relation entre l'assuré et le souscripteur et un optionnel 
âge maximum (ex d'utilisation: rang de naissance d'un enfant)</field>
            <field name="fct_args">code_relation, [age minimum]</field>
        </record>
        <record model="rule_engine.function" id="te_number_of_covered_with_relation_in_set">
            <field name="description">Nombre de personnes couvertes avec la relation dans groupe de contrat</field>
            <field name="name">_re_number_of_covered_with_relation_in_set</field>
            <field name="namespace">rule_engine.runtime</field>
            <field name="type">function</field>
            <field name="parent" ref="offered_insurance.dossier_donnees_couverture"/>
            <field name="translated_technical_name">nombre_relation_dans_groupe</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="long_description">Retourne le nombre d'assuré actif sur 
l'ensemble du contrat ayant la relation dans leurs relations</field>
            <field name="fct_args">code_relation</field>
        </record>
    </data>
</tryton>
