<?xml version="1.0"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Commission rule"/>
    </xpath>
    <xpath expr="/form/label[@name='rule']" position="before">
        <label name="rate"/>
        <group id="rate" states="{'invisible': Eval('config_kind') != 'simple'}">
            <field name="rate" xexpand="0" factor="100"/>
            <label string="%" id="rate" xexpand="0" xalign="0"/>
        </group>
        <newline/>
    </xpath>
</data>
