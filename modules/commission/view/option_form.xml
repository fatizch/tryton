<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/notebook/page[@id='activation_history']" position="before">
        <page string="Option-Commission Option Relations" id="options"
            states="{'invisible': Eval('coverage_kind') != 'commission'}">
            <field name="options" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='activation_history']" position="before">
        <page string="Commissions" id="com_options"
            states="{'invisible': Eval('coverage_kind') != 'insurance'}">
            <field name="com_options" colspan="4"/>
        </page>
    </xpath>
</data>
