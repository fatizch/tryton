<?xml version="1.0" encoding="UTF-8"?>
<tryton>
    <data>
        <!-- ############################################################ -->
        <!-- ########### Contract ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="agreement_view_tree">
            <field name="model">contract</field>
            <field name="type">tree</field>
            <field name="inherit" ref="contract.contract_view_tree"/>
            <!-- <field name="name">contract_tree</field> -->
            <field name="arch" type="xml"/>
        </record>
        <!-- form view -->
        <record model="ir.ui.view" id="agreement_view_form">
            <field name="model">contract</field>
            <field name="type">form</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="arch" type="xml"/>
            <!-- <field name="name">contract_form</field> -->
        </record>
        <record model="ir.action.act_window" id="act_agreement_form">
            <field name="name">Commission Agreements</field>
            <field name="res_model">contract</field>
            <field name="domain">[('product_kind', '=', 'commission')]</field>
        </record>
        <record model="ir.action.act_window.view" id="act_agreement_tree_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="agreement_view_tree"/>
            <field name="act_window" ref="act_agreement_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_agreement_form_view">
            <field name="sequence" eval="20"/>
            <field name="view" ref="agreement_view_form"/>
            <field name="act_window" ref="act_agreement_form"/>
        </record>

        <menuitem parent="menu_commission" sequence="20"
            id="menu_agreement_form" icon="tryton-list" action="act_agreement_form"/>

        <!-- ############################################################ -->
        <!-- ########### Option ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="option_view_tree">
            <field name="model">contract.option</field>
            <field name="type">tree</field>
            <field name="inherit" ref="contract.option_view_list"/>
            <field name="arch" type="xml"/>
        </record>
        <record model="ir.ui.view" id="option_view_form">
            <field name="model">contract.option</field>
            <field name="type">form</field>
            <field name="inherit" ref="contract.option_view_form"/>
            <field name="name">option-commission_option_form</field>
        </record>
        <record model="ir.ui.view" id="option_com_tree">
          <field name="model">contract.option</field>
          <field name="type">tree</field>
          <field name="field_childs">com_options</field>
          <field name="name">option_com_tree</field>
        </record>

        <!-- ############################################################ -->
        <!-- ########### OptionCommissionOptionRelation ########### -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="option_commission_option_view_tree">
            <field name="model">contract.option-commission.option</field>
            <field name="type">tree</field>
            <field name="name">option_commission_option_tree</field>
        </record>
        <record model="ir.ui.view" id="option_commision_option_view_form">
            <field name="model">contract.option-commission.option</field>
            <field name="type">form</field>
            <field name="name">option_commission_option_form</field>
        </record>

        <!-- ########### -->
        <!-- # Premium # -->
        <!-- ########### -->
        <record model="ir.ui.view" id="premium_view_form">
            <field name="model">contract.premium</field>
            <field name="type">form</field>
            <field name="inherit" ref="contract_insurance_invoice.premium_view_form"/>
            <field name="name">premium_form</field>
        </record>

        <!-- ###################### -->
        <!-- # Premium Commission # -->
        <!-- ###################### -->
        <record model="ir.ui.view" id="premium_commission_view_list">
            <field name="model">contract.premium.commission</field>
            <field name="type">tree</field>
            <field name="name">premium_commission_list</field>
        </record>

        <!-- ########### -->
        <!-- # Invoice # -->
        <!-- ########### -->
        <record model="ir.ui.view" id="invoice_view_form">
            <field name="model">account.invoice</field>
            <field name="type">form</field>
            <field name="inherit" ref="account_invoice.invoice_view_form"/>
            <field name="name">invoice_form</field>
        </record>
        <record model="ir.ui.view" id="com_invoice_view_list">
            <field name="model">account.invoice</field>
            <field name="type">tree</field>
            <field name="name">com_invoice_list</field>
        </record>
    </data>
</tryton>
