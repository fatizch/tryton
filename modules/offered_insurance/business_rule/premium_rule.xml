<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <!--################################################################-->
        <!-- Premium Dates -->
        <!--################################################################-->
        <record model="ir.ui.view" id="premium_date_configuration_view_form">
            <field name="model">billing.premium.date_configuration</field>
            <field name="type">form</field>
            <field name="name">premium_date_configuration_form</field>
        </record>
        <!--################################################################-->
        <!-- Pricing Data -->
        <!--################################################################-->
        <record model="ir.ui.view" id="pricing_component_view_tree">
            <field name="model">billing.premium.rule.component</field>
            <field name="type">tree</field>
            <field name="name">premium_rule_component_list</field>
        </record>
        <record model="ir.ui.view" id="pricing_component_view_form">
            <field name="model">billing.premium.rule.component</field>
            <field name="type">form</field>
            <field name="name">premium_rule_component_form</field>
        </record>
        <!--################################################################-->
        <!-- Premium Rule -->
        <!--################################################################-->
        <record model="ir.ui.view" id="pricingrule_view_tree">
            <field name="model">billing.premium.rule</field>
            <field name="type">tree</field>
            <field name="inherit" ref="businessruleroot_view_tree"/>
            <field name="arch" type="xml"/>
        </record>
        <record model="ir.ui.view" id="pricingrule_view_form">
            <field name="model">billing.premium.rule</field>
            <field name="type">form</field>
            <field name="inherit" ref="businessruleroot_view_form"/>
            <field name="name">premium_rule_form</field>
        </record>
        <!--################################################################-->
        <!-- Tax Version -->
        <!--################################################################-->
        <record model="ir.ui.view" id="tax_description_version_form">
            <field name="model">account.tax.description.version</field>
            <field name="type">form</field>
            <field name="inherit" ref="account_cog.tax_description_version_view_form"/>
            <field name="name">tax_description_version_form</field>
        </record>
        <!--################################################################-->
        <!-- Fee Version -->
        <!--################################################################-->
        <record model="ir.ui.view" id="fee_description_version_form">
            <field name="model">account.fee.description.version</field>
            <field name="type">form</field>
            <field name="inherit" ref="account_cog.fee_description_version_view_form"/>
            <field name="name">fee_description_version_form</field>
        </record>
    </data>
</tryton>
