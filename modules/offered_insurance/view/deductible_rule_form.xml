<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Deductible rule"/>
    </xpath>
    <xpath expr="/form/label[@name='config_kind']" position="before">
        <label name="kind"/>
        <field name="kind" xexpand="0"/>
        <newline/>
    </xpath>
    <xpath expr="/form/field[@name='config_kind']" position="after">
        <newline/>
        <label name="simple_config_choice"/>
        <field name="simple_config_choice" xexpand="0"/>
        <label name="scope"/>
        <field name="scope"/>
        <newline/>
    </xpath>
    <xpath expr="/form/label[@name='rule']" position="before">
        <label name="amount"/>
        <field name="amount"/>
        <newline/>
        <label name="duration"/>
        <group id="duration" xexpand="0">
            <field name="duration" xexpand="0"/>
            <field name="duration_unit" xexpand="0"/>
        </group>
        <newline/>
        <field name="durations" colspan="4"/>
        <newline/>
    </xpath>
</data>
