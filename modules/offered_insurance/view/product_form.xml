<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/notebook/page[@id='option']" position="before">
        <page string="Items" id="option">
            <field name="item_descriptors"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='managers']/notebook" position="inside">
        <page string="Deductible" id="deductible">
            <field name="deductible_rules" colspan="4"/>
        </page>
        <page string="Term - Renewal" id="term_renewal">
            <field name="term_renewal_rules" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='managers']/notebook/page[@id='pricing']" position="inside">
        <field name="premium_dates" colspan="4" mode="form" expand_toolbar="0"/>
    </xpath>
</data>
