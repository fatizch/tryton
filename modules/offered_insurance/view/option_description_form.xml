<data>
    <xpath expr="//group[@id='data']" position="inside">
        <label name="item_desc"/>
        <field name="item_desc"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/field[@name='tags']" position="before">
        <label name="with_extra_premiums"/>
        <field name="with_extra_premiums"/>
        <label name="with_exclusions"/>
        <field name="with_exclusions"/>
    </xpath>
    <xpath expr="/form/field[@name='end_date']" position="after">
        <label name="insurer"/>
        <field name="insurer"/>
        <label name="family"/>
        <field name="family"/>
        <label name="insurance_kind"/>
        <field name="insurance_kind"/>
    </xpath>
</data>
