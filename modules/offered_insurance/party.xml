<tryton>
    <data>
        <record model="ir.ui.view" id="party_view_form">
            <field name="model">party.party</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_form</field>
        </record>
        <record model="ir.ui.view" id="insurer_view_form">
            <field name="model">insurer</field>
            <field name="type">form</field>
            <field name="name">insurer_form</field>
        </record>
        <record model="ir.ui.view" id="insurer_view_tree">
            <field name="model">insurer</field>
            <field name="type">tree</field>
            <field name="name">insurer_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_insurer_form">
            <field name="name">Insurers</field>
            <field name="res_model">insurer</field>
        </record>
        <record model="ir.action.act_window.view" id="act_insurer_form_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="insurer_view_tree"/>
            <field name="act_window" ref="act_insurer_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_insurer_form_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="insurer_view_form"/>
            <field name="act_window" ref="act_insurer_form"/>
        </record>
        <menuitem parent="party.menu_configuration" id="menu_insurer_form" icon="insurer" action="act_insurer_form"/>
    </data>
</tryton>
