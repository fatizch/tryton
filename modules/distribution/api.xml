<tryton>
    <data depends="api">
        <!-- ############## -->
        <!-- # Identities # -->
        <!-- ############## -->
        <record model="ir.ui.view" id="api_identity_view_form">
            <field name="model">ir.api.identity</field>
            <field name="inherit" ref="coog_core.api_identity_view_form"/>
            <field name="name">api_identity_form</field>
        </record>
        <record model="ir.ui.view" id="api_identity_view_list">
            <field name="model">ir.api.identity</field>
            <field name="inherit" ref="coog_core.api_identity_view_list"/>
            <field name="name">api_identity_list</field>
        </record>
    </data>
    <data depends="web_configuration">
        <!-- ######################## -->
        <!-- # web configuration Relate # -->
        <!-- ######################## -->
        <record model="ir.action.keyword" id="act_open_distribution_network_web_ui_resources">
            <field name="keyword">form_relate</field>
            <field name="model">distribution.network,-1</field>
            <field name="action" ref="web_configuration.act_web_ui_resource_for_keyword_window"/>
        </record>
    </data>
</tryton>
