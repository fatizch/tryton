<form col="2">
    <label name="parent_rec_name"/>
    <field name="parent_rec_name"/>
    <label name="item_desc"/>
    <field name="item_desc"/>
    <label name="name"/>
    <field name="name"/>
    <label name="party"/>
    <field name="party"/>
