<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <!--  *******************************************
              ************    Fee Description Version    ************
              *******************************************-->
        <record model="ir.ui.view" id="fee_description_version_view_tree">
            <field name="model">account.fee.description.version</field>
            <field name="type">tree</field>
            <field name="name">fee_description_version_tree</field>
        </record>
        <record model="ir.ui.view" id="fee_description_version_view_form">
            <field name="model">account.fee.description.version</field>
            <field name="type">form</field>
            <field name="name">fee_description_version_form</field>
        </record>
        <!--  *******************************************
              **************    Fee Desc   **************
              *******************************************-->
        <record model="ir.ui.view" id="fee_description_view_tree">
            <field name="model">account.fee.description</field>
            <field name="type">tree</field>
            <field name="name">fee_description_tree</field>
        </record>
        <record model="ir.ui.view" id="fee_description_view_form">
            <field name="model">account.fee.description</field>
            <field name="type">form</field>
            <field name="name">fee_description_form</field>
        </record>
        <record model="ir.action.act_window" id="act_fee_form">
            <field name="name">Fees</field>
            <field name="res_model">account.fee.description</field>
        </record>
        <record model="ir.action.act_window.view" id="act_fee_form_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="fee_description_view_tree"/>
            <field name="act_window" ref="act_fee_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_fee_form_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="fee_description_view_form"/>
            <field name="act_window" ref="act_fee_form"/>
        </record>
        <!--  *******************************************
              ****************    Menu    ***************
              *******************************************-->
        <menuitem name="Fees" parent="menu_account" id="menu_product_fees" sequence="50" icon="tryton-preferences" action="act_fee_form"/>
    </data>
</tryton>
