<tryton>
    <data>
        <record model="ir.ui.view" id="account_type_template_view_form">
            <field name="model">account.account.type.template</field>
            <field name="inherit" ref="account.account_type_template_view_form"/>
            <field name="name">account_type_template_form</field>
        </record>
        <record model="ir.ui.view" id="account_type_view_form">
            <field name="model">account.account.type</field>
            <field name="inherit" ref="account.account_type_view_form"/>
            <field name="name">account_type_form</field>
        </record>
        <record model="ir.ui.view" id="aged_balance_view_list">
            <field name="model">account.aged_balance</field>
            <field name="inherit" ref="account.aged_balance_view_list"/>
            <field name="name">aged_balance_list</field>
        </record>
        <record model="ir.action.report" id="report_aged_balance_ods">
            <field name="name">Aged Balance Spreadsheet</field>
            <field name="model">account.aged_balance</field>
            <field name="report_name">account.aged_balance</field>
            <field name="report">account_cog/aged_balance.ods</field>
            <field name="template_extension">ods</field>
            <field name="extension">xlsx</field>
        </record>
        <record model="ir.action.keyword" id="report_aged_balance_ods_keyword">
            <field name="keyword">form_print</field>
            <field name="model">account.aged_balance,-1</field>
            <field name="action" ref="report_aged_balance_ods"/>
        </record>
        <record model="ir.ui.view" id="account_view_form">
            <field name="model">account.account</field>
            <field name="inherit" ref="account.account_view_form"/>
            <field name="name">account_form</field>
        </record>
        <record model="ir.ui.view" id="account_view_list">
            <field name="model">account.account</field>
            <field name="inherit" ref="account.account_view_list"/>
            <field name="name">account_list</field>
        </record>
        <record model="ir.ui.view" id="account_view_tree">
            <field name="model">account.account</field>
            <field name="inherit" ref="account.account_view_tree"/>
            <field name="name">account_list</field>
        </record>
    </data>
    <data noupdate="1">
        <!-- ########### -->
        <!-- # Journal # -->
        <!-- ########### -->
        <record model="account.journal" id="journal_split">
            <field name="name">Move Line Reconciliation Split Journal</field>
            <field name="code">SPLIT</field>
            <field name="type">split</field>
            <field name="sequence" ref="account.sequence_account_journal"/>
        </record>
    </data>
</tryton>
