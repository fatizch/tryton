<?xml version="1.0"?>
<form string="Payment Information">
    <label name="date"/>
    <field name="date"/>
    <label name="party"/>
    <field name="party"/>
    <label name="journal"/>
    <field name="journal"/>
    <label name="description"/>
    <field name="description"/>
    <label name="credit"/>
    <group id="credit_amount">
        <field name="credit" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <notebook colspan="4">
        <page string="Reconciliation Information" id="reconciliation">
            <field name="reconciliation_lines"/>
        </page>
        <page string="Other Info" id="info">
            <label name="maturity_date"/>
            <field name="maturity_date"/>
            <label name="reconciliation"/>
            <field name="reconciliation"/>
            <label name="amount_second_currency"/>
            <field name="amount_second_currency"/>
            <label name="second_currency"/>
            <field name="second_currency"/>
            <label name="period"/>
            <field name="period"/>
            <label name="move"/>
            <field name="move"/>
            <label name="account"/>
            <field name="account" colspan="3"/>
            <label name="origin"/>
            <field name="origin" colspan="3"/>
        </page>
        <page name="tax_lines">
            <field name="tax_lines" colspan="4"/>
        </page>
    </notebook>
    <label name="move_state"/>
    <field name="move_state"/>
    <label name="state"/>
    <field name="state"/>
</form>
