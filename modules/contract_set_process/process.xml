<tryton>
    <data>
        <record model="ir.action.wizard" id="validation_process_launcher">
            <field name="name">Start Validation</field>
            <field name="wiz_name">contract.set.validate</field>
        </record>
        <menuitem parent="contract.menu_contract" sequence="1" action="validation_process_launcher" id="menu_start_validation"/>
        <record model="ir.action.act_window" id="act_contract_set_logs">
            <field name="name">Logs</field>
            <field name="res_model">process.log</field>
            <field name="domain" eval="[('task', '=', (Eval('active_model'),Eval('active_id')))]" pyson="1"/>
        </record>
        <record model="ir.action.keyword" id="act_contract_set_logs_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">contract.set,-1</field>
            <field name="action" ref="act_contract_set_logs"/>
        </record>
        <!-- ########################## -->
        <!-- # Resume Process Keyword # -->
        <!-- ########################## -->
        <record model="ir.action.keyword" id="act_resume_contract_set_process_keyword">
            <field name="keyword">form_action</field>
            <field name="model">contract.set,-1</field>
            <field name="action" ref="process_cog.act_resume_process"/>
        </record>
        <!-- Contracts view in process -->
        <record model="ir.ui.view" id="contract_in_contract_set_process_view_tree">
            <field name="model">contract</field>
            <field name="type">tree</field>
            <field name="name">contract_in_contract_set_process_tree</field>
        </record>
    </data>
</tryton>
