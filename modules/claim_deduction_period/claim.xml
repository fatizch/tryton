<tryton>
    <data>
        <!-- ######## -->
        <!-- # Loss # -->
        <!-- ######## -->
        <record model="ir.ui.view" id="loss_view_form">
            <field name="model">claim.loss</field>
            <field name="inherit" ref="claim.loss_view_form"/>
            <field name="name">loss_form</field>
        </record>
        <record model="ir.ui.view" id="loss_without_service_view_form">
            <field name="model">claim.loss</field>
            <field name="inherit" ref="claim.loss_without_service_view_form"/>
            <field name="name">loss_without_service_form</field>
        </record>
        <!-- #################### -->
        <!-- # Deduction Period # -->
        <!-- #################### -->
        <record model="ir.ui.view" id="deduction_period_view_list">
            <field name="model">claim.loss.deduction.period</field>
            <field name="type">tree</field>
            <field name="name">deduction_period_list</field>
        </record>
        <record model="ir.ui.view" id="deduction_period_display_view_list">
            <field name="model">claim.loss.deduction.period.display</field>
            <field name="type">tree</field>
            <field name="name">deduction_period_display_list</field>
        </record>
    </data>
</tryton>
