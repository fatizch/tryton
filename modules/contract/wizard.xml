<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.action.wizard" id="option_subscription_wizard">
            <field name="name">Option Subscription</field>
            <field name="wiz_name">contract.wizard.option_subscription</field>
        </record>
        <record model="ir.ui.view" id="options_displayer_view_form">
            <field name="model">contract.wizard.option_subscription.options_displayer</field>
            <field name="type">form</field>
            <field name="name">options_displayer_form</field>
        </record>
        <record model="ir.ui.view" id="wizard_option_view_tree">
            <field name="model">contract.wizard.option_subscription.options_displayer.option</field>
            <field name="type">tree</field>
            <field name="name">wizard_option_tree</field>
            <field name="field_childs" eval="None"/>
        </record>
        <record model="ir.ui.view" id="select_package_view_form">
            <field name="model">contract.wizard.option_subscription.select_package</field>
            <field name="type">form</field>
            <field name="name">select_package_form</field>
        </record>
        <record model="ir.action.wizard" id="act_activate">
            <field name="name">Activate</field>
            <field name="wiz_name">contract.activate</field>
        </record>
        <record model="ir.ui.view" id="activate_confirm_view_form">
            <field name="model">contract.activate.confirm</field>
            <field name="type">form</field>
            <field name="name">activate_confirm_form</field>
        </record>
        <record model="ir.action.wizard" id="act_decline">
            <field name="name">Decline</field>
            <field name="wiz_name">contract.decline</field>
        </record>
        <record model="ir.ui.view" id="select_decline_reason_view_form">
            <field name="model">contract.decline.select_reason</field>
            <field name="type">form</field>
            <field name="name">select_decline_reason_form</field>
        </record>
        <!-- ################################## -->
        <!-- # Contract Stop Select Contracts # -->
        <!-- ################################## -->
        <record model="ir.ui.view" id="stop_select_contracts_view_form">
            <field name="model">contract.stop.select_contracts</field>
            <field name="type">form</field>
            <field name="name">stop_select_contracts_form</field>
        </record>
        <!-- ################# -->
        <!-- # Contract Stop # -->
        <!-- ################# -->
        <record model="ir.action.wizard" id="act_stop">
            <field name="name">Stop Contract</field>
            <field name="wiz_name">contract.stop</field>
        </record>
        <!-- ####################################### -->
        <!-- # Contract Reactivate Check Contracts # -->
        <!-- ####################################### -->
        <record model="ir.ui.view" id="reactivate_check_contracts_view_form">
            <field name="model">contract.reactivate.check_contracts</field>
            <field name="type">form</field>
            <field name="name">reactivate_check_contracts_form</field>
        </record>
        <!-- ####################### -->
        <!-- # Contract Reactivate # -->
        <!-- ####################### -->
        <record model="ir.action.wizard" id="act_reactivate">
            <field name="name">Reactivate Contract</field>
            <field name="wiz_name">contract.reactivate</field>
        </record>
        <!-- ######################## -->
        <!-- # Related Attachments # -->
        <!-- ######################## -->
        <record model="ir.action.wizard" id="act_related_attachments">
            <field name="name">Related Attachments</field>
            <field name="wiz_name">contract.related_attachments</field>
        </record>
        <record model="ir.action.keyword" id="act_related_attachments_relate_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_related_attachments"/>
        </record>
    </data>
</tryton>
