<?xml version="1.0"?>
<form string="Select contracts to stop">
    <label name="status"/>
    <field name="status"/>
    <label name="sub_status"/>
    <field name="sub_status" widget="selection"/>
    <label name="at_date"/>
    <field name="at_date"/>
    <newline/>
    <field name="contracts" colspan="4"/>
    <newline/>
    <group id="warning_void" string="" colspan="4">
        <image name="tryton-dialog-warning" xexpand="0" xfill="0"/>
        <label string="The selected contracts will be voided, it will be as though they never happened" id="void_contract_warning" yalign="0.0" xalign="0.0" xexpand="1" colspan="3"/>
    </group>
</form>
