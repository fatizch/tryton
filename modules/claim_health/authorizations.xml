        </record>
        <record model="ir.ui.menu-res.group" id="menu_medical_act_product_manage">
            <field name="menu" ref="claim_health.menu_medical_act"/>
            <field name="group" ref="offered.group_product_manage"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_medical_act_family_product_manage">
            <field name="menu" ref="claim_health.menu_medical_act_family"/>
            <field name="group" ref="offered.group_product_manage"/>
        </record>
        <!-- Contract Management Group -->
        <!-- Health -->
        <record model="ir.model.access" id="access_claim_loss_health_group_claim_manage">
            <field name="model" search="[('model', '=', 'claim.loss.health')]"/>
            <field name="group" ref="claim.group_claim_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_benefit_act_description_group_product_manage">
            <field name="model" search="[('model', '=', 'benefit.act.description')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_benefit_act_family_group_product_manage">
            <field name="model" search="[('model', '=', 'benefit.act.family')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <!-- Default Claim Health Access Rights -->
        <record model="ir.model.access" id="access_benefit_act_description">
            <field name="model" search="[('model', '=', 'benefit.act.description')]"/>
            <field name="perm_read" eval="True"/>
