<tryton>
    <data>
        <!-- Access Rights for Claim Consultation Group -->
        <!-- Health -->
        <record model="ir.ui.menu-res.group" id="menu_health_claim_read">
            <field name="menu" ref="health.menu_health"/>
            <field name="group" ref="claim.group_claim_read"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_health_party_claim_read">
