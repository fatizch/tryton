        <record model="ir.ui.menu-res.group" id="menu_person_form_claim_read">
            <field name="menu" ref="health.menu_person_form"/>
            <field name="group" ref="claim.group_claim_read"/>
        </record>
        <record model="ir.ui.menu-res.group" id="health_menu_contract_form_claim_read">
            <field name="menu" ref="health.menu_contract_form"/>
            <field name="group" ref="claim.group_claim_read"/>
        </record>
        <!-- Access Rights for Claim Management Group -->
        <!-- Health -->
        <record model="ir.ui.menu-res.group" id="menu_health_claim_manage">
            <field name="menu" ref="health.menu_health"/>
            <field name="group" ref="claim.group_claim_manage"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_health_party_claim_manage">
            <field name="menu" ref="health.menu_health_party"/>
