<form>
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="start_date"/>
    <field name="start_date"/>
    <label name="end_date"/>
    <field name="end_date"/>
    <notebook>
        <page string="Summary" id="summary">
            <field name="description" colspan="4"/>
        </page>
        <page string="Extra Datas" id="extra_datas">
            <field name="extra_data" colspan="4"/>
            <newline/>
            <field name="extra_data_def" colspan="4"/>
        </page>
        <page string="Rules" id="managers">
            <label name="automatically_deliver"/>
            <field name="automatically_deliver"/>
            <field name="show_benefit" invisible="1"/>
            <notebook>
            </notebook>
        </page>
        <page string="Administration" id="admin" col="2">
            <group id="left" yexpand="1">
                <label name="beneficiary_kind"/>
                <field name="beneficiary_kind"/>
                <label name="several_delivered"/>
                <field name="several_delivered"/>
                <label name="may_have_origin"/>
                <field name="may_have_origin"/>
                <newline/>
                <field name="loss_descs" colspan="4"/>
            </group>
            <group id="right" col="2">
                <label name="company"/>
                <field name="company"/>
            </group>
            <group id="insurer" colspan="2">
                <label name="insurer"/>
                <field name="insurer"/>
            </group>
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
</form>
