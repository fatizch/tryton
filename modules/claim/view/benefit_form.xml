<form string="Benefit">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="start_date"/>
    <field name="start_date"/>
    <label name="end_date"/>
    <field name="end_date"/>
    <notebook>
        <page string="Summary" id="summary">
            <field name="description" colspan="4"/>
        </page>
        <page string="Extra Datas" id="extra_datas">
            <newline/>
            <field name="extra_data_def" colspan="4"/>
        </page>
        <page string="Rules" id="managers">
            <notebook>
            </notebook>
        </page>
        <page string="Administration" id="admin" col="2">
            <group id="left">
                <label name="beneficiary_kind"/>
                <field name="beneficiary_kind"/>
                <newline/>
                <field name="loss_descs" colspan="4"/>
            </group>
            <group id="right" col="2">
                <label name="company"/>
                <field name="company"/>
                <newline/>
                <field name="extra_data" colspan="2"/>
            </group>
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
</form>
