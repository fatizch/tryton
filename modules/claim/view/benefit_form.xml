<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Benefit"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='summary']" position="after">
        <page string="Extra Datas" id="extra_datas">
            <newline/>
            <field name="extra_data_def" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/group[@id='left']" position="inside">
        <label name="beneficiary_kind"/>
        <field name="beneficiary_kind"/>
        <newline/>
        <field name="loss_descs" colspan="4"/>
    </xpath>
</data>
