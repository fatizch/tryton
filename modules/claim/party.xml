<tryton>
    <data>
        <!-- ##################### -->
        <!-- # Claimant relation # -->
        <!-- ##################### -->
        <record model="ir.action.act_window" id="act_claims_button">
            <field name="name">Claims</field>
            <field name="res_model">claim</field>
            <field name="domain" eval="[('claimant', '=', Eval('active_id'))]" pyson="1"/>
        </record>
        <!-- ###################### -->
        <!-- # Insurer Delegation # -->
        <!-- ###################### -->
        <record model="ir.ui.view" id="insurer_delegation_view_list">
            <field name="model">insurer.delegation</field>
            <field name="inherit" ref="offered_insurance.insurer_delegation_view_list"/>
            <field name="name">insurer_delegation_list</field>
        </record>
        <!-- ########## -->
        <!-- # Party  # -->
        <!-- ########## -->
        <record model="ir.ui.view" id="party_view_form">
            <field name="model">party.party</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_form</field>
        </record>
    </data>
</tryton>
