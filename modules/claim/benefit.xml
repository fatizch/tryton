<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <!-- ############################################################ -->
        <!-- Benefit -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="benefit_view_form">
            <field name="model">benefit</field>
            <field name="type">form</field>
            <field name="inherit" eval="None"/>
            <field name="name">benefit_form</field>
        </record>
        <record model="ir.ui.view" id="benefit_view_tree">
            <field name="model">benefit</field>
            <field name="type">tree</field>
            <field name="inherit" eval="None"/>
            <field name="name">benefit_list</field>
        </record>
        <!-- Read / Write rule -->
        <record model="ir.rule.group" id="rule_group_benefit">
            <field name="model" search="[('model', '=', 'benefit')]"/>
            <field name="global_p" eval="True"/>
        </record>
        <record model="ir.rule" id="rule_benefit_1">
            <field name="domain" eval="[('company', '=', Eval('user', {}).get('company', None))]" pyson="1"/>
            <field name="rule_group" ref="rule_group_benefit"/>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_benefit_form">
            <field name="name">Benefits</field>
            <field name="res_model">benefit</field>
            <field name="domain" eval="[('company', '=', Eval('context', {}).get('company', False))]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_benefit_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="benefit_view_tree"/>
            <field name="act_window" ref="act_benefit_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_benefit_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="benefit_view_form"/>
            <field name="act_window" ref="act_benefit_form"/>
        </record>
        <menuitem parent="offered.menu_product" sequence="1" id="menu_benefit_form" icon="tryton-list" action="act_benefit_form"/>
        <!-- ############################################################ -->
        <!-- Event Description -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="event_desc_view_form">
            <field name="model">benefit.event.description</field>
            <field name="type">form</field>
            <field name="name">event_description_form</field>
        </record>
        <record model="ir.ui.view" id="event_desc_view_tree">
            <field name="model">benefit.event.description</field>
            <field name="type">tree</field>
            <field name="name">event_description_list</field>
        </record>
        <!-- Read / Write rule -->
        <record model="ir.rule.group" id="rule_group_event_desc">
            <field name="model" search="[('model', '=', 'benefit.event.description')]"/>
            <field name="global_p" eval="True"/>
        </record>
        <record model="ir.rule" id="rule_event_desc_1">
            <field name="domain" eval="[('company', '=', Eval('user', {}).get('company', None))]" pyson="1"/>
            <field name="rule_group" ref="rule_group_event_desc"/>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_event_desc_form">
            <field name="name">Events</field>
            <field name="res_model">benefit.event.description</field>
            <field name="domain" eval="[('company', '=', Eval('context', {}).get('company', False))]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_event_desc_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="event_desc_view_tree"/>
            <field name="act_window" ref="act_event_desc_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_event_desc_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="event_desc_view_form"/>
            <field name="act_window" ref="act_event_desc_form"/>
        </record>
        <menuitem parent="offered.menu_product_configuration" sequence="11" id="menu_event_desc_form" icon="tryton-list" action="act_event_desc_form"/>
        <!-- ############################################################ -->
        <!-- Loss Description -->
        <!-- ############################################################ -->
        <record model="ir.ui.view" id="loss_desc_view_form">
            <field name="model">benefit.loss.description</field>
            <field name="type">form</field>
            <field name="name">loss_description_form</field>
        </record>
        <record model="ir.ui.view" id="loss_desc_view_tree">
            <field name="model">benefit.loss.description</field>
            <field name="type">tree</field>
            <field name="name">loss_description_list</field>
        </record>
        <!-- Read / Write rule -->
        <record model="ir.rule.group" id="rule_group_loss_desc">
            <field name="model" search="[('model', '=', 'benefit.loss.description')]"/>
            <field name="global_p" eval="True"/>
        </record>
        <record model="ir.rule" id="rule_loss_desc_1">
            <field name="domain" eval="[('company', '=', Eval('user', {}).get('company', None))]" pyson="1"/>
            <field name="rule_group" ref="rule_group_loss_desc"/>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_loss_desc_form">
            <field name="name">Losses</field>
            <field name="res_model">benefit.loss.description</field>
            <field name="domain" eval="[('company', '=', Eval('context', {}).get('company', False))]" pyson="1"/>
        </record>
        <record model="ir.action.act_window.view" id="act_loss_desc_view_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="loss_desc_view_tree"/>
            <field name="act_window" ref="act_loss_desc_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_loss_desc_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="loss_desc_view_form"/>
            <field name="act_window" ref="act_loss_desc_form"/>
        </record>
        <menuitem parent="offered.menu_product_configuration" sequence="11" id="menu_loss_desc_form" icon="tryton-list" action="act_loss_desc_form"/>
    </data>
</tryton>
