<form>
    <label name="contract"/>
    <field name="contract" colspan="3"/>
    <label name="new_contract_amount"/>
    <group id="new_contract_amount_value">
        <field name="new_contract_amount" xalign="1" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <label name="old_contract_amount"/>
    <group id="old_contract_amount_value">
        <field name="old_contract_amount" xalign="1" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <notebook colspan="4">
        <page name="new_contract_payments">
            <field name="new_contract_payments" mode="tree"/>
        </page>
        <page name="old_contract_payments">
            <field name="old_contract_payments" mode="tree"/>
        </page>
    </notebook>
</form>
