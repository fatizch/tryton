<form col="2">
    <group id="right" string="" col="2">
        <label name="name"/>
        <field name="name"/>
        <label name="code"/>
        <field name="code"/>
        <label name="sequence"/>
        <field name="sequence"/>
    </group>
    <notebook colspan="4">
        <page name="parts">
            <field name="parts" colspan="4" mode="tree,form"/>
        </page>
        <page string="Products" id="products">
            <field name="products" colspan="4"/>
        </page>
        <page string="Administration" id="admin">
            <label name="icon"/>
            <field name="icon"/>
            <label name="company"/>
            <field name="company"/>
            <separator name="description" colspan="4"/>
            <field name="description" colspan="4"/>
        </page>
        <page name="tags">
            <field name="tags"/>
        </page>
    </notebook>
</form>
