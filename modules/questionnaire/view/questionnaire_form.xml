<form>
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="sequence"/>
    <field name="sequence"/>
    <notebook colspan="4">
        <page string="Questionnaire" id="questionnaire">
            <field name="extra_data_def" colspan="4"/>
            <label name="rule"/>
            <field name="rule"/>
            <newline/>
            <field name="rule_extra_data" no_command="1" colspan="2"/>
        </page>
        <page string="Products" id="products">
            <field name="products" product="document" colspan="4"/>
        </page>
        <page string="Administration" id="admin">
            <label name="icon"/>
            <field name="icon"/>
            <label name="company"/>
            <field name="company"/>
            <separator name="description" colspan="4"/>
            <field name="description" colspan="4"/>
            <field name="tags" colspan="4"/>
        </page>
    </notebook>
</form>
