<tryton>
    <data>
        <!-- ######## -->
        <!-- # Menu # -->
        <!-- ######## -->
        <record model="ir.ui.menu-res.group" id="menu_questionnaire_product_management">
            <field name="menu" ref="questionnaire.menu_questionnaire"/>
            <field name="group" ref="offered.group_product_manage"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_questionnaire_product_read">
            <field name="menu" ref="questionnaire.menu_questionnaire"/>
            <field name="group" ref="offered.group_product_read"/>
        </record>
        <!-- ################# -->
        <!-- # Access rights # -->
        <!-- ################# -->
        <record model="ir.model.access" id="access_questionnaire_group_product_manage">
            <field name="model" search="[('model', '=', 'questionnaire')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_questionnaire_read_only">
            <field name="model" search="[('model', '=', 'questionnaire')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_questionnaire_part_group_product_manage">
            <field name="model" search="[('model', '=', 'questionnaire.part')]"/>
            <field name="group" ref="offered.group_product_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_questionnaire_part_read_only">
            <field name="model" search="[('model', '=', 'questionnaire.part')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
    </data>
    <data depends="api">
        <!-- ############## -->
        <!-- # API Access # -->
        <!-- ############## -->
        <record model="ir.api.access" id="list_questionnaires_default_access">
            <field name="api">api.core.list_questionnaires</field>
            <field name="group" ref="api.group_default_api"/>
        </record>
        <record model="ir.api.access" id="compute_questionnaire_default_access">
            <field name="api">api.core.compute_questionnaire</field>
            <field name="group" ref="api.group_default_api"/>
        </record>
    </data>
    <data depends="contract">
        <!-- ################# -->
        <!-- # Access Rights # -->
        <!-- ################# -->
        <record model="ir.model.access" id="access_contract_questionnaire_group_contract_manage">
            <field name="model" search="[('model', '=', 'contract.questionnaire')]"/>
            <field name="group" ref="contract.group_contract_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_contract_questionnaire_read_only">
            <field name="model" search="[('model', '=', 'contract.questionnaire')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_contract_questionnaire_answer_group_contract_manage">
            <field name="model" search="[('model', '=', 'contract.questionnaire.answer')]"/>
            <field name="group" ref="contract.group_contract_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
