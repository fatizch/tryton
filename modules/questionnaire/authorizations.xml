            <field name="model" search="[('model', '=', 'contract.questionnaire.answer')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_contract_questionnaire_result_group_contract_manage">
            <field name="model" search="[('model', '=', 'contract.questionnaire.result')]"/>
            <field name="group" ref="contract.group_contract_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="True"/>
        </record>
        <record model="ir.model.access" id="access_contract_questionnaire_result_read_only">
            <field name="model" search="[('model', '=', 'contract.questionnaire.result')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
    </data>
</tryton>
