    <group name="other_contracts" colspan="4" yexpand="1" yfill="1">
        <field name="other_contracts" expand_toolbar="0"/>
    </group>
    <field name="product"/>
    <field name="endorsement_part"/>
    <field name="subscriber"/>
</form>
