<form string="Change Billing Information" col="4">
    <group id="header" colspan="4">
        <label name="effective_date"/>
        <field name="effective_date"/>
        <label name="endorsement_definition"/>
        <field name="endorsement_definition"/>
        <label name="contract"/>
        <field name="contract"/>
    </group>
    <group name="previous_billing_information" colspan="4" yexpand="1" yfill="1">
        <field name="previous_billing_information" mode="form" expand_toolbar="0" view_ids="endorsement_insurance_invoice.endorsement_billing_information_view_form"/>
