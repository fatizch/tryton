<tryton>
    <data noupdate="1">
        <record model="rule_engine.function" id="pret">
            <field name="translated_technical_name">pret</field>
            <field name="description">Prêt</field>
            <field name="type">folder</field>
            <field name="language" ref="ir.lang_fr"/>
        </record>
        <record model="rule_engine.context-function" id="cte_default_pret">
            <field name="context" ref="rule_engine.default_context"/>
            <field name="tree_element" ref="pret"/>
        </record>
    </data>
    <data>
        <record model="rule_engine.function" id="objet_pret">
            <field name="translated_technical_name">pret</field>
            <field name="description">Prêt</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan</field>
            <field name="parent" ref="pret"/>
            <field name="long_description">Retourne l'instance du prêt. Pour 
utilisateurs avancés seulement.</field>
        </record>
        <record model="rule_engine.function" id="fin_de_pret">
            <field name="translated_technical_name">fin_du_pret</field>
            <field name="description">Fin du Prêt</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_end_date</field>
            <field name="long_description">Retourne la date de fin du prêt</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="type_de_pret">
            <field name="translated_technical_name">type_de_pret</field>
            <field name="description">Type de Prêt</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_kind</field>
            <field name="long_description">Retourne le type de prêt parmi les 
valeurs suivantes: 'fixed_rate', 'interest_free', 'graduated', 'intermediate',
'balloon'</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="duree_pret_en_mois">
            <field name="translated_technical_name">duree_pret_en_mois</field>
            <field name="description">Durée du prêt en mois</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_duration</field>
            <field name="long_description">Retourne la durée du prêt en mois</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="frequence_mensualite">
            <field name="translated_technical_name">frequence_mensualite</field>
            <field name="description">Fréquence Mensualité</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_payment_frequency</field>
            <field name="parent" ref="pret"/>
            <field name="long_description">Retourne la fréquence du remboursement 
des mensualités du prêt</field>
        </record>
        <record model="rule_engine.function" id="montant_de_la_mensualite">
            <field name="translated_technical_name">montant_de_la_mensualite</field>
            <field name="description">Montant de la Mensualité</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_payment_amount</field>
            <field name="long_description">Retourne le montant de la mensualité 
à la date passée en paramètre. Si aucune date n'est passée en paramètre, la date 
d'exécution de la règle est utilisée</field>
            <field name="fct_args">date</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="montant_du_pret">
            <field name="translated_technical_name">montant_du_pret</field>
            <field name="description">Montant du Prêt</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_amount</field>
            <field name="long_description">Retourne le montant du prêt</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="date_de_deblocage_des_fonds">
            <field name="translated_technical_name">date_de_deblocage_des_fonds</field>
            <field name="description">Date de Déblocage des Fonds</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_funds_release_date</field>
            <field name="long_description">Retourne la date de blocage des 
fonds</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="date_du_premier_paiement">
            <field name="translated_technical_name">date_du_premier_paiement</field>
            <field name="description">Date du Premier Paiement</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_first_payment_date</field>
            <field name="long_description">Retourne la date de premier 
paiement du prêt</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="capital_restant_du">
            <field name="translated_technical_name">capital_restant_du</field>
            <field name="description">Capital restant dû</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_outstanding_loan_balance</field>
            <field name="fct_args">date</field>
            <field name="long_description">Retourne le montant du capital restant 
dû à la date passée en paramètre. Si aucune date n'est passée en paramètre, la date 
d'exécution de la règle est utilisée</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="capital_restant_derniere_date_anniversaire">
            <field name="translated_technical_name">capital_restant_derniere_date_anniversaire</field>
            <field name="description">Capital restant dû à la date du dernier anniversaire</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_outstanding_loan_balance_previous_anniversary_date</field>
            <field name="long_description">Retourne le montant du capital restant 
dû à la dernière date anniversaire.</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="quotite">
            <field name="translated_technical_name">quotite</field>
            <field name="description">Quotité</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_loan_share</field>
            <field name="long_description">Retourne la quotité du prêt</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="taux_periode_depuis_taux_annuel">
            <field name="translated_technical_name">taux_periode_depuis_taux_annuel</field>
            <field name="description">Taux Période Depuis Taux Annuel</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_periodic_rate_from_annual_rate</field>
            <field name="long_description">Retourne le taux du prêt par échéance 
à partir du taux annuel</field>
            <field name="fct_args">taux_annuel</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="montant_remboursement_anticipes">
            <field name="translated_technical_name">montant_remboursement_anticipes</field>
            <field name="description">Montant des remboursements anticipés</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_early_repayments_amount</field>
            <field name="long_description">Retourne la somme des remboursements 
anticipés sur le prêt jsuqu'à la date passée en paramètre. Si aucune date 
n'est passée en paramètre, la date d'exécution de la règle est utilisée</field>
            <field name="fct_args">date</field>
            <field name="parent" ref="pret"/>
        </record>
        <record model="rule_engine.function" id="encours_assure">
            <field name="translated_technical_name">encours_assure</field>
            <field name="description">Encours Assuré</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_insured_outstanding_loan_balance</field>
            <field name="long_description">Montant total assuré pour ce type 
de garantie et chez cet assureur</field>
            <field name="fct_args">date</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="encours_garantie">
            <field name="translated_technical_name">encours_garantie</field>
            <field name="description">Encours de la garantie</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_option_loan_balance</field>
            <field name="long_description">Montant de l'encours des prêts 
rattachés à cette garantie</field>
            <field name="fct_args">date</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="encours_total_plus_garantie">
            <field name="translated_technical_name">encours_total_plus_garantie</field>
            <field name="description">Somme de l'encours total et de la garantie</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_total_loan_balance</field>
            <field name="long_description">Montant total de l'encours de 
l'ensemble des prêts définis dans le système ainsi que ceux définis sur cette 
garantie</field>
            <field name="fct_args">date</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
    </data>
</tryton>
