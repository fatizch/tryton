<?xml version="1.0"?>
<tree string="Loans">
    <field name="order"/>
    <field name="number"/>
    <field name="kind"/>
    <field name="rate" factor="100">
        <suffix string="%"/>
    </field>
    <field name="duration"/>
    <field name="duration_unit"/>
    <field name="amount">
        <suffix name="currency_symbol"/>
    </field>
    <field name="payment_frequency"/>
    <field name="funds_release_date"/>
    <field name="first_payment_date"/>
    <field name="insured_persons_name"/>
</tree>
