<?xml version="1.0"?>
<form string="Propagate Loan Share">
    <label name="share"/>
    <group id="share">
        <field name="share" factor="100" xexpand="0"/>
        <label string="%" id="percent" xexpand="0" xalign="0"/>
    </group>
    <newline/>
    <field name="covered_elements" widget="multiselection" colspan="2"/>
    <field name="loans" colspan="2" widget="multiselection"/>
    <field name="options" colspan="2" widget="multiselection"/>
    <newline/>
    <field name="contract"/>
</form>
