<data>
    <xpath expr="//page[@name='details']" position="after">
        <page name="extra_data">
            <label name="data_status" xalign="0"/>
            <field name="data_status"/>
            <newline/>
            <field name="extra_data" no_command="1" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="//notebook" position="after">
        <group col="99" id="buttons">
            <button name="confirm_attachment" string="Confirm"/>
        </group>
    </xpath>
</data>
