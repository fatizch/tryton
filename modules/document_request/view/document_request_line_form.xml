<form>
    <label name="document_desc"/>
    <group id="document_desc">
        <field name="document_desc"/>
        <field name="name"/>
    </group>
    <label name="for_object_selection"/>
    <field name="for_object_selection"/>
    <label name="attachment"/>
    <group id="attachment">
        <field name="attachment"/>
        <field name="attachment_data"/>
    </group>
    <label name="received"/>
    <group id="checkboxes">
        <field name="received"/>
        <label name="blocking"/>
        <field name="blocking"/>
    </group>
    <label name="request_date"/>
    <group id="request_date">
        <field name="request_date"/>
        <label name="send_date"/>
        <field name="send_date"/>
    </group>
    <label name="first_reception_date"/>
    <group id="first_reception_date">
        <field name="first_reception_date"/>
        <label name="reception_date"/>
        <field name="reception_date"/>
    </group>
    <notebook colspan="4">
        <page name="details">
            <field name="details"/>
        </page>
        <page name="matching_attachments">
            <field name="matching_attachments"/>
        </page>
    </notebook>
    <field name="allow_force_receive" invisible="1"/>
    <field name="for_object" invisible="1"/>
    <field name="added_manually" invisible="1"/>
</form>
