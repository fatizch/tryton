<tryton>
    <data>
        <!-- Default Document Request Access Rights -->
        <record model="ir.model.access" id="access_document_receive_line">
            <field name="model" search="[('model', '=', 'document.receive.line')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_document_request">
            <field name="model" search="[('model', '=', 'document.request')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.model.access" id="access_document_request_line">
            <field name="model" search="[('model', '=', 'document.request.line')]"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="False"/>
            <field name="perm_create" eval="False"/>
            <field name="perm_delete" eval="False"/>
        </record>
    </data>
</tryton>
