<tryton>
    <data>
        <!-- *******************************************
            ******** salary_range ***************
            ******************************************* -->
        <record model="ir.ui.view" id="salary_range_view_tree">
            <field name="model">salary_range</field>
            <field name="inherit" ref="cog_utils.versionned_object_view_tree"/>
            <field name="name">salary_range_tree</field>
        </record>
        <record model="ir.ui.view" id="salary_range_view_form">
            <field name="model">salary_range</field>
            <field name="inherit" ref="cog_utils.versionned_object_view_form"/>
            <field name="name">salary_range_form</field>
        </record>
        <record model="ir.action.act_window" id="act_model_salary_range">
            <field name="name">Salary Range</field>
            <field name="res_model">salary_range</field>
        </record>
        <record model="ir.action.act_window.view" id="act_model_salary_range_tree">
            <field name="sequence" eval="10"/>
            <field name="view" ref="salary_range_view_tree"/>
            <field name="act_window" ref="act_model_salary_range"/>
        </record>
        <record model="ir.action.act_window.view" id="act_model_salary_range_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="salary_range_view_form"/>
            <field name="act_window" ref="act_model_salary_range"/>
        </record>
        <menuitem parent="offered.menu_product_configuration" action="act_model_salary_range" id="menu_model_salary_range"/>
        <!-- *******************************************
            ******** salary_range Version ***************
            ******************************************* -->
        <record model="ir.ui.view" id="salary_range_version_view_tree">
            <field name="model">salary_range.version</field>
            <field name="inherit" ref="cog_utils.version_object_view_tree"/>
            <field name="name">salary_range_version_tree</field>
        </record>
        <record model="ir.ui.view" id="salary_range_version_view_form">
            <field name="model">salary_range.version</field>
            <field name="inherit" ref="cog_utils.version_object_view_form"/>
            <field name="name">salary_range_version_form</field>
        </record>
    </data>
</tryton>
