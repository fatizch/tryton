<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/field[@name='config_kind']" position="after">
        <newline/>
        <label name="min_age"/>
        <field name="min_age"/>
        <label name="max_age"/>
        <field name="max_age"/>
    </xpath>
    <xpath expr="/form/group[@id='sub_elem']/group[@id='sub_simple']" position="inside">
        <label name="sub_min_age"/>
        <field name="sub_min_age"/>
        <label name="sub_max_age"/>
        <field name="sub_max_age"/>
    </xpath>
</data>
