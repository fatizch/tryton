<form col="2">
    <group id="status" col="2">
        <label name="eligibility_status"/>
        <field name="eligibility_status"/>
    </group>
    <group id="buttons" col="99">
        <button name="check_eligibility" string="Check Eligibility" colspan="2" icon="reload_and_check"/>
        <button name="validate_services" string="Validate Eligibility" icon="check"/>
        <button name="reject_services" string="Decline Eligibility" icon="cancel-list"/>
    </group>
    <separator id="separator" colspan="2"/>
    <label name="eligibility_comment_wrapped" xalign="0"/>
    <label name="benefit_summary" xalign="0"/>
    <field name="eligibility_comment_wrapped" yexpand="0"/>
    <field name="benefit_summary" yexpand="0"/>
</form>
