<data>
    <xpath expr="/form/notebook/page[@id='managers']/notebook" position="inside">
        <page string="Eligibility" id="eligibility">
            <label name="refuse_from_rules"/>
            <field name="refuse_from_rules"/>
            <field name="eligibility_rules" colspan="4"/>
            <field name="eligibility_decisions" widget="one2many" colspan="4"/>
            <group id='default_decisions' col="6">
                <label name="decision_default"/>
                <field name="decision_default"/>
                <label name="accept_decision_default"/>
                <field name="accept_decision_default"/>
