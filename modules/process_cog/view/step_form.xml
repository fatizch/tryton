<data>
    <xpath expr="/form/group[@id='global_part']/field[@name='technical_name']" position="after">
        <label name="pyson"/>
        <field name="pyson"/>
        <label name="button_domain"/>
        <field name="button_domain"/>
        <label name="main_model"/>
        <field name="main_model"/>
        <label name="static_view"/>
