<data>
    <xpath expr="/form/field[@name='from_step']" position="after">
        <newline/>
        <label name="pyson_choice" xalign="0"/>
        <field name="pyson_choice"/>
        <label name="pyson_description" xalign="0"/>
        <field name="pyson_description"/>
        <newline/>
        <label name="choice_if_true" xalign="0"/>
        <field name="choice_if_true"/>
        <label name="choice_if_false" xalign="0"/>
        <field name="choice_if_false"/>
    </xpath>
    <xpath expr="/form/group[@id='methods']" position="replace_attributes">
        <group id="methods" colspan="2" xexpand="1" xfill="1" yexpand="1" yfill="1"/>
    </xpath>
    <xpath expr="/form/field[@name='authorizations']" position="replace_attributes">
        <field name="authorizations" xexpand="1" xfill="1" yexpand="1" yfill="1" colspan="4"/>
    </xpath>
</data>
