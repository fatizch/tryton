<tryton>
    <data depends="account_aggregate">
        <!-- Contract Manage Group -->
        <record model="ir.model.access" id="access_account_move_line_aggregated_contract_manage_group">
            <field name="model" search="[('model', '=', 'account.move.line.aggregated')]"/>
            <field name="group" ref="contract.group_contract_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="False"/>
        </record>
    </data>
