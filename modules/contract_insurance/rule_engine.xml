<tryton>
    <data>
        <record model="rule_engine.function" id="number_of_covered_elements">
            <field name="translated_technical_name">nombre_elements_couverts</field>
            <field name="description">Nombre élements couverts</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_number_of_covered_elements</field>
            <field name="long_description">Retourne le nombre d'élément couvert 
actif sur le contrat à la date du calcul de la règle</field>
            <field name="parent" ref="contract.te_contract"/>
        </record>
        <record model="rule_engine.function" id="initial_number_of_sub_covered_elements">
            <field name="translated_technical_name">nombre_initial_de_sous_elements_couverts</field>
            <field name="description">Nombre initial de sous-élements couverts</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_initial_number_of_sub_covered_elements</field>
            <field name="long_description">Retourne le nombre de sous élément couvert
à la date initiale d'effet du contrat</field>
            <field name="parent" ref="contract.te_contract"/>
        </record>
        <record model="rule_engine.function" id="_re_covered_element_rank">
            <field name="translated_technical_name">rang_element_couvert</field>
            <field name="description">Rang élement couvert</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_covered_element_rank</field>
            <field name="long_description">Retourne le rang de l'élement couvert 
courant pour la garantie courante à partir de 1.</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="number_of_covered_elements_for_coverage">
            <field name="translated_technical_name">nombre_elements_couverts_pour_garantie</field>
            <field name="description">Nombre élements couverts pour la garantie</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_number_of_covered_elements_for_coverage</field>
            <field name="long_description">Retourne le nombre d'élément couvert 
ayant souscrit à la garantie indépendamment d'une date</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="other_coverage_subscribed">
            <field name="translated_technical_name">autre_garantie_souscrite</field>
            <field name="description">Autre garantie souscrite</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_is_other_coverage_subscribed</field>
            <field name="long_description">Retourne 'Vrai' si la garantie passée en paramètre est souscrite</field>
            <field name="fct_args">code_garantie</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="all_subscribed_coverages">
            <field name="translated_technical_name">garanties_souscrites</field>
            <field name="description">Garanties souscrites</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_all_subscribed_coverages</field>
            <field name="long_description">Retourne la liste des codes des garanties souscrites. Le paramètre "date", optionnel, permet d'indiquer la date à considérer</field>
            <field name="fct_args">date</field>
            <field name="parent" ref="offered_insurance.dossier_garantie"/>
        </record>
        <record model="rule_engine.function" id="etape_du_processus_contrat">
            <field name="translated_technical_name">etape_du_processus_contrat</field>
            <field name="description">État du contrat dans le processus</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_contract_process_state</field>
            <field name="long_description">
Indique l'état dans lequel est le contrat dans le processus en cours. Il s'agit
du nom technique de l'étape actuelle. Si aucun processus n'est actif, retourne
'None'</field>
            <field name="parent" ref="contract.te_contract"/>
        </record>
    </data>
</tryton>
