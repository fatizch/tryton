<?xml version="1.0" encoding="utf-8"?>
<tryton>
  <data>
    <!-- ############################################################ -->
    <!-- Renewal Start -->
    <!-- ############################################################ -->
    <record model="ir.ui.view" id="contract_renew_parameters_form">
      <field name="model">contract.renew.parameters</field>
      <field name="type">form</field>
      <field name="name">contract_renew_parameters_form</field>
    </record>
    <!-- ############################################################ -->
    <!-- Renewal Result -->
    <!-- ############################################################ -->
    <record model="ir.ui.view" id="contract_renew_report_form">
      <field name="model">contract.renew.report</field>
      <field name="type">form</field>
      <field name="name">contract_renew_report_form</field>
    </record>
    <!-- ############################################################ -->
    <!-- Renewal Wizard -->
    <!-- ############################################################ -->
    <record model="ir.action.wizard" id="act_renewal_wizard">
      <field name="name">Renew Contracts</field>
      <field name="wiz_name">contract.renew</field>
    </record>
    <menuitem id="menu_contract_renewal" sequence="15" action="act_renewal_wizard"
        parent="contract.menu_contract_batch"/>
  </data>
</tryton>
