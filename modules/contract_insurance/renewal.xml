<?xml version="1.0" encoding="utf-8"?>
<tryton>
  <data>
    <!-- ############################################################ -->
    <!-- Renewal Start -->
    <!-- ############################################################ -->
    <record model="ir.ui.view" id="renewal_start_form">
      <field name="model">contract.renew.parameters</field>
      <field name="type">form</field>
      <field name="name">renewal_start_form</field>
    </record>
    <!-- ############################################################ -->
    <!-- Renewal Result -->
    <!-- ############################################################ -->
    <record model="ir.ui.view" id="renewal_result_form">
      <field name="model">contract.renew.report</field>
      <field name="type">form</field>
      <field name="name">renewal_result_form</field>
    </record>
    <!-- ############################################################ -->
    <!-- Renewal Wizard -->
    <!-- ############################################################ -->
    <record model="ir.action.wizard" id="act_renewal_wizard">
      <field name="name">Renew Contracts</field>
      <field name="wiz_name">contract.renew</field>
    </record>
    <menuitem id="menu_renew_contract" sequence="15" action="act_renewal_wizard"
        parent="contract_insurance.menu_contract"/>
  </data>
</tryton>
