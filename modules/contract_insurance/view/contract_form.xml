<?xml version="1.0" encoding="UTF-8"?>
<data>
  <xpath expr="/form/field[@name='end_date']" position="after">
    <label name="last_renewed"/>
    <field name="last_renewed"/>
    <label name="next_renewal_date"/>
    <field name="next_renewal_date"/>
  </xpath>
  <xpath expr="/form/notebook/page[@id='options']" position="before">
    <page string="Covered Elements" id="cov_elements" states="{'invisible': Eval('product_kind') != 'insurance'}">
        <field name="covered_elements"/>
    </page>
  </xpath>
  <xpath expr="/form/notebook/page[@id='options']" position="after">
    <page string="Documents" id="documents">
      <field name="documents" xexpand="1" xfill="1" yexpand="1" yfill="1" mode="form"/>
    </page>
    <page string="Managing Entities" id="managing_entities">
      <field name="agreements" colspan="4"/>
      <newline/>
      <field name="contracts" colspan="4"/>
    </page>
    <page string="Addresses" id="contract_adresses">
      <field name="addresses"/>
    </page>
  </xpath>
</data>
