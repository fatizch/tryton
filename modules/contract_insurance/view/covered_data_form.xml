<?xml version="1.0" encoding="utf-8"?>
<form string="Covered Data">
  <notebook>
    <page string="Main Data" id="main_data">
      <label name="option"/>
      <field name="option" xfill="1"/>
      <newline/>
      <label name="covered_element"/>
      <field name="covered_element"/>
      <newline/>
      <label name="deductible_duration"/>
      <field name="deductible_duration" widget="selection"/>
      <newline/>
      <label name="currency"/>
      <field name="currency"/>
      <newline/>
      <field name="extra_data" colspan="2" no_command="1"/>
      <newline/>
      <field name="clauses" colspan="2"/>
    </page>
    <page string="Extra Premium" id="extra_premium">
      <field name="extra_premiums" colspan="4"/>
      <button string="Propagate" name="propagate_extra_premiums" colspan="4"/>
    </page>
    <page string="Exclusions" id="exclusions">
      <field name="exclusions" colspan="4"/>
      <button string="Propagate" name="propagate_exclusions" colspan="4"/>
    </page>
  </notebook>
</form>
