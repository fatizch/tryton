<?xml version="1.0" encoding="utf-8"?>
<form string="Covered Element">
    <label name="contract"/>
    <field name="contract"/>
    <newline/>
    <label name="main_contract"/>
    <field name="main_contract"/>
    <newline/>
    <label name="item_desc"/>
    <field name="item_desc" widget="selection"/>
    <label name="name"/>
    <field name="name"/>
    <newline/>
    <label name="party"/>
    <field name="party"/>
    <field name="covered_name" invisible="1"/>
    <newline/>
    <notebook>
        <page id="details" string="Details">
            <field name="extra_data" colspan="4" no_command="1"/>
            <field name="options" colspan="4"/>
        </page>
        <page id="covered_relations" string="Covered Relations">
            <field name="covered_relations" colspan="4"/>
        </page>
        <page id="sub_elements" string="Sub Covered Elements">
            <field name="sub_covered_elements" colspan="4"/>
        </page>
    </notebook>
    <field name="item_kind"/>
    <newline/>
    <field name="is_person" invisible="1"/>
    <newline/>
    <field name="product" invisible="1"/>
    <field name="party_extra_data" invisible="1"/>
</form>
