<?xml version="1.0" encoding="utf-8"?>
<tryton>
  <data>
    <!-- ############# -->
    <!-- # Main Menu # -->
    <!-- ############# -->
    <menuitem name="Process Main Menu" id="process_menu_id" sequence="11"/>
    <menuitem parent="process_menu_id" sequence="100" id="menu_process_lang" name="Processes"/>
    <!-- ######### -->
    <!-- # Model # -->
    <!-- ######### -->
    <record model="ir.ui.view" id="model_tree">
      <field name="model">ir.model</field>
      <field name="type">tree</field>
      <field name="inherit" ref="ir.model_view_tree"/>
      <field name="name">model_tree</field>
    </record>
    <record model="ir.ui.view" id="model_view">
      <field name="model">ir.model</field>
      <field name="type">form</field>
      <field name="inherit" ref="ir.model_view_form"/>
      <field name="name">model_form</field>
    </record>
    <!-- ################## -->
    <!-- # Process Status # -->
    <!-- ################## -->
    <record model="ir.ui.view" id="process_status_form">
      <field name="model">process.status</field>
      <field name="type">form</field>
      <field name="name">process_status_form</field>
    </record>
    <record model="ir.ui.view" id="process_status_tree">
      <field name="model">process.status</field>
      <field name="type">tree</field>
      <field name="name">process_status_tree</field>
    </record>
    <!-- Entry Point -->
    <record model="ir.action.act_window" id="act_process_status">
      <field name="name">Process Status</field>
      <field name="res_model">process.status</field>
    </record>
    <record model="ir.action.act_window.view" id="act_process_status_view1">
      <field name="sequence" eval="16"/>
      <field name="view" ref="process_status_tree"/>
      <field name="act_window" ref="act_process_status"/>
    </record>
    <record model="ir.action.act_window.view" id="act_process_status_view2">
      <field name="sequence" eval="16"/>
      <field name="view" ref="process_status_form"/>
      <field name="act_window" ref="act_process_status"/>
    </record>
    <menuitem parent="process_menu_id" sequence="16" action="act_process_status" id="menu_process_status"/>
    <!-- ######################### -->
    <!-- # Process Step Relation # -->
    <!-- ######################### -->
    <record model="ir.ui.view" id="process_step_relation_form">
      <field name="model">process-process.step</field>
      <field name="type">form</field>
      <field name="name">process_step_relation_form</field>
    </record>
    <record model="ir.ui.view" id="process_step_relation_tree">
      <field name="model">process-process.step</field>
      <field name="type">tree</field>
      <field name="name">process_step_relation_tree</field>
    </record>
    <!-- ########### -->
    <!-- # Process # -->
    <!-- ########### -->
    <record model="ir.ui.view" id="process_desc_form">
      <field name="model">process</field>
      <field name="type">form</field>
      <field name="name">process_desc_form</field>
    </record>
    <record model="ir.ui.view" id="process_desc_tree">
      <field name="model">process</field>
      <field name="type">tree</field>
      <field name="name">process_desc_tree</field>
    </record>
    <!-- Entry Point -->
    <record model="ir.action.act_window" id="act_process_desc">
      <field name="name">Processes</field>
      <field name="res_model">process</field>
    </record>
    <record model="ir.action.act_window.view" id="act_process_desc_view1">
      <field name="sequence" eval="10"/>
      <field name="view" ref="process_desc_tree"/>
      <field name="act_window" ref="act_process_desc"/>
    </record>
    <record model="ir.action.act_window.view" id="act_process_desc_view2">
      <field name="sequence" eval="10"/>
      <field name="view" ref="process_desc_form"/>
      <field name="act_window" ref="act_process_desc"/>
    </record>
    <menuitem parent="process_menu_id" sequence="10" action="act_process_desc" id="menu_process_desc"/>
    <!-- ################ -->
    <!-- # Process Step # -->
    <!-- ################ -->
    <record model="ir.ui.view" id="step_desc_form">
      <field name="model">process.step</field>
      <field name="type">form</field>
      <field name="name">step_desc_form</field>
    </record>
    <record model="ir.ui.view" id="step_desc_tree">
      <field name="model">process.step</field>
      <field name="type">tree</field>
      <field name="name">step_desc_tree</field>
    </record>
    <!-- Entry Point -->
    <record model="ir.action.act_window" id="act_step_desc">
      <field name="name">Process Steps</field>
      <field name="res_model">process.step</field>
    </record>
    <record model="ir.action.act_window.view" id="act_step_desc_view1">
      <field name="sequence" eval="11"/>
      <field name="view" ref="step_desc_tree"/>
      <field name="act_window" ref="act_step_desc"/>
    </record>
    <record model="ir.action.act_window.view" id="act_step_desc_view2">
      <field name="sequence" eval="11"/>
      <field name="view" ref="step_desc_form"/>
      <field name="act_window" ref="act_step_desc"/>
    </record>
    <menuitem parent="process_menu_id" sequence="11" action="act_step_desc" id="menu_step_desc"/>
    <!-- ###################### -->
    <!-- # Process Transition # -->
    <!-- ###################### -->
    <record model="ir.ui.view" id="step_transition_form">
      <field name="model">process.transition</field>
      <field name="type">form</field>
      <field name="name">step_transition_form</field>
    </record>
    <record model="ir.ui.view" id="step_transition_tree">
      <field name="model">process.transition</field>
      <field name="type">tree</field>
      <field name="name">step_transition_tree</field>
    </record>
    <!-- ######################### -->
    <!-- # Generate Proces Graph # -->
    <!-- ######################### -->
    <record model="ir.action.report" id="report_generate_graph">
      <field name="name">Graph</field>
      <field name="model">process</field>
      <field name="report_name">process.generate_graph.report</field>
    </record>
    <record model="ir.action.wizard" id="generate_graph_wizard">
      <field name="name">Graph</field>
      <field name="wiz_name">process.generate_graph</field>
      <field name="model">process</field>
    </record>
    <record model="ir.action.keyword" id="generate_graph_wizard_keyword">
      <field name="keyword">form_print</field>
      <field name="model">process,-1</field>
      <field name="action" ref="generate_graph_wizard"/>
    </record>
    <!-- ######## -->
    <!-- # Code # -->
    <!-- ######## -->
    <record model="ir.ui.view" id="code_form">
      <field name="model">process.action</field>
      <field name="type">form</field>
      <field name="name">code_form</field>
    </record>
    <record model="ir.ui.view" id="code_tree">
      <field name="model">process.action</field>
      <field name="type">tree</field>
      <field name="name">code_tree</field>
    </record>
  </data>
</tryton>
