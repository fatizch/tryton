<tryton>
    <data>
        <!-- ######################### -->
        <!-- # Process Configuration # -->
        <!-- ######################### -->
        <record model="ir.ui.view" id="configuration_view_form">
            <field name="model">process.configuration</field>
            <field name="type">form</field>
            <field name="name">configuration_form</field>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_configuration">
            <field name="name">Configuration</field>
            <field name="res_model">process.configuration</field>
        </record>
        <record model="ir.action.act_window.view" id="act_configuration_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="configuration_view_form"/>
            <field name="act_window" ref="act_configuration"/>
        </record>
        <menuitem parent="process_menu_id" sequence="100" action="act_configuration" id="menu_configuration"/>
    </data>
</tryton>
