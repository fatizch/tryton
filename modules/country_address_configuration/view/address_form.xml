<data>
    <xpath expr="/form/label[@name='street']" position="after">
        <newline/>
        <group id="left" colspan="2" col="1">
            <field name="address_lines" no_command="1" xfill="1" xexpand="1"/>
            <group id="street" xfill="1" xexpand="1" col="1">
                <separator name="street" xfill="1" xexpand="1"/>
                <field name="street" xfill="1" xexpand="1"/>
            </group>
        </group>
    </xpath>
    <xpath expr="/form" position="inside">
        <newline/>
        <field name="has_address_lines"/>
    </xpath>
</data>
