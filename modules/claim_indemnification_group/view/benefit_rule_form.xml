<data>
    <xpath expr="/form/label[@name='annuity_frequency']" position="before">
        <label name="force_annuity_frequency"/>
        <field name="force_annuity_frequency"/>
    </xpath>
    <xpath expr="/form/label[@name='indemnification_rule']" position="before">
        <label name="force_indemnification_rule"/>
        <field name="force_indemnification_rule"/>
    </xpath>
    <xpath expr="/form/field[@name='indemnification_rule_extra_data']" position="after">
        <field name="indemnification_rules" colspan="4"/>
    </xpath>
    <xpath expr="/form/label[@name='deductible_rule']" position="before">
        <label name="force_deductible_rule"/>
        <field name="force_deductible_rule"/>
    </xpath>
    <xpath expr="/form/field[@name='deductible_rule_extra_data']" position="after">
        <field name="deductible_rules" colspan="4"/>
    </xpath>
    <xpath expr="/form/label[@name='revaluation_rule']" position="before">
        <label name="force_revaluation_rule"/>
        <field name="force_revaluation_rule"/>
    </xpath>
    <xpath expr="/form/field[@name='revaluation_rule_extra_data']" position="after">
        <field name="revaluation_rules" colspan="4"/>
    </xpath>

    <xpath expr="/form" position="inside">
        <field name="is_group" invisible="1"/>
        <field name="benefit" invisible="1"/>
    </xpath>
</data>
