<tryton>
    <data>
        <record model="rule_engine.function" id="max_revaluation_date">
            <field name="translated_technical_name">date_revalorisation_maximum</field>
            <field name="description">Date maximum de revalorisation</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_revaluation_max_revaluation_date</field>
            <field name="long_description">Retourne la date maximale de 
revalorisation. Cette donnée est basée sur la date de résiliation du contrat 
lorsque celui ci est résilié avec blocage des prestations au niveau atteint</field>
            <field name="parent" ref="claim.dossier_sinistre"/>
        </record>
        <record model="rule_engine.function" id="min_revaluation_date">
            <field name="translated_technical_name">date_revalorisation_minimum</field>
            <field name="description">Date minimum de revalorisation</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_revaluation_min_revaluation_date</field>
            <field name="long_description">Retourne la date minimale de 
revalorisation. Cette donnée est basée sur la date de début du contrat 
lorsque celui ci est souscrit avec l'option "en complément du précédent assureur"</field>
            <field name="parent" ref="claim.dossier_sinistre"/>
        </record>
        <record model="rule_engine.function" id="get_previous_insurer_base_amount">
            <field name="translated_technical_name">montant_base_ancien_assureur</field>
            <field name="description">Montant de base versé par l'assureur précédent</field>
            <field name="type">function</field>
            <field name="language" ref="ir.lang_fr"/>
            <field name="namespace">rule_engine.runtime</field>
            <field name="name">_re_get_previous_insurer_base_amount</field>
            <field name="long_description">Retourne le montant versé par le précédent assureur</field>
            <field name="parent" ref="claim.dossier_sinistre"/>
        </record>
    </data>
</tryton>
