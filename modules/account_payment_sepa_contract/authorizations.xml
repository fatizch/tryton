<tryton>
    <data depends="bank_fr">
        <!-- Contract Manage Group -->
        <record model="ir.model.access" id="access_bank_contract_management_group">
            <field name="model" search="[('model', '=', 'bank.agency')]"/>
            <field name="group" ref="contract.group_contract_manage"/>
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_delete" eval="False"/>
        </record>
        <record model="ir.ui.menu-res.group" id="menu_message_form_contract_manage">
            <field name="menu" ref="account_payment_sepa.menu_message_form"/>
            <field name="group" ref="contract.group_contract_manage"/>
        </record>
        <record model="ir.model.button-res.group" id="init_sepa_mandate_button_group_contract_manage">
            <field name="button" ref="init_sepa_mandate"/>
            <field name="group" ref="contract.group_contract_manage"/>
        </record>
    </data>
</tryton>
