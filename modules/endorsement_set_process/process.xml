<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <!-- ########################## -->
        <!-- # Resume Process Keyword # -->
        <!-- ########################## -->
        <record model="ir.action.keyword" id="act_resume_endorsement_set_process_keyword">
            <field name="keyword">form_action</field>
            <field name="model">endorsement.set,-1</field>
            <field name="action" ref="process_cog.act_resume_process"/>
        </record>
        <!-- Endorsements view in process -->
        <record model="ir.ui.view" id="endorsement_in_endorsement_set_process_view_tree">
            <field name="model">endorsement</field>
            <field name="type">tree</field>
            <field name="name">endorsement_in_endorsement_set_process_tree</field>
        </record>
        <!-- Generated Endorsement Sets view in process -->
        <record model="ir.ui.view" id="generated_endorsement_sets_in_process_view_tree">
            <field name="model">endorsement.set</field>
            <field name="type">tree</field>
            <field name="inherit" ref="endorsement_set.endorsement_set_view_list"/>
            <field name="name">generated_endorsement_sets_in_process_tree</field>
        </record>
    </data>
</tryton>
