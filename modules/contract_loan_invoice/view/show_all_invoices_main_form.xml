<form col="8">
    <label name="contract"/>
    <field name="contract" colspan="7"/>
    <label name="amount"/>
    <group id="amount">
        <field name="amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <label name="tax_amount"/>
    <group id="tax_amount">
        <field name="tax_amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <label name="fee"/>
    <group id="fee">
        <field name="fee" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <label name="total_amount"/>
    <group id="total_amount">
        <field name="total_amount" xexpand="0"/>
        <label name="currency_symbol" string="" xexpand="0" xalign="0"/>
    </group>
    <field name="invoices" colspan="8" expand_toolbar="0"/>
    <field name="currency_digits" invisible="1"/>
</form>
