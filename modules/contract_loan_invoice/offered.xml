<?xml version="1.0"?>
<tryton>
    <data>
        <!-- ########### -->
        <!-- # Product # -->
        <!-- ########### -->
        <record model="ir.ui.view" id="product_view_form">
            <field name="model">offered.product</field>
            <field name="type">form</field>
            <field name="inherit" ref="offered.product_view_form"/>
            <field name="name">product_form</field>
        </record>
        <!-- ####################### -->
        <!-- # Loan Mean Rate Rule # -->
        <!-- ####################### -->
        <record model="ir.ui.view" id="mean_rate_rule_view_form">
            <field name="model">loan.mean_rate_rule</field>
            <field name="type">form</field>
            <field name="name">mean_rate_rule_form</field>
        </record>
        <record model="ir.ui.view" id="mean_rate_rule_view_list">
            <field name="model">loan.mean_rate_rule</field>
            <field name="type">tree</field>
            <field name="name">mean_rate_rule_list</field>
        </record>
        <!-- Entry Point -->
        <record model="ir.action.act_window" id="act_mean_rate_rule">
            <field name="name">Mean Rate Rule</field>
            <field name="res_model">loan.mean_rate_rule</field>
        </record>
        <record model="ir.action.act_window.view" id="act_mean_rate_rule_view1">
            <field name="sequence" eval="16"/>
            <field name="view" ref="mean_rate_rule_view_list"/>
            <field name="act_window" ref="act_mean_rate_rule"/>
        </record>
        <record model="ir.action.act_window.view" id="act_mean_rate_rule_view2">
            <field name="sequence" eval="16"/>
            <field name="view" ref="mean_rate_rule_view_form"/>
            <field name="act_window" ref="act_mean_rate_rule"/>
        </record>
        <menuitem parent="offered.menu_product_configuration" sequence="20" action="act_mean_rate_rule" id="menu_mean_rate_rule"/>
        <!-- ############ -->
        <!-- # Fee Rule # -->
        <!-- ############ -->
        <record model="ir.ui.view" id="fee_rule_view_form">
            <field name="model">loan.mean_rate_rule.fee_rule</field>
            <field name="type">form</field>
            <field name="name">fee_rule_form</field>
        </record>
        <record model="ir.ui.view" id="fee_rule_view_list">
            <field name="model">loan.mean_rate_rule.fee_rule</field>
            <field name="type">tree</field>
            <field name="name">fee_rule_list</field>
        </record>
    </data>
</tryton>
