<data>
    <xpath expr="//field[@name='action']" position="after">
        <label name="delay"/>
        <group id="delay">
            <field name="delay" xexpand="0" xalign="0"/>
            <field name="delay_unit" xexpand="0" xalign="0"/>
        </group>
    </xpath>
    <xpath expr="//field[@name='event_types']" position="after">
        <newline/>
        <field name="planned_event_types" colspan="4"/>
    </xpath>
</data>
