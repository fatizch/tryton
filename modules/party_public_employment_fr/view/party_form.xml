<data>
    <xpath expr="/form/notebook/page[@id='role']/group[@id='role_group']" position="inside">
        <label name="is_csrh"/>
        <field name="is_csrh" xexpand="0" width="25"/>
        <label name="is_payroll_service"/>
        <field name="is_payroll_service" xexpand="0" width="25"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='role']/notebook" position="inside">
        <page id="csrh" string="CSRH">
            <field name="csrh_role" mode="form,tree"/>
        </page>
        <page id="payroll_service" string="Payroll Service">
            <field name="payroll_service_role" mode="form,tree"/>
        </page>
    </xpath>
</data>
