<data>
    <xpath expr="/form/field[@name='currency']" position="after">
        <newline/>
        <label name="start_date"/>
        <field name="start_date"/>
        <label name="end_date"/>
        <field name="end_date"/>
    </xpath>
    <xpath expr="/form/label[@name='bank']" position="before">
        <label name="number"/>
        <field name="number"/>
    </xpath>
    <xpath expr="/form/field[@name='numbers']" position="replace_attributes">
        <field name="numbers" invisible="1"/>
    </xpath>
</data>
