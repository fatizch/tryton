<?xml version="1.0"?>
<data>
    <xpath expr="/tree/field[@name='type']" position="replace_attributes">
        <field name="type" tree_invisible="1"/>
    </xpath>
    <xpath expr="/tree/field[@name='unit_price']" position="replace">
        <field name="unit_price">
            <suffix name="currency_symbol"/>
        </field>
    </xpath>
</data>


