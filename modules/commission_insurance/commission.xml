<?xml version="1.0"?>
<tryton>
    <data>
        <!-- ######### -->
        <!-- # Agent # -->
        <!-- ######### -->
        <record model="ir.ui.view" id="agent_view_form">
            <field name="model">commission.agent</field>
            <field name="type">form</field>
            <field name="inherit" ref="commission.agent_view_form"/>
            <field name="name">agent_form</field>
        </record>
        <record model="ir.ui.view" id="agent_view_list">
            <field name="model">commission.agent</field>
            <field name="type">tree</field>
            <field name="inherit" ref="commission.agent_view_list"/>
            <field name="name">agent_list</field>
        </record>
        <!-- ############# -->
        <!-- # Plan Line # -->
        <!-- ############# -->
        <record model="ir.ui.view" id="plan_line_view_form">
            <field name="model">commission.plan.line</field>
            <field name="inherit" ref="commission.plan_line_view_form"/>
            <field name="name">plan_line_form</field>
        </record>
        <record model="ir.ui.view" id="plan_line_view_list_sequence">
            <field name="model">commission.plan.line</field>
            <field name="inherit" ref="commission.plan_line_view_list_sequence"/>
            <field name="name">plan_line_list_sequence</field>
        </record>
        <!-- ############## -->
        <!-- # Commission # -->
        <!-- ############## -->
        <record model="ir.ui.view" id="commission_view_form">
            <field name="model">commission</field>
            <field name="type">form</field>
            <field name="inherit" ref="commission.commission_view_form"/>
            <field name="name">commission_form</field>
        </record>
        <record model="ir.ui.view" id="commission_view_list">
            <field name="model">commission</field>
            <field name="type">tree</field>
            <field name="inherit" ref="commission.commission_view_list"/>
            <field name="name">commission_list</field>
        </record>
        <record model="ir.action.act_window.domain" id="act_commission_domain_broker">
            <field name="name">Broker</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[('type_', '=', 'out')]" pyson="1"/>
            <field name="act_window" ref="commission.act_commission_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_commission_domain_insurer">
            <field name="name">Insurer</field>
            <field name="sequence" eval="20"/>
            <field name="domain" eval="[('type_', '=', 'in')]" pyson="1"/>
            <field name="act_window" ref="commission.act_commission_form"/>
        </record>
        <!-- ######## -->
        <!-- # Plan # -->
        <!-- ######## -->
        <record model="ir.ui.view" id="plan_view_form">
            <field name="model">commission.plan</field>
            <field name="type">form</field>
            <field name="inherit" ref="commission.plan_view_form"/>
            <field name="name">plan_form</field>
        </record>
        <record model="ir.ui.view" id="plan_view_list">
            <field name="model">commission.plan</field>
            <field name="type">tree</field>
            <field name="inherit" ref="commission.plan_view_list"/>
            <field name="name">plan_list</field>
        </record>
        <!-- ######################### -->
        <!-- # Plan Calculation Date # -->
        <!-- ######################### -->
        <record model="ir.ui.view" id="plan_calculation_date_view_list">
            <field name="model">commission.plan.date</field>
            <field name="type">tree</field>
            <field name="name">plan_calculation_date_list</field>
        </record>
        <!-- ################# -->
        <!-- # Create Agents # -->
        <!-- ################# -->
        <record model="ir.action.wizard" id="act_commission_create_agents">
            <field name="name">Create Agents</field>
            <field name="wiz_name">commission.create_agents</field>
        </record>
        <record model="ir.ui.view" id="commission_create_agents_parties_view_form">
            <field name="model">commission.create_agents.parties</field>
            <field name="type">form</field>
            <field name="name">commission_create_agents_parties_form</field>
        </record>
        <record model="ir.ui.view" id="commission_create_agents_ask_view_form">
            <field name="model">commission.create_agents.ask</field>
            <field name="type">form</field>
            <field name="name">commission_create_agents_ask_form</field>
        </record>
        <menuitem parent="commission.menu_commission" action="act_commission_create_agents" id="menu_commission_create_agents" sequence="30"/>
        <!-- ################## -->
        <!-- # Create Invoice # -->
        <!-- ################## -->
        <record model="ir.ui.view" id="commission_create_invoice_ask_view_form">
            <field name="model">commission.create_invoice.ask</field>
            <field name="type">form</field>
            <field name="inherit" ref="commission.commission_create_invoice_ask_view_form"/>
            <field name="name">commission_create_invoice_ask_form</field>
        </record>
        <!-- ################# -->
        <!-- # Change Broker # -->
        <!-- ################# -->
        <record model="ir.action.wizard" id="act_change_broker">
            <field name="name">Change Broker</field>
            <field name="wiz_name">commission.change_broker</field>
        </record>
        <menuitem parent="commission.menu_commission" action="act_change_broker" id="menu_commission_change_broker" sequence="31"/>
        <!-- ##################### -->
        <!-- # Select New Broker # -->
        <!-- ##################### -->
        <record model="ir.ui.view" id="select_new_broker_view_form">
            <field name="model">commission.change_broker.select_new_broker</field>
            <field name="type">form</field>
            <field name="name">select_new_broker_form</field>
        </record>
        <!-- ##################### -->
        <!-- # Filter commission # -->
        <!-- ##################### -->
        <record model="ir.action.wizard" id="act_filter_commission">
            <field name="name">Commissions</field>
            <field name="wiz_name">commission.filter_commission</field>
            <field name="model">account.invoice</field>
        </record>
        <!-- ###################################### -->
        <!-- # Display Commissions Synthesis View # -->
        <!-- ###################################### -->
        <record model="ir.ui.view" id="synthesis_start_view_form">
            <field name="model">commission.synthesis.start</field>
            <field name="type">form</field>
            <field name="name">commission_synthesis_start_form</field>
        </record>
        <record model="ir.ui.view" id="synthesis_show_view_form">
            <field name="model">commission.synthesis.show</field>
            <field name="type">form</field>
            <field name="name">commission_synthesis_show_form</field>
        </record>
        <record model="ir.ui.view" id="synthesis_year_line_list">
            <field name="model">commission.synthesis.year_line</field>
            <field name="type">tree</field>
            <field name="name">commission_synthesis_year_line_list</field>
        </record>
        <!-- ######################## -->
        <!-- # Commission per agent # -->
        <!-- ######################## -->
	<record model="ir.action.act_window" id="act_commission_per_agent_form_relate">
	    <field name="name">Commissions per agent</field>
	    <field name="res_model">commission.per_agent</field>
	    <field name="domain" eval="[('invoice', 'in', Eval('active_ids'))]" pyson="1"/>
	</record>
	<record model="ir.action.wizard" id="act_commission_per_agent_relate_details">
	    <field name="name">Filtering wizard</field>
	    <field name="wiz_name">commission.per_agent.open_detail</field>
	</record>
	<record model="ir.action.keyword" id="act_commission_per_agent_relate_keyword1">
	    <field name="keyword">tree_open</field>
	    <field name="model">commission.per_agent,-1</field>
	    <field name="action" ref="act_commission_per_agent_relate_details"/>
	</record>
        <record model="ir.ui.view" id="commission_per_agent_view_list">
	    <field name="model">commission.per_agent</field>
	    <field name="type">tree</field>
	    <field name="name">commission_per_agent_view_list</field>
	</record>
	<record model="ir.action.act_window.view" id="act_commission_per_agent_relate_view1">
	    <field name="sequence" eval="10"/>
	    <field name="view" ref="commission_per_agent_view_list"/>
	    <field name="act_window" ref="act_commission_per_agent_form_relate"/>
	</record>
	<record model="ir.action.keyword" id="act_commission_per_agent_form_relate_keyword">
	    <field name="keyword">form_relate</field>
	    <field name="model">account.invoice,-1</field>
	    <field name="action" ref="act_commission_per_agent_form_relate"/>
	</record>
    </data>
</tryton>
