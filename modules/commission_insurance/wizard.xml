<tryton>
    <data>
        <record model="ir.ui.view" id="commission_simulate_params_view_form">
            <field name="model">commission.simulate.params</field>
            <field name="type">form</field>
            <field name="name">simulate_params_form</field>
        </record>
        <record model="ir.action.wizard" id="act_simulate_commissions_wizard">
            <field name="name">Simulate Commissions</field>
            <field name="wiz_name">commission.simulate</field>
            <field name="window" eval="True"/>
        </record>
        <record model="ir.action.keyword" id="act_simulate_commissions_keyword">
            <field name="keyword">form_action</field>
            <field name="model">offered.product,-1</field>
            <field name="action" ref="act_simulate_commissions_wizard"/>
        </record>
        <record model="ir.ui.view" id="simulate_commissions_line_view_list">
            <field name="model">commission.simulate.line</field>
            <field name="type">tree</field>
            <field name="name">simulate_commissions_line_list</field>
        </record>
        <record model="ir.ui.view" id="simulate_commissions_line_view_form">
            <field name="model">commission.simulate.line</field>
            <field name="type">form</field>
            <field name="name">simulate_commissions_line_form</field>
        </record>
        <record model="ir.model.button" id="button_simulate_commissions_refresh">
            <field name="name">refresh</field>
            <field name="model" search="[('model', '=', 'commission.simulate.params')]"/>
        </record>
    </data>
</tryton>
