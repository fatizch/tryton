<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="party_view_form">
            <field name="model">party.party</field>
            <field name="type">form</field>
            <field name="inherit" ref="party.party_view_form"/>
            <field name="name">party_form</field>
        </record>
        <record model="ir.ui.view" id="party_view_tree">
            <field name="model">party.party</field>
            <field name="type">tree</field>
            <field name="inherit" ref="party.party_view_tree"/>
            <field name="name">party_tree</field>
        </record>
        <menuitem parent="commission.menu_commission" sequence="20" id="menu_commission_insurer_form" icon="insurer" action="offered_insurance.act_insurer_form"/>
        <record model="ir.action.act_window" id="act_broker_commissionned_contract_relate_form">
            <field name="name">Commissionned Contracts</field>
            <field name="res_model">contract</field>
            <field name="domain">[('agent.party.network', '=', Eval('active_id'))]</field>
        </record>
        <record model="ir.action.keyword" id="act_broker_commissioned_contract_relate_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">distribution.network,-1</field>
            <field name="action" ref="act_broker_commissionned_contract_relate_form"/>
        </record>
        <record model="ir.action.act_window" id="act_commission_relate_form">
            <field name="name">Commission</field>
            <field name="res_model">commission</field>
            <field name="domain">[('broker', '=', Eval('active_id'))]</field>
        </record>
        <record model="ir.action.keyword" id="act_commission_relate_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">distribution.network,-1</field>
            <field name="action" ref="act_commission_relate_form"/>
        </record>
    </data>
</tryton>
