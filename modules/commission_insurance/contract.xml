<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="type">form</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="name">contract_form</field>
        </record>
        <!-- ############################################################ -->
        <!-- ########### Commission relate on contract ########### -->
        <!-- ############################################################ -->
        <record model="ir.action.act_window" id="act_commission_aggregated_contract_relate_form1">
            <field name="name">Aggregated commissions</field>
            <field name="res_model">commission.aggregated</field>
            <field name="domain" eval="[('contract', 'in', Eval('active_ids'))]" pyson="1"/>
        </record>
	<record model="ir.action.act_window.view" id="act_commission_aggregated_relate_view3">
	    <field name="sequence" eval="10"/>
	    <field name="view" ref="commission_insurance.commission_aggregated_view_list"/>
	    <field name="act_window" ref="act_commission_aggregated_contract_relate_form1"/>
	</record>
        <record model="ir.action.keyword" id="act_commission_contract_relate_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">contract,-1</field>
            <field name="action" ref="act_commission_aggregated_contract_relate_form1"/>
        </record>
    </data>
</tryton>
