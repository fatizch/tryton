<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/notebook/page[@id='role']/group[@id='role_group']" position="inside">
        <label name="is_broker"/>
        <field name="is_broker" xexpand="0" width="25"/>
        <group id="invisible" string="">
            <button name="button_commissions_synthesis" string="Show commissions synthesis" />
        </group>
    </xpath>
    <xpath expr="/form/notebook/page[@id='role']/notebook" position="inside">
        <page id="broker" string="Broker">
            <label name="broker"/>
            <field name="broker"/>
            <field name="agents" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='accounting']/field[@name='supplier_payment_term']" position="after">
        <label name="automatic_wire_transfer"/>
        <field name="automatic_wire_transfer"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='role']/notebook/page[@id='insurer']/field[@name='insurer_role']" position="after">
        <field name="agents" colspan="4"/>
    </xpath>
</data>
