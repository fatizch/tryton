<data>
    <xpath expr="/form/field[@name='name']" position="after">
        <label name="code"/>
        <field name="code"/>
    </xpath>
    <xpath expr="/form/field[@name='lines']" position="replace">
        <notebook colspan="4">
            <page name="lines">
                <field name="lines" colspan="2" view_ids="commission.plan_line_view_list_sequence"/>
                <field name="computation_dates" colspan="2"/>
            </page>
            <page id="admin" string="Administration">
                <field name="tags" colspan="2"/>
                <field name="commissioned_products" colspan="2"/>
            </page>
        </notebook>
    </xpath>
    <xpath expr="/form/notebook" position="before">
        <label name="type_"/>
        <field name="type_"/>
        <newline/>
        <label name="insurer_plan"/>
        <field name="insurer_plan"/>
        <newline/>
    </xpath>
</data>
