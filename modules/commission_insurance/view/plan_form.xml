<?xml version="1.0" encoding="utf-8"?>
<data>
    <xpath expr="/form/field[@name='name']" position="after">
        <label name="code"/>
        <field name="code"/>
    </xpath>
    <xpath expr="/form/field[@name='lines']" position="replace">
        <notebook colspan="4">
            <page string="Lines" id="lines">
                <field name="lines" colspan="4" view_ids="commission.plan_line_view_list_sequence"/>
            </page>
            <page string="Plan Relation" id="plan_relations">
                <field name="plan_relation"/>
                <field name="reverse_plan_relation"/>
            </page>
        </notebook>
    </xpath>
</data>

