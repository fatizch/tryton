<data>
    <xpath expr="/form/field[@name='name']" position="after">
        <label name="code"/>
        <field name="code"/>
        <label name="active"/>
        <field name="active"/>
    </xpath>
    <xpath expr="/form/field[@name='lines']" position="replace">
        <notebook colspan="4">
            <page name="lines">
                <field name="lines" colspan="4" view_ids="commission.plan_line_view_form,commission.plan_line_view_list_sequence" mode="form,tree"/>
            </page>
            <page id="data" string="Data">
                <field name="computation_dates" colspan="2"/>
            </page>
            <page name="tags">
                <field name="tags" colspan="4"/>
            </page>
        </notebook>
    </xpath>
    <xpath expr="/form/notebook" position="before">
        <label name="type_"/>
        <field name="type_"/>
        <newline/>
        <label name="insurer_plan"/>
        <field name="insurer_plan"/>
        <newline/>
    </xpath>
</data>
