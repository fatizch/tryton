<data>
    <xpath expr="/form/field[@name='origin']" position="after">
        <newline/>
        <label name="commissioned_contract"/>
        <field name="commissioned_contract"/>
        <label name="commissioned_option"/>
        <field name="commissioned_option"/>
    </xpath>
    <xpath expr="/form/field[@name='currency']" position="after">
        <label name="base_amount" xexpand="0"/>
        <field name="base_amount"/>
        <label name="commission_rate"/>
        <group id="commission_rate">
            <field name="commission_rate" factor="100" xexpand="0"/>
            <label string="%" id="rate" xexpand="0" xalign="0"/>
        </group>
    </xpath>
</data>
