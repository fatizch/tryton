<tryton>
    <data>
        <record model="ir.action.act_window" id="act_option_agents_relate_form">
            <field name="name">Agents</field>
            <field name="res_model">commission.agent</field>
            <field name="domain" eval="[('plan.lines.options', 'in', [Eval('active_id')])]" pyson="1"/>
        </record>
        <record model="ir.action.keyword" id="act_option_agents_relate_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">offered.option.description,-1</field>
            <field name="action" ref="act_option_agents_relate_form"/>
        </record>
        <record model="ir.action.act_window" id="act_product_agents_relate_form">
            <field name="name">Agents</field>
            <field name="res_model">commission.agent</field>
            <field name="domain" eval="[('commissioned_products', 'in', [Eval('active_id')])]" pyson="1"/>
        </record>
        <record model="ir.action.keyword" id="act_product_agents_relate_keyword">
            <field name="keyword">form_relate</field>
            <field name="model">offered.product,-1</field>
            <field name="action" ref="act_product_agents_relate_form"/>
        </record>
    </data>
</tryton>
