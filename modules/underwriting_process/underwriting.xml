<tryton>
    <data>
        <!-- ################ -->
        <!-- # Underwriting # -->
        <!-- ################ -->
        <record model="ir.ui.view" id="underwriting_view_list">
            <field name="model">underwriting</field>
            <field name="inherit" ref="underwriting.underwriting_view_list"/>
            <field name="name">underwriting_list</field>
        </record>
        <record model="ir.action.keyword" id="act_resume_underwriting_process_keyword">
            <field name="keyword">form_action</field>
            <field name="model">underwriting,-1</field>
            <field name="action" ref="process_cog.act_resume_process"/>
        </record>
        <!-- ######################## -->
        <!-- # Underwriting Buttons # -->
        <!-- ######################## -->
        <record model="ir.model.button" id="button_resume">
            <field name="name">button_resume</field>
            <field name="model" search="[('model', '=', 'underwriting')]"/>
        </record>
    </data>
</tryton>
