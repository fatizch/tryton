<tryton>
    <data>
        <record model="ir.action.act_window" id="act_statement_bank_deposit_form">
            <field name="name">Bank Deposit Ticket</field>
            <field name="domain" eval="[('in_bank_deposit_ticket', '=', 'True')]" pyson="1"/>
            <field name="res_model">account.statement</field>
        </record>
        <record model="ir.action.act_window.view" id="act_statement_bank_deposit_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="account_statement.statement_view_tree"/>
            <field name="act_window" ref="act_statement_bank_deposit_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_statement_bank_deposit_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="account_statement.statement_view_form"/>
            <field name="act_window" ref="act_statement_bank_deposit_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_statement_bank_deposit_form_domain_draft">
            <field name="name">Draft</field>
            <field name="sequence" eval="10"/>
            <field name="domain" eval="[('state', '=', 'draft')]" pyson="1"/>
            <field name="act_window" ref="act_statement_bank_deposit_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_statement_bank_deposit_form_domain_validated">
            <field name="name">Validated</field>
            <field name="sequence" eval="20"/>
            <field name="domain" eval="[('state', '=', 'validated')]" pyson="1"/>
            <field name="act_window" ref="act_statement_bank_deposit_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_statement_bank_deposit_form_domain_posted">
            <field name="name">Posted</field>
            <field name="sequence" eval="30"/>
            <field name="domain" eval="[('state', '=', 'posted')]" pyson="1"/>
            <field name="act_window" ref="act_statement_bank_deposit_form"/>
        </record>
        <record model="ir.action.act_window.domain" id="act_statement_bank_deposit_form_domain_all">
            <field name="name">All</field>
            <field name="sequence" eval="40"/>
            <field name="act_window" ref="act_statement_bank_deposit_form"/>
        </record>
        <menuitem parent="account_statement.menu_statements" sequence="10" action="act_statement_bank_deposit_form" id="menu_statement_bank_deposit_form"/>
        <record model="ir.sequence.type" id="sequence_type_statement">
            <field name="name">Statement</field>
            <field name="code">statement</field>
        </record>
        <record model="ir.sequence.type-res.group" id="sequence_type_statement_group_admin">
            <field name="sequence_type" ref="sequence_type_statement"/>
            <field name="group" ref="res.group_admin"/>
        </record>
        <record model="ir.sequence.type-res.group" id="sequence_type_statement_group_statement">
            <field name="sequence_type" ref="sequence_type_statement"/>
            <field name="group" ref="account_statement.group_statement"/>
        </record>
        <record model="ir.ui.view" id="statement_line_view_tree_editable">
            <field name="model">account.statement.line</field>
            <field name="inherit" ref="account_statement.statement_line_view_tree_editable"/>
            <field name="name">statement_line_tree_editable</field>
        </record>
        <record model="ir.ui.view" id="statement_line_view_tree">
            <field name="model">account.statement.line</field>
            <field name="inherit" ref="account_statement.statement_line_view_tree"/>
            <field name="name">statement_line_tree</field>
        </record>
        <record model="ir.ui.view" id="statement_line_view_form">
            <field name="model">account.statement.line</field>
            <field name="inherit" ref="account_statement.statement_line_view_form"/>
            <field name="name">statement_line_form</field>
        </record>
        <record model="ir.ui.view" id="line_group_view_list">
            <field name="model">account.statement.line.group</field>
            <field name="inherit" ref="account_statement.line_group_view_list"/>
            <field name="name">line_group_list</field>
        </record>
        <record model="ir.ui.view" id="line_group_view_form">
            <field name="model">account.statement.line.group</field>
            <field name="inherit" ref="account_statement.line_group_view_form"/>
            <field name="name">line_group_form</field>
        </record>
        <!-- ####################################### -->
        <!-- # Account Statement Line Group Cancel # -->
        <!-- ####################################### -->
	<record model="ir.action.wizard" id="act_cancel_line_group">
	    <field name="name">Cancel</field>
	    <field name="wiz_name">account.statement.line.group.cancel</field>
	</record>
	<record model="ir.ui.view" id="line_group_cancel_start_view_form">
	    <field name="model">account.statement.line.group.cancel.start</field>
	    <field name="type">form</field>
	    <field name="name">line_group_cancel_start_view_form</field>
	</record>
    </data>
</tryton>
