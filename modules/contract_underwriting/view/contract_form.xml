<?xml version="1.0"?>
<data>
    <xpath expr="/form" position="inside">
        <field name="needs_underwriting" invisible="1"/>
    </xpath>
    <xpath expr="/form/notebook/page[@id='data']" position="after">
        <page string="Underwritings" id="underwritings">
            <field name="underwritings" mode="form"/>
        </page>
    </xpath>
</data>
