<?xml version="1.0" encoding="UTF-8"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="contract_view_form">
            <field name="model">contract</field>
            <field name="inherit" ref="contract.contract_view_form"/>
            <field name="name">contract_form</field>
        </record>
        <record model="ir.ui.view" id="underwriting_decision_view_form">
            <field name="model">underwriting.decision</field>
            <field name="type">form</field>
            <field name="name">underwriting_decision_form</field>
        </record>
        <record model="ir.ui.view" id="underwriting_decision_view_list">
            <field name="model">underwriting.decision</field>
            <field name="type">tree</field>
            <field name="name">underwriting_decision_list</field>
        </record>
        <record model="ir.action.act_window" id="act_underwriting_decision_form">
            <field name="name">Underwriting Decision</field>
            <field name="res_model">underwriting.decision</field>
        </record>
        <record model="ir.action.act_window.view" id="act_underwriting_decision_view_list">
            <field name="sequence" eval="10"/>
            <field name="view" ref="underwriting_decision_view_list"/>
            <field name="act_window" ref="act_underwriting_decision_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_underwriting_decision_viem_tree">
            <field name="sequence" eval="20"/>
            <field name="view" ref="underwriting_decision_view_form"/>
            <field name="act_window" ref="act_underwriting_decision_form"/>
        </record>
        <menuitem parent="offered.menu_product_configuration" sequence="30" id="menu_underwriting_decision_form" icon="tryton-list" action="act_underwriting_decision_form"/>
        <record model="ir.ui.view" id="underwriting_view_form">
            <field name="model">contract.underwriting</field>
            <field name="type">form</field>
            <field name="name">underwriting_form</field>
        </record>
        <record model="ir.ui.view" id="option_underwriting_view_form">
            <field name="model">contract.underwriting.option</field>
            <field name="type">form</field>
            <field name="name">option_underwriting_form</field>
        </record>
        <record model="ir.ui.view" id="option_underwriting_view_list">
            <field name="model">contract.underwriting.option</field>
            <field name="type">tree</field>
            <field name="name">option_underwriting_list</field>
        </record>
    </data>
</tryton>
