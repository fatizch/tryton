<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="configuration_view_form">
            <field name="model">account.configuration</field>
            <field name="inherit" ref="account.configuration_view_form"/>
            <field name="name">configuration_form</field>
        </record>
        <record model="ir.ui.view" id="payment_group_view_list">
            <field name="model">account.payment.group</field>
            <field name="inherit" ref="account_payment.payment_group_view_list"/>
            <field name="name">payment_group_list</field>
        </record>
        <record model="ir.ui.view" id="payment_group_view_form">
            <field name="model">account.payment.group</field>
            <field name="inherit" ref="account_payment.payment_group_view_form"/>
            <field name="name">payment_group_form</field>
        </record>
        <record model="ir.ui.view" id="payment_journal_view_form">
            <field name="model">account.payment.journal</field>
            <field name="inherit" ref="account_payment.payment_journal_view_form"/>
            <field name="name">payment_journal_form</field>
        </record>
        <record model="ir.ui.view" id="payment_journal_failure_action_view_form">
            <field name="model">account.payment.journal.failure_action</field>
            <field name="type">form</field>
            <field name="name">payment_journal_failure_action_form</field>
        </record>
        <record model="ir.ui.view" id="payment_journal_failure_action_view_list">
            <field name="model">account.payment.journal.failure_action</field>
            <field name="type">tree</field>
            <field name="name">payment_journal_failure_action_list</field>
        </record>
        <record model="ir.ui.view" id="payment_journal_reject_reason_view_form">
            <field name="model">account.payment.journal.reject_reason</field>
            <field name="type">form</field>
            <field name="name">payment_journal_reject_reason_form</field>
        </record>
        <record model="ir.ui.view" id="payment_journal_reject_reason_view_list">
            <field name="model">account.payment.journal.reject_reason</field>
            <field name="type">tree</field>
            <field name="name">payment_journal_reject_reason_list</field>
        </record>

        <record model="ir.action.act_window" id="act_reject_reason_form">
            <field name="name">Reject reasons</field>
            <field name="res_model">account.payment.journal.reject_reason</field>
        </record>
        <record model="ir.action.act_window.view" id="act_reject_reason_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="payment_journal_reject_reason_view_list"/>
            <field name="act_window" ref="act_reject_reason_form"/>
        </record>
        <record model="ir.action.act_window.view" id="act_reject_reason_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="payment_journal_reject_reason_view_form"/>
            <field name="act_window" ref="act_reject_reason_form"/>
        </record>
        <menuitem parent="account_payment.menu_payment_configuration" sequence="20"
            action="act_reject_reason_form" id="menu_reject_reason_form"/>
    </data>
</tryton>
