<data>
    <xpath expr="/form/notebook/page[@id='description']/field[@name='description']" position="after">
        <label name="reject_description"/>
        <field name="reject_description"/>
        <label name="manual_fail_status"/>
        <field name="manual_fail_status"/>
    </xpath>
    <xpath expr="/form/group/group[@id='buttons']/button[@name='fail']" position="replace">
        <button string="Fail" name="button_fail_payments"/>
    </xpath>
    <xpath expr="/form/notebook" position="before">
        <label name="state"/>
        <field name="state"/>
        <field name='journal_method' invisible="1"/>
        <field name="can_approve" invisible="1"/>
        <field name="line_reconciled" invisible="1"/>
    </xpath>
    <xpath expr="/form" position="inside">
        <newline/>
        <field name="color" invisible="1"/>
    </xpath>
</data>
