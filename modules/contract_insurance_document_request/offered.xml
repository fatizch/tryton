        <!-- # Document Rule # -->
        <!-- ################# -->
        <record model="ir.ui.view" id="documents_rule_form">
            <field name="model">document.rule</field>
            <field name="type">form</field>
            <field name="name">document_rule_form</field>
        </record>
        <record model="ir.ui.view" id="documents_rule-document_relation_form">
            <field name="model">document.rule-document.description</field>
            <field name="type">form</field>
            <field name="name">document_rule-document_relation_form</field>
        </record>
        <record model="ir.ui.view" id="documents_rule-document_relation_list">
            <field name="model">document.rule-document.description</field>
            <field name="type">tree</field>
            <field name="name">document_rule-document_relation_list</field>
        </record>
        <record model="ir.ui.view" id="product_view_form">
            <field name="model">offered.product</field>
            <field name="inherit" ref="offered.product_view_form"/>
            <field name="name">product_form</field>
        </record>
        <record model="ir.ui.view" id="coverage_view_form">
            <field name="model">offered.option.description</field>
            <field name="inherit" ref="offered.coverage_view_form"/>
            <field name="name">option_description_form</field>
        </record>
