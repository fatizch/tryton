<?xml version="1.0"?>
<form string="Rich Process">
  <group id="process_header">
  </group>
  <newline/>  
  <group id="process_body" yfill="1" yexpand="1">
  </group>
  <newline/>
  <group id="jump_around">
    <label name="history" xalign="0"/>
    <field name="history"/>
  </group>
  <newline/>
  <group id="buttons" col="8">
    <button string="Next" name="transition_next"/>
    <field name="display_next"/>
    <button string="Check" name="transition_check"/>
    <field name="display_check"/>
    <button string="Previous" name="transition_previous"/>
    <field name="display_previous"/>
    <button string="Cancel" name="transition_cancel"/>
    <field name="display_cancel"/>
  </group>
</form>
