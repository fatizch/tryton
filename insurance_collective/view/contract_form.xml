<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook" position="inside">
        <page id="enrollments" string="Enrollments"
                states="{'invisible': Eval('kind') != 'group'}">
            <field name="enrollments" colspan="4"/>
        </page>
    </xpath>
    <xpath expr="/form" position="inside">
        <field name="kind"/>
    </xpath>
</data>
