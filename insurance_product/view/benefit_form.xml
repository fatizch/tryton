<?xml version="1.0"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Benefit"/>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id='managers']/notebook/page[@id='eligibility']"
        position="before">
        <page string="Benefit" id="benefit">
            <field name="benefit_mgr" colspan="4" mode="form"/>
        </page>
        <page string="Reserve" id="reserve">
            <field name="reserve_mgr" colspan="4" mode="form"/>
        </page>
    </xpath>
    <xpath expr="/form/notebook/page[@id='admin']/group[@id='left']" position="inside">
        <label name="kind"/>
        <field name="kind" xexpand="0"/>
    </xpath>
</data>
