<?xml version="1.0"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="benefit rule"/>
    </xpath>
    <xpath expr="/form/label[@name='config_kind']" position="before">
        <label name="indemnification_calc_unit"/>
        <field name="indemnification_calc_unit"/>
        <newline/>
    </xpath>
    <xpath expr="/form/label[@name='rule']" position="before">
        <label name="kind"/>
        <field name="kind" xexpand="0"/>
        <newline/>
        <label name="amount"/>
        <field name="amount"/>
        <newline/>
        <label name="coef_coverage_amount"/>
        <field name="coef_coverage_amount"/>
        <newline/>
    </xpath>
</data>
