<?xml version="1.0"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Term - Renewal rule"/>
    </xpath>
    <xpath expr="/form" position="replace_attributes">
        <group id="config_kind" states="{'invisible': True}"/>
    </xpath>
    <xpath expr="/form/label[@name='rule']" position="before">
        <label name="with_term"/>
        <field name="with_term"/>
        <newline/>
        <label name="term_date_choice"/>
        <field name="term_date_choice"/>
        <label name="date_for_sync"/>
        <field name="date_for_sync"/>
        <newline/>
        <label name="can_be_renewed"/>
        <field name="can_be_renewed"/>
        <label name="frequency"/>
        <field name="frequency"/>
    </xpath>
</data>
