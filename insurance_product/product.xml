<?xml version="1.0"?>
  <tryton>
		<data>
			<!-- Product-->
			    <!-- Application Entry point-->
			    <menuitem name="Laboratory" id="menu_product" sequence="10"/>     
			
			    <!-- Tree view-->
			    <record model="ir.ui.view" id="product_view_tree">             
				    <field name="model">ins_product.product</field>             
				    <field name="type">tree</field>             
				    <field name="arch" type="xml">                 
					    <![CDATA[                 
						    <tree string="Product">
							    <field name="code"/>
							    <field name="name"/>
						    </tree>                 
					    ]]>
				    </field>         
			    </record>

			    <!-- form view-->
			    <record model="ir.ui.view" id="product_view_form">
				     <field name="model">ins_product.product</field>
				     <field name="type">form</field>
				     <field name="arch" type="xml">
					     <![CDATA[
						    <form string="product" col="2">
						        <label name="code" colspan="1"/>
						        <field name="code" colspan="1"/>
						        <label name="name" colspan="1"/>
						        <field name="name" colspan="1" xexpand="0"/>
						        <label name="effective_date" colspan="1"/>
						        <field name="effective_date" colspan="1"/>
						        <notebook  colspan="2">
							        <page string="Options" id="option">
								        <field name="options">
								        </field>
							        </page>
						        </notebook>
						    </form>               ]]>
				     </field>
			    </record>

			    <!-- View for the main menu and the event -->         
			    <record model="ir.action.act_window" id="act_product_form">
				    <field name="name">Products</field>
				    <field name="res_model">ins_product.product</field>
			    </record>

			    <!-- View that connect the form in the tree with the specification -->
			    <record model="ir.action.act_window.view" id="act_product_form_view1">
				    <field name="sequence" eval="10"/>
				    <field name="view" ref="product_view_tree"/>
				    <field name="act_window" ref="act_product_form"/>
			    </record>

			    <!-- View for the edition or the Form of product-->
			    <record model="ir.action.act_window.view" id="act_product_form_view2">
				    <field name="sequence" eval="20"/>
				    <field name="view" ref="product_view_form"/>
				    <field name="act_window" ref="act_product_form"/>
			    </record>

			    <menuitem parent="menu_product" sequence="1"  
			    id="menu_product_form" icon="tryton-list" action="act_product_form"/>
			<!-- /Product-->
			
			<!-- Coverage-->   
			    <!-- Tree view-->
			    <record model="ir.ui.view" id="coverage_view_tree">             
				    <field name="model">ins_product.coverage</field>             
				    <field name="type">tree</field>             
				    <field name="arch" type="xml">                 
					    <![CDATA[                 
					        <tree>
					            <field name="code"/>
					            <field name="name"/>
					        </tree>                 
					    ]]>
				    </field>         
			    </record>

			    <!-- form view-->
			    <record model="ir.ui.view" id="coverage_view_form">
				     <field name="model">ins_product.coverage</field>
				     <field name="type">form</field>
				     <field name="arch" type="xml">
					     <![CDATA[
					         <form string="coverage" col="6">
					             <label name="code"/>
					             <field name="code"/>
					             <label name="name"/>
					             <field name="name"/>
					             <label name="effective_date" />
					             <field name="effective_date" />
					             <notebook colspan="6">
							        <page string="General" id="general2">
								        <field name="managers">
								        </field>
							        </page>
						        </notebook>
					            
					         </form>               
					     ]]>
				     </field>
			    </record>

			    <!-- View for the main menu and the event -->         
			    <record model="ir.action.act_window" id="act_coverage_form">
				    <field name="name">Coverages</field>
				    <field name="res_model">ins_product.coverage</field>
			    </record>

			    <!-- View that connect the form in the tree with the specification -->
			    <record model="ir.action.act_window.view" id="act_coverage_form_view1">
				    <field name="sequence" eval="10"/>
				    <field name="view" ref="coverage_view_tree"/>
				    <field name="act_window" ref="act_coverage_form"/>
			    </record>

			    <!-- View for the edition or the Form of coverage-->
			    <record model="ir.action.act_window.view" id="act_coverage_form_view2">
				    <field name="sequence" eval="20"/>
				    <field name="view" ref="coverage_view_form"/>
				    <field name="act_window" ref="act_coverage_form"/>
			    </record>

			    <menuitem parent="menu_product" sequence="2"  
			    id="menu_coverage_form" icon="tryton-list" action="act_coverage_form"/>
			<!--/Coverage-->
						
			<!-- businessrulemanager-->
			    <!-- Tree view-->
			    <record model="ir.ui.view" id="businessrulemanager_view_tree">             
				    <field name="model">ins_product.businessrulemanager</field>             
				    <field name="type">tree</field>             
				    <field name="arch" type="xml">                 
					    <![CDATA[                 
					        <tree string="businessrulemanager">
					            <field name="code"/>
					            <field name="name"/>
					        </tree>                 
					    ]]>
				    </field>         
			    </record>

			    <!-- form view-->
			    <record model="ir.ui.view" id="businessrulemanager_view_form">
				     <field name="model">ins_product.businessrulemanager</field>
				     <field name="type">form</field>
				     <field name="arch" type="xml">
					     <![CDATA[
					         <form string="businessrulemanager">
					             <label name="code"/>
					             <field name="code"/>
					             <label name="name"/>
					             <field name="name"/>
					             <field name="belongs_to"/>
					              <notebook colspan="6">
							        <page string="General" id="general">
								        <field name="business_rules"/>
							        </page>
						        </notebook>
					         </form>               
					     ]]>
				     </field>
			    </record>

			    <!-- View for the main menu and the event -->         
			    <record model="ir.action.act_window" id="act_businessrulemanager_form">
				    <field name="name">Business Rule Manager</field>
				    <field name="res_model">ins_product.businessrulemanager</field>
			    </record>

			    <!-- View that connect the form in the tree with the specification -->
			    <record model="ir.action.act_window.view" id="act_businessrulemanager_form_view1">
				    <field name="sequence" eval="10"/>
				    <field name="view" ref="businessrulemanager_view_tree"/>
				    <field name="act_window" ref="act_businessrulemanager_form"/>
			    </record>

			    <!-- View for the edition or the Form of businessrulemanager-->
			    <record model="ir.action.act_window.view" id="act_businessrulemanager_form_view2">
				    <field name="sequence" eval="20"/>
				    <field name="view" ref="businessrulemanager_view_form"/>
				    <field name="act_window" ref="act_businessrulemanager_form"/>
			    </record>

			   
			<!--/businessrulemanager-->
			
			<!-- businessrule-->
			
			    <!-- Tree view-->
			    <record model="ir.ui.view" id="businessrule_view_tree">             
				    <field name="model">ins_product.businessrule</field>             
				    <field name="type">tree</field>             
				    <field name="arch" type="xml">                 
					    <![CDATA[                 
					        <tree >
					            <field name="code"/>
					            <field name="name"/>
					            <field name="from_date"/>
					        </tree>                 
					    ]]>
				    </field>         
			    </record>

			    <!-- form view-->
			    <record model="ir.ui.view" id="businessrule_view_form">
				     <field name="model">ins_product.businessrule</field>
				     <field name="type">form</field>
				     <field name="arch" type="xml">
					     <![CDATA[
					         <form string="Business rule">
					             <label name="code"/>
					             <field name="code"/>
					             <label name="name"/>
					             <field name="name"/>
					             <label name="from_date"/>
					             <field name="from_date"/>
					             <field name="manager"/>
					         </form>               
					     ]]>
				     </field>
			    </record>

			    <!-- View for the main menu and the event -->         
			    <record model="ir.action.act_window" id="act_businessrule_form">
				    <field name="name">Business Rule</field>
				    <field name="res_model">ins_product.businessrule</field>
			    </record>

			    <!-- View that connect the form in the tree with the specification -->
			    <record model="ir.action.act_window.view" id="act_businessrule_form_view1">
				    <field name="sequence" eval="10"/>
				    <field name="view" ref="businessrule_view_tree"/>
				    <field name="act_window" ref="act_businessrule_form"/>
			    </record>

			    <!-- View for the edition or the Form of businessrule-->
			    <record model="ir.action.act_window.view" id="act_businessrule_form_view2">
				    <field name="sequence" eval="20"/>
				    <field name="view" ref="businessrule_view_form"/>
				    <field name="act_window" ref="act_businessrule_form"/>
			    </record>

			   
			<!--/businessrule-->
			
		</data>
	</tryton>
