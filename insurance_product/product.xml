<?xml version="1.0"?>
<tryton>
    <data>
        <!--  ******************************************* 
              **************     Menus     **************
              *******************************************-->
        <!-- Application Entry point -->
        <menuitem name="Laboratory" id="menu_product" sequence="10" />
        <menuitem name="Configuration" parent="menu_product"
            id="menu_product_configuration" sequence="0" icon="tryton-preferences"/>

        <!-- Moving existing Entry Points -->
        <menuitem parent="menu_product" name="Rule Engine"
            id="rule_engine.menu_rule_engine" sequence="100"/>


        <!--  ******************************************* 
              **************    Offered    **************
              *******************************************-->
        <record model="ir.ui.view" id="offered_view_tree">
            <field name="model">ins_product.offered</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <![CDATA[
                <tree string="Offered">
                        <field name="code"/>
                         <field name="name"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                </tree>
                ]]>
            </field>
        </record>
        <!-- form view -->
        <record model="ir.ui.view" id="offered_view_form">
            <field name="model">ins_product.offered</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <![CDATA[
                <form string="Offered">
                    <label name="code" xalign="0"/>
                    <field name="code"/>
                    <label name="name" xalign="0"/>
                    <field name="name"/>
                    <newline/>
                    <label name="start_date" xalign="0"/>
                    <field name="start_date"/>
                    <label name="end_date" xalign="0"/>
                    <field name="end_date"/>
                    <newline/>
                    <group id="templating" colspan="4">
                        <label name="template" xalign="0"/>
                        <field name="template" xexpand="1"/>
                        <field name="template_behaviour"/>
                    </group>
                    <notebook>
                        <page string="Summary" id="summary">
                            <field name="description" widget="richtext"
                                colspan="4"/>
                            <field name="summary" widget="richtext"
                                colspan="4"/>
                        </page>
                        <page string="Managers" id="managers">
                            <notebook>
                                <page string="Eligibilty" id="eligibility">
                                    <field name="eligibility_mgr" colspan="4" mode="form"/>
                                </page>
                                <page string="Pricing" id="pricing">
                                    <field name="pricing_mgr" colspan="4" mode="form"/>
                                </page>
                            </notebook>
                        </page>
                        <page string="Administration" id="admin">

                        </page>
                    </notebook>
                </form>
                ]]>
            </field>
        </record>
        <!--  ******************************************* 
              **************    Product    **************
              *******************************************-->

        <!-- Product -->

        <!-- form view -->
        <record model="ir.ui.view" id="product_view_form">
            <field name="model">ins_product.product</field>
            <field name="type">form</field>
            <field name="inherit" ref="offered_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                <xpath expr="/form" position="replace_attributes">
                    <form string="Product"/>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;admin&quot;]" position="after">
                    <page string="Dynamic Data" id="dynamic_data">
                        <field name="dynamic_data_manager" mode="form"/>
                    </page>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;managers&quot;]" position="before">
                    <page string="Options" id="option">
                        <field name="options"/>
                    </page>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;admin&quot;]"
                        position="inside">
                    <label name="contract_generator" xalign="0"/>
                    <field name="contract_generator"/>
                    <label name="currency"/>
                    <field name="currency"/>
                </xpath>
                </data>
                ]]>
            </field>
        </record>
        <record model="ir.ui.view" id="product_view_tree">
            <field name="model">ins_product.product</field>
            <field name="type">tree</field>
            <field name="inherit" ref="offered_view_tree"/>
            <field name="arch" type="xml"/>
        </record>

        <!-- View for the main menu and the event -->
        <record model="ir.action.act_window" id="act_product_form">
            <field name="name">Products</field>
            <field name="res_model">ins_product.product</field>
        </record>

        <!-- View that connects the form in the tree with the specification -->
        <record model="ir.action.act_window.view" id="act_product_form_view1">
            <field name="sequence" eval="10" />
            <field name="view" ref="product_view_tree" />
            <field name="act_window" ref="act_product_form" />
        </record>

        <!-- View for the edition or the Form of product -->
        <record model="ir.action.act_window.view" id="act_product_form_view2">
            <field name="sequence" eval="20" />
            <field name="view" ref="product_view_form" />
            <field name="act_window" ref="act_product_form" />
        </record>

        <menuitem parent="menu_product" sequence="1" id="menu_product_form"
            icon="tryton-list" action="act_product_form" />

        <!--  ******************************************* 
              **************    Coverage    **************
              *******************************************-->
        <!-- form view -->
        <record model="ir.ui.view" id="coverage_view_form">
            <field name="model">ins_product.coverage</field>
            <field name="type">form</field>
            <field name="inherit" ref="offered_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                <xpath expr="/form" position="replace_attributes">
                    <form string="Coverage"/>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;managers&quot;]" position="after">
                    <page string="Benefits" id="benefits">
                        <field name="benefits"/>
                    </page>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;managers&quot;]/notebook/page[@id=&quot;eligibility&quot;]" position="before">
                    <page string="Coverage Amount" id="coverage_amount">
                        <field name="coverage_amount_mgr" mode="form"/>
                    </page>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;admin&quot;]"
                position="inside">
                    <label name="currency"/>
                    <field name="currency"/>
                    <label name="insurer" xalign="0"/>
                    <field name="insurer" widget="selection"/>
                </xpath>
                <xpath expr="/form/group[@id=&quot;templating&quot;]"
                    position="before">
                    <label name="family" xalign="0"/>
                    <field name="family" xexpand="0"/>
                </xpath>
                </data>
                ]]>
            </field>
        </record>
        <record model="ir.ui.view" id="coverage_view_tree">
            <field name="model">ins_product.coverage</field>
            <field name="type">tree</field>
            <field name="inherit" ref="offered_view_tree"/>
            <field name="arch" type="xml">
             <![CDATA[
                <data>
                <xpath expr="/tree/field[@name=&quot;code&quot;]"
                    position="after">
                    <field name="family"/>
                </xpath>
                </data>
            ]]>
            </field>
        </record>

        <!-- View for the main menu and the event -->
        <record model="ir.action.act_window" id="act_coverage_form">
            <field name="name">Coverages</field>
            <field name="res_model">ins_product.coverage</field>
        </record>

        <!-- View that connect the form in the tree with the specification -->
        <record model="ir.action.act_window.view" id="act_coverage_tree_view">
            <field name="sequence" eval="10" />
            <field name="view" ref="coverage_view_tree" />
            <field name="act_window" ref="act_coverage_form" />
        </record>

        <!-- View for the edition or the Form of coverage -->
        <record model="ir.action.act_window.view" id="act_coverage_form_view2">
            <field name="sequence" eval="20" />
            <field name="view" ref="coverage_view_form" />
            <field name="act_window" ref="act_coverage_form" />
        </record>

        <menuitem parent="menu_product" sequence="2" id="menu_coverage_form"
            icon="tryton-list" action="act_coverage_form" />
        <!--/Coverage -->

        <!--  ******************************************* 
              ********    BusinessRuleManager    ********
              *******************************************-->
        <record model="ir.ui.view" id="businessrulemanager_view_form">
            <field name="model">ins_product.business_rule_manager</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <![CDATA[
                <form string="Business Rule Manager">
                    <group id="templating">
                        <label name="template"/>
                        <field name="template"/>
                        <field name="template_behaviour"/>
                    </group>
                    <newline/>
                    <field name="business_rules" colspan="4" />
                </form>
            ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    BusinessRuleRoot    ********
              *******************************************-->
        <record model="ir.ui.view" id="businessruleroot_view_form">
            <field name="model">ins_product.business_rule_root</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <![CDATA[
                <form string="Business Rule Root">
                    <group id="templating">
                        <label name="template"/>
                        <field name="template"/>
                        <field name="template_behaviour"/>
                    </group>
                    <notebook>
                        <page id="main" string="Main">
                            <group id="config_kind" colspan="2">
                            <label name="config_kind" xalign="0" xfill="1"/>
                            <field name="config_kind" xfill="1" xexpand="0" xalign="0"/>
                            </group>
                            <group id="rule" colspan="4" 
                              states="{'invisible': Eval('config_kind') != 'rule'}">
                                <label name="rule" xalign="0"/>
                                <field name="rule"/>
                            </group>
                            <group id="simple" colspan="4"
                            states="{'invisible': Eval('config_kind') != 'simple'}">
                            </group>
                        </page>
                    </notebook>
                </form>
            ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    GenericBusinessRule    ********
              *******************************************-->
        <!-- GenericBusinessRule -->
        <!-- form view -->
        <record model="ir.ui.view" id="genericbusinessrule_view_form">
            <field name="model">ins_product.generic_business_rule</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <![CDATA[
                <form string="Business Rule" col="4">
                    <label name="kind"/>
                    <field name="kind"/>
                    <newline/>
                    <label name="start_date"/>
                    <field name="start_date"/>
                    <label name="end_date"/>
                    <field name="end_date"/>
                    <newline/>
                    <group id ="Rules" colspan="4" yexpand="1" yfill="1" >
                        <field name="pricing_rule" mode="form"/>
                        <field name="eligibility_rule" mode="form"/>
                        <field name="benefit_rule" mode="form"/>
                        <field name="reserve_rule" mode="form"/>
                        <field name="coverage_amount_rule" mode="form" pre_validate="1"/>
                    </group>
                </form>
            ]]>
            </field>
        </record>
        <record model="ir.ui.view" id="genericbusinessrule_view_tree">
            <field name="model">ins_product.generic_business_rule</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
            <![CDATA[
                <tree string="Generic Business Rule"
                colors="If(Bool(Eval('is_current')), 'blue', 'black')">
                    <field name="start_date"/>
                    <field name="end_date"/>
                </tree>
            ]]>
            </field>
        </record>
        <!--  ******************************************* 
              ********        PricingData        ********
              *******************************************-->
        <record model="ir.ui.view" id="pricingdata_view_tree">
            <field name="model">ins_product.pricing_data</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <![CDATA[
                    <tree string="Pricing Components">
                        <field name="kind"/>
                        <field name="code"/>
                        <field name="config_kind"/>
                        <field name="summary"/>
                    </tree>
                ]]>
            </field>
        </record>

        <record model="ir.ui.view" id="pricingdata_view_form">
            <field name="model">ins_product.pricing_data</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <![CDATA[
                    <form string="Pricing Component">
                        <group id="main">
                            <label name="kind" xalign="0" xfill="1"/>
                            <field name="kind"/>
                            <label name="code" xalign="0"/>
                            <field name="code"/>
                        </group>
                        <newline/>
                        <group id="tax"
                            states="{'invisible': Eval('kind') != 'tax'}">
                            <label name="the_tax" xalign="0"/>
                            <field name="the_tax"/>
                        </group>
                        <newline/>
                        <group id="fee"
                            states="{'invisible': Eval('kind') != 'fee'}">
                            <label name="the_fee" xalign="0"/>
                            <field name="the_fee"/>
                        </group>
                        <newline/>
                        <group id="other"
                            states="{'invisible': Eval('kind') != 'base'}">
                            <group id="config_kind" colspan="2">
                                <label name="config_kind" xalign="0" xfill="1"/>
                                <field name="config_kind" xexpand="1"/>
                            </group>
                            <newline/>
                            <group id="simple" colspan="4"
                                states="{'invisible': Eval('config_kind') != 'simple'}">
                                <label name="fixed_amount" xalign="0"/>
                                <field name="fixed_amount"/>
                            </group>
                            <group id="rule" colspan="4"
                                states="{'invisible': Eval('config_kind') != 'rule'}">
                               <label name="rule" xalign="0"/>
                               <field name="rule"/>
                            </group>
                        </group>
                    </form>
                ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********        Calculator         ********
              *******************************************-->

        <record model="ir.ui.view" id="calculator_view_tree">
            <field name="model">ins_product.pricing_calculator</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <![CDATA[
                    <tree string="Calculators">
                        <field name="rec_name"/>
                        <field name="key"/>
                    </tree>
                ]]>
            </field>
        </record>
        <record model="ir.ui.view" id="calculator_view_form">
            <field name="model">ins_product.pricing_calculator</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <![CDATA[
                    <form string="Price Calculator">
                        <group id="main" col="8">
                            <label name="key" xalign="0"/>
                            <field name="key" />
                            <newline/>
                            <label name="simple" xalign="0"/>
                            <field name="simple" xexpand="0"/>
                            <label name="combine" xalign="0"/>
                            <field name="combine" colspan="5"/>
                        </group>
                        <newline/>
                        <group id="test_data" xfill="1"
                            yexpand="1" yfill="1">
                            <field name="data" yfill="1" yexpand="1"/>
                        </group>
                    </form>
                ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    PricingRule    ********
              *******************************************-->
        <record model="ir.ui.view" id="pricingrule_view_form">
            <field name="model">ins_product.pricing_rule</field>
            <field name="type">form</field>
            <field name="inherit" ref="businessruleroot_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath expr="/form" position="replace_attributes">
                        <form string="Pricing rule"/>
                    </xpath>
                    <xpath expr="/form/notebook/page[@id=&quot;main&quot;]"
                     position="after">
                        <page id="calculators" string="Main"
                            states="{'invisible' : Eval('config_kind') == 'simple'}">
                            <field name="calculators" yfill="1" yexpand="1"
                                xexpand="1" colspan="4"/>
                        </page>
                    </xpath>
                    <xpath expr="/form/notebook/page[@id=&quot;main&quot;]"
                        position="replace_attributes">
                        <page id="main" states="{'invisible': True}"/>
                    </xpath>
                    <xpath expr="/form/group[@id=&quot;templating&quot;]"
                        position="after">
                            <newline/>
                            <group id="config_kind">
                                <label name="config_kind" xalign="0" xfill="1"/>
                                <field name="config_kind" xexpand="1"/>
                                <label name="frequency" xalign="0"/>
                                <field name="frequency" xexpand="1"/>
                            </group>
                            <newline/>
                            <group id="price_kind"
                                states="{'invisible': Eval('config_kind') == 'simple'}">
                                <label name="price_kind" xalign="0"/>
                                <field name="price_kind" xexpand="0"/>
                            </group>
                            <newline/>
                            <group id="basic"
                                states="{'invisible': Eval('config_kind') == 'rule'}">
                                <label name="basic_price" xalign="0"/>
                                <field name="basic_price" xexpand="1"/>
                                <newline/>
                                <label name="basic_tax" xalign="0"/>
                                <field name="basic_tax" xexpand="1"/>
                            </group>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    EligibilityRule    ********
              *******************************************-->
        <record model="ir.ui.view" id="eligibilityrule_view_form">
            <field name="model">ins_product.eligibility_rule</field>
            <field name="type">form</field>
            <field name="inherit" ref="businessruleroot_view_form"/>
            <field name="arch" type="xml">
            <![CDATA[
            <data>
                <xpath expr="/form" position="replace_attributes">
                    <form string="Eligibility rule"/>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;main&quot;]"
                     position="after">
                    <page id="sub" string="Covered Element">
                        <field name="relation_kinds" colspan="4" xexpand="1"
                            yfill="1" yexpand="1"/>
                        <group id="sub_elem_config_kind" colspan="2">
                            <label name="sub_elem_config_kind" xalign="0" xfill="1"/>
                            <field name="sub_elem_config_kind" xfill="1" xexpand="0" xalign="0"/>
                        </group>
                        <newline/>
                        <group id="sub_simple" colspan="4"
                            states="{'invisible': Eval('sub_elem_config_kind') != 'simple'}">
                        </group>
                        <group id="sub_rule" colspan="4"
                            states="{'invisible': Eval('sub_elem_config_kind') != 'rule'}">
                           <label name="sub_elem_rule" xalign="0"/>
                           <field name="sub_elem_rule"/>
                        </group>
                    </page>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;main&quot;]/group[@id=&quot;simple&quot;]"
                 position="inside">
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;main&quot;]"
                 position="inside">
                    <group id="subscriber_classes" colspan="2">
                        <label name="subscriber_classes" xalign="0"/>
                        <field name="subscriber_classes" xexpand="0"/>
                    </group>
                </xpath>
            </data>
            ]]>
            </field>
        </record>
        <!--  ******************************************* 
              *************    Benefit          *********
              *******************************************-->
        <!-- form view -->
        <record model="ir.ui.view" id="benefit_view_form">
            <field name="model">ins_product.benefit</field>
            <field name="type">form</field>
            <field name="inherit" ref="offered_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                <xpath expr="/form" position="replace_attributes">
                    <form string="Benefit"/>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;managers&quot;]/notebook/page[@id=&quot;eligibility&quot;]" position="before">
                    <page string="Benefit" id="benefit">
                        <field name="benefit_mgr" colspan="4" mode="form"/>
                    </page>
                    <page string="Reserve" id="reserve">
                        <field name="reserve_mgr" colspan="4" mode="form"/>
                    </page>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;admin&quot;]" position="inside">
                    <label name="kind" xalign="0"/>
                    <field name="kind" xexpand="0"/>
                </xpath>
                </data>
                ]]>
            </field>
        </record>
        <record model="ir.ui.view" id="benefit_view_tree">
            <field name="model">ins_product.benefit</field>
            <field name="type">tree</field>
            <field name="inherit" ref="offered_view_tree"/>
            <field name="arch" type="xml">
            <![CDATA[
                <data>
                    <xpath expr="/tree/field[@name=&quot;code&quot;]" position="before">
                        <field name="kind"/>
                    </xpath>
                </data>
            ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    Reserve Rule    ***************
              *******************************************-->
        <record model="ir.ui.view" id="reserverule_view_form">
            <field name="model">ins_product.reserve_rule</field>
            <field name="type">form</field>
            <field name="inherit" ref="businessruleroot_view_form"/>
            <field name="arch" type="xml">
            <![CDATA[
            <data>
                <xpath expr="/form" position="replace_attributes">
                    <form string="Reserve rule"/>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;main&quot;]/group[@id=&quot;simple&quot;]"
                 position="inside">
                    <label name="amount"/>
                    <field name="amount"/>
                </xpath>
            </data>
            ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    Benefit Rule    ***************
              *******************************************-->
        <record model="ir.ui.view" id="benefitrule_view_form">
            <field name="model">ins_product.benefit_rule</field>
            <field name="type">form</field>
            <field name="inherit" ref="businessruleroot_view_form"/>
            <field name="arch" type="xml">
            <![CDATA[
            <data>
                <xpath expr="/form" position="replace_attributes">
                    <form string="benefit rule"/>
                </xpath>
                <xpath expr="/form/notebook/page[@id=&quot;main&quot;]/group[@id=&quot;simple&quot;]"
                 position="inside">
                    <label name="kind" xalign="0"/>
                    <field name="kind" xexpand="0"/>
                    <newline/>
                    <label name="amount"/>
                    <field name="amount"/>
                    <newline/>
                    <label name="coef_coverage_amount"/>
                    <field name="coef_coverage_amount"/>
                </xpath>
            </data>
            ]]>
            </field>
        </record>

        <!--  ******************************************* 
              ********    Coverage Amount Rule    ***************
              *******************************************-->
        <record model="ir.ui.view" id="coverage_amountrule_view_form">
            <field name="model">ins_product.coverage_amount_rule</field>
            <field name="type">form</field>
            <field name="inherit" ref="businessruleroot_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                    <data>
                        <xpath expr="/form" position="replace_attributes">
                            <form string="Coverage Amount rule"/>
                        </xpath>
                        <xpath expr="/form/notebook/page[@id=&quot;main&quot;]/group[@id=&quot;simple&quot;]"
                         position="inside">
                            <group id="kind" colspan="2">
                                <label name="kind" xalign="0"/>
                                <field name="kind" xexpand="0"/>
                            </group>
                            <newline/>
                            <label name="amounts"/>
                            <field name="amounts"/>
                            <newline/>
                            <group id="calc_list" col="6" colspan="4">
	                            <label name="amount_start"/>
	                            <field name="amount_start"/>
	                            <label name="amount_end"/>
	                            <field name="amount_end"/>
	                            <label name="amount_step"/>
	                            <field name="amount_step"/>
                            </group>
                        </xpath>
                    </data>
                ]]>
            </field>
        </record>

    <!--################################################################-->
    <!--                       Dynamic Data Manager                     -->
    <!--################################################################-->

        <record model="ir.ui.view" id="dynamic_data_view_form">
            <field name="model">ins_product.dynamic_data_manager</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <![CDATA[
                    <form string="Dynamic Data Manager">
                        <field name="specific_dynamic"/>
                        <field name="shared_dynamic"/>
                    </form>
                ]]>
            </field>
        </record>

    <!--################################################################-->
    <!--                       Coop Schema Elements                     -->
    <!--################################################################-->

        <record model="ir.ui.view" id="schema_element_view_tree">
            <field name="model">ins_product.schema_element</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <![CDATA[
                    <tree string="Dynamic Data Descriptors">
                        <field name="is_shared"/>
                        <field name="name"/>
                        <field name="type_"/>
                        <field name="technical_name"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </tree>
