<?xml version="1.0"?>
<tryton>
  <data>
    <!-- Document Desc -->
    <record model="ir.ui.view" id="document_desc_form">
      <field name="model">ins_product.document_desc</field>
      <field name="type">form</field>
      <field name="name">document_desc_form</field>
    </record>
    <record model="ir.ui.view" id="document_desc_tree">
      <field name="model">ins_product.document_desc</field>
      <field name="type">tree</field>
      <field name="name">document_desc_tree</field>
    </record>
    <record model="ir.action.act_window" id="act_document_desc">
      <field name="name">Document Descriptors</field>
      <field name="res_model">ins_product.document_desc</field>
    </record>
    <record model="ir.action.act_window.view" id="act_document_desc_view1">
      <field name="sequence" eval="16"/>
      <field name="view" ref="document_desc_tree"/>
      <field name="act_window" ref="act_document_desc"/>
    </record>
    <record model="ir.action.act_window.view" id="act_document_desc_view2">
      <field name="sequence" eval="16"/>
      <field name="view" ref="document_desc_form"/>
      <field name="act_window" ref="act_document_desc"/>
    </record>
    <menuitem parent="menu_product_configuration" sequence="30" action="act_document_desc" id="menu_document_desc"/>
    <!-- Document Rule -->
    <record model="ir.ui.view" id="document_rule_form">
      <field name="model">ins_product.document_rule</field>
      <field name="type">form</field>
      <field name="inherit" ref="businessruleroot_view_form"/>
      <field name="name">document_rule_form</field>
    </record>
    <record model="ir.ui.view" id="document_rule_tree">
      <field name="model">ins_product.document_rule</field>
      <field name="type">tree</field>
      <field name="inherit" ref="businessruleroot_view_tree"/>
      <field name="arch" type="xml"/>
    </record>
    <!-- Document -->
    <record model="ir.ui.view" id="document_form">
      <field name="model">ins_product.document</field>
      <field name="type">form</field>
      <field name="name">document_form</field>
    </record>
    <record model="ir.ui.view" id="document_tree">
      <field name="model">ins_product.document</field>
      <field name="type">tree</field>
      <field name="name">document_tree</field>
    </record>
    <!-- Document Request -->
    <record model="ir.ui.view" id="document_request_form">
      <field name="model">ins_product.document_request</field>
      <field name="type">form</field>
      <field name="name">document_request_form</field>
    </record>
    <record model="ir.ui.view" id="document_request_tree">
      <field name="model">ins_product.document_request</field>
      <field name="type">tree</field>
      <field name="name">document_request_tree</field>
    </record>
    <!-- Report -->
    <record model="ir.action.wizard" id="letter_generation_wizard">
        <field name="name">Letter Generation</field>
        <field name="wiz_name">ins_product.letter_creation_wizard</field>
    </record>
    <record model="ir.action.report" id="letter_generation_report">
      <field name="name">Letter Report</field>
      <field name="report_name">ins_product.letter_report</field>
    </record>
    <record model="ir.action.keyword" id="report_document_request_keyword">
      <field name="keyword">form_action</field>
      <field name="model">ins_product.document_request,-1</field>
      <field name="action" ref="letter_generation_wizard"/>
    </record>
    <!-- Letter Model -->
    <record model="ir.ui.view" id="letter_model_form">
      <field name="model">ins_product.letter_model</field>
      <field name="type">form</field>
      <field name="name">letter_model_form</field>
    </record>
    <record model="ir.ui.view" id="letter_model_tree">
      <field name="model">ins_product.letter_model</field>
      <field name="type">tree</field>
      <field name="name">letter_model_tree</field>
    </record>
    <record model="ir.action.act_window" id="act_letter_model">
      <field name="name">Letter Models</field>
      <field name="res_model">ins_product.letter_model</field>
    </record>
    <record model="ir.action.act_window.view" id="act_letter_model_view1">
      <field name="sequence" eval="16"/>
      <field name="view" ref="letter_model_tree"/>
      <field name="act_window" ref="act_letter_model"/>
    </record>
    <record model="ir.action.act_window.view" id="act_letter_model_view2">
      <field name="sequence" eval="16"/>
      <field name="view" ref="letter_model_form"/>
      <field name="act_window" ref="act_letter_model"/>
    </record>
    <menuitem parent="menu_product_configuration" sequence="30"
      action="act_letter_model" id="menu_letter_model"/>
    <!-- Letter Version -->
    <record model="ir.ui.view" id="letter_version_form">
      <field name="model">ins_product.letter_version</field>
      <field name="type">form</field>
      <field name="name">letter_version_form</field>
    </record>
    <record model="ir.ui.view" id="letter_version_tree">
      <field name="model">ins_product.letter_version</field>
      <field name="type">tree</field>
      <field name="name">letter_version_tree</field>
    </record>
    <!-- Letter Model Selection -->
    <record model="ir.ui.view" id="letter_model_selection_form">
      <field name="model">ins_product.letter_model_selection</field>
      <field name="type">form</field>
      <field name="name">letter_model_selection_form</field>
    </record>
    <record model="ir.ui.view" id="letter_model_displayer_tree">
      <field name="model">ins_product.letter_model_displayer</field>
      <field name="type">tree</field>
      <field name="name">letter_model_displayer_tree</field>
    </record>
  </data>
</tryton>
